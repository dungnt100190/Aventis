  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>select 
  Verfuegung = V.Bezeichnung,
  Entscheid = convert(varchar, V.EntscheidDatum, 104),
  EntscheidDatum = V.EntscheidDatum,
  RevisionsDatum = V.Revisionsdatum,
  Status = dbo.fnLOVText('AmBerechnungsStatus',V.AmVerfuegungStatusCode),
  FtFallNr = convert(varchar, F.FaFallID),
  ArtAlbvKkbb = case when L.FaProzessCode = 404 
   then 'Kleinkinderbetreuungsbeiträge' 
   else 'Alimentenbevorschussung' 
  end,
  BaPersonID$ = F.BaPersonID,

  ReviVon = convert(varchar, {3}, 104),
  ReviBis = convert(varchar, {4}, 104),
  BriefDatum = convert(varchar, convert(datetime, {0}), 104),
  LetzterReviBrief = convert(varchar, {1}, 104),
  AktJahr = convert(varchar, Year(convert(datetime,{0}))),
  AktVorjahr = convert(varchar, Year(convert(datetime,{0}))-1),
  Jahrgang19 = convert(varchar, Year(convert(datetime,{0}))-19),
  Jahrgang16 = convert(varchar, Year(convert(datetime,{0}))-16),
  Frist10TageDatum = convert(varchar, DateAdd(DAY, 10, convert(datetime,{0})), 104),

  SehrGeehrter = case P.GeschlechtCode 
    when 1 then 'Sehr geehrter Herr '
    when 2 then 'Sehr geehrte Frau '
    else 'Sehr geehrte Frau/Herr '
  end + P.Name,
  FtAnrede = case P.GeschlechtCode 
    when 1 then 'Herr'
    when 2 then 'Frau'
    else 'Frau/Herr'
  end,
  FtName = P.Name,
  FtVorname = P.Vorname,
  FtStrasse = P.WohnsitzStrasseHausNr,
  FtPlz = P.WohnsitzPLZ,
  FtOrt = P.WohnsitzOrt,

  SbAnzeige = U.DisplayText,
  SbKurzzeichen = U.LogonName,
  SbName = U.LastName,
  SbVorname = U.FirstName,
  SbTelefon = U.Telefon,
  SbQT = U.SozialzentrumKurz,
  SbSozZentrum = U.OrgEinheitName,

  KopieAn = 'Kopie an: ' + IsNull(UW.FirstName + ' ', '') + UW.LastName, 
  KopieAn_QT = UW.SozialzentrumKurz,
  KopieAn_SozZentrum = UW.OrgEinheitName,

  W_SbKurzzeichen = UW.LogonName,
  W_SbName = UW.LastName,
  W_SbVorname = UW.FirstName,
  W_SbQT = UW.SozialzentrumKurz,
  W_SbSozZentrum = UW.OrgEinheitName

from AmVerfuegung V
left join FaLeistung L on L.FaLeistungID = V.FaLeistungID
left join FaFall F on F.FaFallID = L.FaFallID
left join vwPerson P on P.BaPersonID = F.BaPersonID
left join vwUser U on U.UserID = L.UserID
left join vwUser UW on UW.UserID = (
  SELECT TOP 1 XL.UserID from FaLeistung XL
  where XL.FaFallID = L.FaFallID
    and XL.FaProzessCode = 300
  order by DatumVon desc
)
where V.AmVerfuegungStatusCode in (3,4)
and V.AmVerfuegungID = (
  select top 1 YV.AmVerfuegungID from AmVerfuegung YV
  left join FaLeistung YL on YL.FaLeistungID = YV.FaLeistungID
  where YL.FaFallID = L.FaFallID
  and (YL.FaProzessCode = {5})
  and YV.AmVerfuegungStatusCode in (3,4)
  order by YV.EntscheidDatum desc
)
and V.RevisionsDatum between IsNull({3}, V.RevisionsDatum) and IsNull({4}, V.RevisionsDatum)
and (U.UserID = {2} or {2} = 0)
and (L.FaProzessCode = {5})
order by V.EntscheidDatum desc</value>
