  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>DECLARE @OrgGruppeID int
DECLARE @TeamID int
--- SET @OrgGruppeID = {edtSucheGruppe}
--- SET @TeamID = {edtSucheTeam}

select SZ$              = USR.Sozialzentrum,
       OE$              = USR.OrgUnit,
       MA$              = USR.NameVorname,
       [Fall-Nr.]       = LEI.FaFallID,
       [Pers.Nr.$]      = PRS.BaPersonID,
       Person           = PRS.NameVorname,
       [Geb.Dat.$]      = PRS.Geburtsdatum,
       StatusFP$        = FPS.Text,
       [FP von]         = isNull(FPL.DatumVon, FPL.GeplantVon),
       [FP bis]         = isNull(FPL.DatumBis, FPL.GeplantBis),
       [grün (letztes)] = dbo.fnXKurzMonat(BDG1.Monat) + ' ' + convert(varchar,BDG1.Jahr),
       [grau (erstes)]  = dbo.fnXKurzMonat(BDG2.Monat) + ' ' + convert(varchar,BDG2.Jahr),
       [rot  (erstes)]  = dbo.fnXKurzMonat(BDG3.Monat) + ' ' + convert(varchar,BDG3.Jahr),
--       [letzte Zahlung] = null, --TODO
--       LA               = null, --TODO
       BgBudgetID$      = BDG1.BgBudgetID,
       FallBaPersonID$  = FAL.BaPersonID
from   BgFinanzplan FPL
       left  join FaLeistung LEI  ON LEI.FaLeistungID = FPL.FaLeistungID
       left  join FaFall     FAL  ON FAL.FaFallID = LEI.FaFallID
       left  join vwPerson   PRS  ON PRS.BaPersonID = LEI.BaPersonID
       left  join vwUser     USR  ON USR.UserID = LEI.UserID
       left  join BgBudget   BDG1 ON BDG1.BgFinanzplanID = FPL.BgFinanzplanID and -- letztes grünes Budget
                                     BDG1.Masterbudget = 0 and
                                     BDG1.BgBewilligungStatusCode = 5 and
                                     BDG1.BgBudgetID = (select top 1 BgBudgetID
                                                        from   BgBudget
                                                        where  BgFinanzplanID = FPL.BgFinanzplanID and
                                                               Masterbudget = 0 and
                                                               BgBewilligungStatusCode = 5
                                                        order by Jahr desc,Monat desc)
       left  join BgBudget   BDG2 ON BDG2.BgFinanzplanID = FPL.BgFinanzplanID and -- erstes graues Budget
                                     BDG2.BgBewilligungStatusCode = 1 and
                                     BDG2.Masterbudget = 0 and
                                     BDG2.BgBudgetID = (select top 1 BgBudgetID
                                                        from   BgBudget
                                                        where  BgFinanzplanID = FPL.BgFinanzplanID and
                                                               Masterbudget = 0 and
                                                               BgBewilligungStatusCode = 1
                                                        order by Jahr,Monat)
       left  join BgBudget   BDG3 ON BDG3.BgFinanzplanID = FPL.BgFinanzplanID and -- erstes rotes Budget
                                     BDG3.BgBewilligungStatusCode = 9 and
                                     BDG3.Masterbudget = 0 and
                                     BDG3.BgBudgetID = (select top 1 BgBudgetID
                                                        from   BgBudget
                                                        where  BgFinanzplanID = FPL.BgFinanzplanID and
                                                               Masterbudget = 0 and
                                                               BgBewilligungStatusCode = 9
                                                        order by Jahr,Monat)
       left  join XLOVCode   FPS  ON FPS.LOVName = 'BgBewilligungStatus' and FPS.Code = FPL.BgBewilligungStatusCode
where ((@OrgGruppeID IS NULL AND @TeamID IS NULL) OR USR.OrgUnitID IN (SELECT OrgUnitID FROM dbo.fnOrgUnitsOfTeam(@OrgGruppeID,@TeamID)))
--- and USR.UserID =  {edtUserID.LookupID}
--- and ({edtNurAktive} = 0 OR LEI.DatumBis IS NULL) 
--- and ({edtNurLaufende} = 0 OR ( FPL.BgBewilligungStatusCode = 5 /*Bewilligung erteilt*/ AND GETDATE() &lt;= IsNull(FPL.DatumBis,FPL.GeplantBis)))

order by SZ$,OE$,MA$,Person</value>
