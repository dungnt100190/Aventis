  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>declare @Tage int

set @Tage = 500
--- set @Tage = {edtTage}

select SZ               = USR.Sozialzentrum,
       OE               = USR.OrgUnit,
       MA               = USR.NameVorname,
       FallNr           = FAL.FaFallID,
       Klient           = PRS.NameVorname,
       Belegnr          = BUC.BelegNr,
       LA               = KOA.KontoNr + ' ' + KOA.Name,
       Betrag           = BUK.Betrag,
       Splitting        = SPL.Text,
       [Verw. von]      = BUK.VerwPeriodeVon,
       [Verw. bis]      = BUK.VerwPeriodeBis,
       [Diff. in Tagen] = datediff(d,BUK.VerwPeriodeVon,BUK.VerwPeriodeBis),
       Tagesbetrag      = space(20-len(convert(varchar(20),convert(decimal(16,6),BUK.Betrag / datediff(d,BUK.VerwPeriodeVon,BUK.VerwPeriodeBis))))) +
                          convert(varchar(20),convert(decimal(16,6),BUK.Betrag / datediff(d,BUK.VerwPeriodeVon,BUK.VerwPeriodeBis))),  -- pseudo rechtsbündig
       Text             = BUK.Buchungstext,
       Belegstatus      = STA.Text,
       PSCDTransfer     = BUC.TransferDatum,
       FallBaPersonID$  = FAL.BaPersonID
from   KbBuchungBruttoPerson BUK
       inner join KbBuchungBrutto BUC on BUC.KbBuchungBruttoID = BUK.KbBuchungBruttoID
       inner join BgKostenart     KOA on KOA.BgKostenartID = BUC.BgKostenartID
       left  join BgPosition      BPO on BPO.BgPositionID = BUK.BgPositionID
       left  join BgBudget        BDG on BDG.BgBudgetID = BPO.BgBudgetID
       left  join BgFinanzplan    FPL on FPL.BgFinanzplanID = BDG.BgFinanzplanID
       left  join FaLeistung      LEI on LEI.FaLeistungID = FPL.FaLeistungID
       left  join FaFall          FAL on FAL.FaFallID = LEI.FaFallID
       left  join vwPerson        PRS on PRS.BaPersonID = LEI.BaPersonID
       left  join vwUser          USR on USR.UserID = LEI.UserID
       left  join XLOVCode        STA on STA.LOVName = 'KbBuchungsStatus' AND STA.Code = BUC.KbBuchungStatusCode
       left  join XLOVCode        SPL on SPL.LOVName = 'BgSplittingart' AND SPL.Code = KOA.BgSplittingartCode
where  datediff(d,BUK.VerwPeriodeVon,BUK.VerwPeriodeBis) &gt;= @Tage
order by [Diff. in Tagen] desc</value>
