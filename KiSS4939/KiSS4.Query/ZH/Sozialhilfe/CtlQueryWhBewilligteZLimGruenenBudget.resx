  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>select SZ              = USR.Sozialzentrum,
       OE              = USR.OrgUnit,
       MA              = USR.NameVorname,
       FallNr          = FAL.FaFallID,
       Klient          = PRS.NameVorname,
       Budget          = dbo.fnXKurzMonat(BDG.Monat) + convert(varchar,BDG.Jahr),
       Budgetstatus    = case 
                         when BDG.BgBewilligungStatusCode = 5 then 'grün'
                         when BDG.BgBewilligungStatusCode = 9 then 'rot'
                         end,
       LA              = BKA.KontoNr,
       Text            = BPO.Buchungstext,
       Betrag          = BPO.Betrag,
       Valuta          = BAT.Datum,
       Erstellt        = BPO.ErstelltDatum,
       [Erstellt MA]   = USR1.DisplayText, --USR1.Logon + ' - ' + USR1.NameVorname,
       Mutiert         = BPO.MutiertDatum,
       [Mutiert MA]    = USR2.DisplayText, --USR2.Logon + ' - ' + USR2.NameVorname
       FallBaPersonID$ = FAL.BaPersonID,
       BgFinanzplanID$ = FPL.BgFinanzplanID,
       BgBudgetID$     = BDG.BgBudgetID,
       BgPositionID$   = BPO.BgPositionID
from   vwBgPosition BPO
       left  join BgBudget                 BDG  ON BDG.BgBudgetID = BPO.BgBudgetID
       left  join BgFinanzplan             FPL  ON FPL.BgFinanzplanID = BDG.BgFinanzplanID
       left  join FaLeistung               LEI  ON LEI.FaLeistungID = FPL.FaLeistungID
       left  join FaFall                   FAL  ON FAL.FaFallID = LEI.FaFallID
       left  join BgAuszahlungPerson       BAP  ON BAP.BgPositionID = BPO.BgPositionID
       left  join BgAuszahlungPersontermin BAT  ON BAT.BgAuszahlungPersonID = BAP.BgAuszahlungPersonID
       left  join BgPositionsart           BPA  ON BPA.BgPositionsartID = BPO.BgPositionsartID
       left  join BgKostenart              BKA  ON BKA.BgKostenartID = BPA.BgKostenartID
       left  join vwUser                   USR  ON USR.UserID = LEI.UserID
       left  join vwPerson                 PRS  ON PRS.BaPersonID = LEI.BaPersonID
       left  join KbBuchungKostenart       BUK  ON BUK.BgPositionID = BPO.BgPositionID
       left  join vwUser                   USR1 ON USR1.UserID = BPO.ErstelltUserID
       left  join vwUser                   USR2 ON USR2.UserID = BPO.MutiertUserID
where  BPO.BgBewilligungStatusCode = 5 and
       BDG.Masterbudget = 0 and
       BDG.BgBewilligungStatusCode &gt;= 5 and
       BUK.KbBuchungKostenartID is null and
       BPO.BgSpezKontoID is null and
       BPO.betrag &lt;&gt; 0
order by BPO.ErstelltDatum</value>
