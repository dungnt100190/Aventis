  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>DECLARE @OrgGruppeID int
SET @OrgGruppeID = NULL
--- SET @OrgGruppeID = {ctlOrgUnitTeamUser.SucheGruppe}

DECLARE @TeamID int
SET @TeamID = NULL
--- SET @TeamID = {ctlOrgUnitTeamUser.SucheTeam}


select KBU.KbBuchungID,
       KBU.BelegNr,
       KBU.Betrag,
       KBU.Referenznummer,
       KBU.Text,
       KBU.BgBudgetID,
       LEI.FaFallID,
       LEI.FaLeistungID,
       KBU.KbBuchungStatusCode,
       BZ.BaPersonID,
       BZ.BaInstitutionID,
       BZ.BaZahlungswegID,
       FF.BaPersonID AS Falltraeger,
       BZ.Kreditor,
       BZ.Zusatzinfo,
       BZ.EinzahlungsscheinCode,
       CAST(KBU.KbBuchungTS AS BIGINT) AS KbBuchungTimestamp,
       KBU.KbBuchungTS,
       KBU.ValutaDatum,
       RLF.BelegDatum,
       PscdZahlwegArt = XLC.Value1,
       KBU.BeguenstigtName,
       KBU.BeguenstigtPostfach, 
       KBU.BeguenstigtStrasse,
       KBU.BeguenstigtHausNr,
       KBU.BeguenstigtPLZ,
       KBU.BeguenstigtOrt,
       KBU.BeguenstigtLandCode,
       KBU.PCKontoNr,
       KBU.BaBankID,
       KBU.BankKontoNr,
       KBU.IBAN,
       KBU.BankName,
       KBU.BankStrasse,
       KBU.BankPLZ,
       KBU.BankOrt,
       KBU.BankBCN,
       KBU.RuecklaeuferErledigt,
       KBU.RuecklaeuferBemerkung

from KbBuchung KBU
     INNER JOIN dbo.FaLeistung LEI ON LEI.FaLeistungID = KBU.FaLeistungID
     INNER JOIN (SELECT OpBuchungID, BelegDatum = MAX(RLF.BelegDatum)
                 FROM dbo.KbOpAusgleich     AUG
                   INNER JOIN dbo.KbBuchung RLF ON RLF.KbBuchungID = AUG.AusgleichBuchungID -- Rückläuferbuchung
                 GROUP BY OpBuchungID) RLF ON RLF.OpBuchungID = KBU.KbBuchungID
     INNER JOIN dbo.FaFall FF ON FF.FaFallID = LEI.FaFallID
     INNER JOIN dbo.VwKreditor BZ ON BZ.BaZahlungswegID = KBU.BaZahlungswegID
     LEFT  JOIN dbo.XLOVCode   XLC ON XLC.LOVName = 'BgEinzahlungsschein' AND XLC.Code = BZ.EinzahlungsscheinCode
     INNER JOIN dbo.vwUser     USR ON USR.UserID = LEI.UserID
WHERE ((@OrgGruppeID IS NULL AND @TeamID IS NULL) OR USR.OrgUnitID IN (SELECT OrgUnitID FROM dbo.fnOrgUnitsOfTeam(@OrgGruppeID,@TeamID)))
  AND KbBuchungStatusCode in (9,16)
--AND RLF.BelegDatum &gt;= '2008-08-01'
---AND RLF.BelegDatum &gt;= {edtDatumVon}
---AND LEI.UserID = {ctlOrgUnitTeamUser.SucheUserID.LookupID}
---AND (KBU.RuecklaeuferErledigt = {edtErledigteAnzeigen} OR KBU.RuecklaeuferErledigt = 0)
ORDER BY RLF.BelegDatum</value>
