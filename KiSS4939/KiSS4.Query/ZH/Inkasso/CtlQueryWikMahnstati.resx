  <data name="edtUser.LookupSQL" xml:space="preserve">
    <value>SELECT DISTINCT
  ID = USR.UserID,
  SAR = USR.LastName + isNull(', ' + USR.FirstName,''),
  [Kuerzel] = USR.LogonName
FROM FaLeistung LEI
  INNER JOIN XUser USR ON USR.UserID = LEI.UserID
WHERE LEI.FaProzessCode IN (301, 302, 304)
  AND (USR.LastName + isNull(', ' + USR.FirstName,'') LIKE {0} + '%' OR
       USR.LastName + isNull(' '  + USR.FirstName,'') LIKE {0} + '%' OR
       USR.LogonName LIKE {0} + '%')
  AND EXISTS (SELECT KbBuchungID FROM KbBuchung BUC WHERE BUC.FaLeistungID = LEI.FaLeistungID)
ORDER by SAR
----
</value>
