  <data name="qryForderungStatus.SelectStatement" xml:space="preserve">
    <value>SELECT
  status = '',
  value  = null
WHERE {0} = 's' -- Suche nach Sollstellungsdatum
UNION
SELECT
  status = CASE WHEN {0} = 's' THEN 'offen' ELSE 'ohne Zahlungseingang' END,
  value  = 'o'
UNION
SELECT
  status = CASE WHEN {0} = 's' THEN 'bezahlt' ELSE 'mit Zahlungseingang' END,
  value  = 'b'
</value>
