  <data name="edtSchuldner.LookupSQL" xml:space="preserve">
    <value>--DECLARE @p0 sql_variant
--SELECT @po = {0}
--SELECT @p0 = N'142261'
--SELECT @p0 = N'afu'
DECLARE @fallOrPersonID int
BEGIN TRY
  SELECT @fallOrPersonID = Convert(int, @p0)
END TRY
BEGIN CATCH
  SELECT @fallOrPersonID = -1
END CATCH
SELECT DISTINCT
  ID$ = BAP.BaPersonID, 
  Klient = BAP.Name + isNull(', ' + BAP.Vorname,''),
  [Person ID] = BAP.BaPersonID
FROM FaFall FAL
  INNER JOIN FaLeistung LEI ON LEI.FaFallID = FAL.FaFallID AND LEI.FaProzessCode IN (301, 302, 304)
  INNER JOIN BaPerson BAP ON BAP.BaPersonID = LEI.BaPersonID
WHERE BAP.Name + IsNull(', ' + BAP.Vorname,'') LIKE '%' + Convert(varchar(100), @p0) + '%' OR LEI.BaPersonID = @fallOrPersonID OR FAL.FaFallID = @fallOrPersonID
ORDER by Klient


--@p0= N'ob' </value>
