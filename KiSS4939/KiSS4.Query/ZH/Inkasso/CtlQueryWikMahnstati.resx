  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>DECLARE
  @UserID int,
  @SchuldnerBaPersonID int,
  @Datumsart char, -- z: Zahlungseingang, s: Sollstellung
  @DatumVon datetime,
  @DatumBis datetime,
  @forderungsStatus char, -- o: offen, b: bezahlt, null: alle
  @FaProzessCode int,
  @ForderungsArt char, -- p: Periodische, e: einmalige, null: alle
  @LeistungAktiv bit

--- SET @UserID = {edtUser.LookupID}
--- SET @SchuldnerBaPersonID = {edtSchuldner.LookupID}
--- SET @Datumsart = {edtDatumsart}
--- SET @DatumVon = {edtDatumVon.EditValue}
--- SET @DatumBis = {edtDatumBis.EditValue}
--- SET @forderungsStatus = {edtForderungStatus}
--- SET @FaProzessCode = {edtInkassoTyp}
--- SET @forderungsArt = {edtForderungsart}
--- SET @LeistungAktiv = {edtInkassoStatus}

--SELECT
--  FaFallID,
--  FaLeistungID,
--  FallBaPersonID,
--  KbBuchungID,
--  BuchungsText,
--  [Status],
--  Forderungsart,
--  SchuldnerNameVorname,
--  Verpflichtung,
--  Bezahlt,
--  Differenz,
--  ValutaDatum
--FROM dbo.fnWikForderungen(
--  @UserID,
--  @SchuldnerBaPersonID,
--  @DatumVon,
--  @DatumBis,
--  @Datumsart,
--  @FaProzessCode,
--  @forderungsStatus,
--  @ForderungsArt,
--  @LeistungAktiv
--)

EXEC dbo.spWikForderungen
  @UserID,
  @SchuldnerBaPersonID,
  @Datumsart,
  @DatumVon,
  @DatumBis,
  @forderungsStatus,
  @FaProzessCode,
  @ForderungsArt,
  @LeistungAktiv

</value>
