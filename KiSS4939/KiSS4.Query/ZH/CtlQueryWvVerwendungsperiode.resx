  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>DECLARE @BEDIncludeListe varchar(500)
--- SET @BEDIncludeListe = {edtBEDinclude} -- Wert wie er von KiSS übergeben wird
SET @BEDIncludeListe = @BEDIncludeListe + ','
DECLARE @tblBEDInclude TABLE (BEDInclude int)
DECLARE @Pos int, @OldPos int
SELECT @Pos = 1, @OldPos = 1
WHILE @Pos &lt; len(@BEDIncludeListe) BEGIN
  SET @Pos = CharIndex(',', @BEDIncludeListe, @OldPos)
  INSERT INTO @tblBEDInclude VALUES (SubString(@BEDIncludeListe, @OldPos, @Pos - @OldPos))
  SET @OldPos = @Pos + 1
END

DECLARE @BEDExcludeListe varchar(500)
--- SET @BEDExcludeListe = {edtBEDexclude} -- Wert wie er von KiSS übergeben wird
SET @BEDExcludeListe = @BEDExcludeListe + ','
DECLARE @tblBEDExclude TABLE (BEDExclude int)
SELECT @Pos = 1, @OldPos = 1
WHILE @Pos &lt; len(@BEDExcludeListe) BEGIN
  SET @Pos = CharIndex(',', @BEDExcludeListe, @OldPos)
  INSERT INTO @tblBEDExclude VALUES (SubString(@BEDExcludeListe, @OldPos, @Pos - @OldPos))
  SET @OldPos = @Pos + 1
END

SELECT distinct FallNr = LEI.FaFallID, KBB.BelegNr, KBB.Betrag, LA = BKA.KontoNr,  
		WVM.BaPersonID AS [WV-E-Mitglied], BAP.Name, BAP.Vorname, KBP.VerwPeriodeVon,[1.WV-DatumVon] = WVE.DatumVon, BAP.BaPersonID as BaPersonID$, BEDCode AS BED
FROM KbBuchungBrutto               KBB
  INNER JOIN FaLeistung            LEI ON LEI.FaLeistungID      = KBB.FaLeistungID
  INNER JOIN KbBuchungBruttoPerson KBP ON KBP.KbBuchungBruttoID = KBB.KbBuchungBruttoID AND KBP.VerwPeriodeVon = (select min(VerwPeriodeVon) from KbBuchungBruttoPerson where KbBuchungBruttoID = KBB.KbBuchungBruttoID)
  INNER JOIN BgKostenart		   BKA ON BKA.BgKostenartID		= KBB.BgKostenartID AND BKA.Weiterverrechenbar = 1
  INNER JOIN WhWVEinheitMitglied   WVM ON WVM.BaPersonID        = KBP.BaPersonID 
  INNER JOIN WhWVEinheit           WVE ON WVE.WhWVEinheitID     = (SELECT TOP 1 WVET.WhWVEinheitID FROM WhWVEinheit WVET
                                                                    INNER JOIN WhWVEinheitMitglied   WVMT ON WVMT.WhWVEinheitID = WVET.WhWVEinheitID  
                                                                    WHERE WVMT.BaPersonID = KBP.BaPersonID AND WVET.Ungueltig = 0 AND WVET.FaFallID = LEI.FaFallID
                                                                    ORDER BY WVET.DatumVon)  
  INNER JOIN BaPerson			   BAP ON BAP.BaPersonID		= KBP.BaPersonID
WHERE WVE.DatumVon &gt; KBP.VerwPeriodeVon 
and StorniertKbBuchungBruttoID is null
and NOT EXISTS (SELECT Top 1 1 FROM KbBuchungBrutto where StorniertKbBuchungBruttoID = KBB.KbBuchungBruttoID)
---and ((BEDCode not in (select * from @tblBEDExclude)) or ((select count(*) from @tblBEDExclude)=0))
---and ((BEDCode in (select * from @tblBEDInclude)) or ((select count(*) from @tblBEDInclude)=0))
ORDER BY LEI.FaFallID, KBB.BelegNr, KBP.VerwPeriodeVon</value>
