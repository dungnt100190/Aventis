  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>declare @Person table (
  BaPersonID int,
  FUserID     int,
  SUserID     int,
  IUserID     int,
  VUserID     int,
  AUserID     int
)

insert @Person (BaPersonID)
select distinct BaPersonID
from   FaLeistung 
where  DatumBis is null


update P
set    FUserID = (select top 1 FAL.UserID
                  from   FaLeistung FAL
                  where  FAL.BaPersonID = P.BaPersonID and
                         FAL.ModulID = 2 and
                         FAL.DatumBis is null
                  order by FAL.DatumVon desc),
       SUserID = (select top 1 FAL.UserID
                  from   FaLeistung FAL
                  where  FAL.BaPersonID = P.BaPersonID and
                         FAL.ModulID = 3 and
                         FAL.DatumBis is null
                  order by FAL.DatumVon desc),
       IUserID = (select top 1 FAL.UserID
                  from   FaLeistung FAL
                  where  FAL.BaPersonID = P.BaPersonID and
                         FAL.ModulID = 4 and
                         FAL.DatumBis is null
                  order by FAL.DatumVon desc),
       VUserID = (select top 1 FAL.UserID
                  from   FaLeistung FAL
                  where  FAL.BaPersonID = P.BaPersonID and
                         FAL.ModulID = 5 and
                         FAL.DatumBis is null
                  order by FAL.DatumVon desc),
       AUserID = (select top 1 FAL.UserID
                  from   FaLeistung FAL
                  where  FAL.BaPersonID = P.BaPersonID and
                         FAL.ModulID = 6 and
                         FAL.DatumBis is null
                  order by FAL.DatumVon desc)
from   @Person P

select Person = PRS.Name + isNull(', ' + PRS.Vorname,''),
       [SAR Fallführung]   = F.LastName + isNull(', ' + F.FirstName,''),
       [SAR Sozialhilfe]   = S.LastName + isNull(', ' + S.FirstName,''),
       [SAR Inkasso]       = I.LastName + isNull(', ' + I.FirstName,''),
       [SAR Vormundschaft] = V.LastName + isNull(', ' + V.FirstName,''),
       [SAR Asyl]          = A.LastName + isNull(', ' + A.FirstName,''),
       -- Zusatz              = PRS.NavigatorZusatz,
       AHVNummer       = PRS.AHVNummer,
       NNummer         = PRS.NNummer,
       Geburtsdatum    = PRS.Geburtsdatum,
       Geschlecht      = case PRS.GeschlechtCode
                         when 1 then 'm'
                         when 2 then 'f'
                         else '?'
                         end,
       Strasse         = ADR.Strasse,
       StrasseNr       = ADR.HausNr,
       PLZ             = ADR.PLZ,
       Ort             = ADR.Ort,
       Kanton          = ADR.Kanton,
       Zivilstand      = ZIV.Text,
       Nationalität    = NAT.Text,
       Heimatort            = HEI.Text,
       [Heimatort Kanton]    = HEI.Value2,
       --[Zuständige Gemeinde] = (select top 1 GMD.Text
       --                        from   FaLeistung FAL
       --                             inner join XLOVCode GMD on GMD.Lovname = 'GemeindeSozialdienst' and
       --                                               GMD.Code = FAL.GemeindeCode
       --                  where  FAL.BaPersonID = PRS.BaPersonID
       --                order by FAL.ModulID,FAL.DatumVon desc),
       BaPersonID$ = PRS.BaPersonID
from   @Person P
       inner join BaPerson        PRS on PRS.BaPersonID = P.BaPersonID
       left  join BaAdresse DWO on DWO.BaPersonID = PRS.BaPersonID and
                                          DWO.AdresseCode = 1
       left  join BaAdresse       ADR on ADR.BaAdresseID = DWO.BaAdresseID
       left  join BaLand          NAT on NAT.BaLandID = PRS.NationalitaetCode
       left  join XLOVCode         ZIV on ZIV.Code = PRS.ZivilstandCode and
                                          ZIV.LovName = 'Zivilstand'
       left  join XLOVCode         HEI on HEI.Code = PRS.HeimatGemeindeCode and
                                         HEI.LovName = 'Gemeinde'
       left  join XUser            F   on F.UserID = P.FUserID
       left  join XUser            S   on S.UserID = P.SUserID
       left  join XUser            I   on I.UserID = P.IUserID
       left  join XUser            V   on V.UserID = P.VUserID
       left  join XUser            A   on A.UserID = P.AUserID

where 1 = 1
-- AND (PRS.PersonSichtbarSDFlag = dbo.fnGetPersonSichtbarFlag({GetLogonName}) or PRS.PersonSichtbarSDFlag = 1) 
--- and {edtSAR} in (FUserID,SUserID,IUSerID,VUserID,AUserID)
--- and PRS.Name + isNull(', ' + PRS.Vorname,'') like {edtKlient} + '%'</value>
