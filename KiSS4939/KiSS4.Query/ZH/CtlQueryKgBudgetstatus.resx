  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>DECLARE @OrgGruppeID int
SET @OrgGruppeID = NULL
--- SET @OrgGruppeID = {ctlOrgUnitTeamUser.SucheGruppe}

DECLARE @TeamID int
SET @TeamID = NULL
--- SET @TeamID = {ctlOrgUnitTeamUser.SucheTeam}

DECLARE @NurAktive bit
--- SET @NurAktive = {edtNurAktive}

DECLARE @NurLaufende bit
--- SET @NurLaufende = {edtNurLaufende}

select SZ               = USR.Sozialzentrum,
       OE               = USR.OrgUnit,
       MA               = USR.NameVorname,
       FaFallID         = LEI.FaFallID,
       BaPersonID       = PRS.BaPersonID,
       Person           = PRS.NameVorname,
       GebDat           = PRS.Geburtsdatum,
       StatusMaster     = BEW.Text,
       BewilligtVon     = MST.BewilligtVon,
       BewilligtBis     = MST.BewilligtBis,
       grün             = dbo.fnXKurzMonat(BDG1.Monat) + ' ' + convert(varchar,BDG1.Jahr),
       grau             = dbo.fnXKurzMonat(BDG2.Monat) + ' ' + convert(varchar,BDG2.Jahr),
       rot              = dbo.fnXKurzMonat(BDG3.Monat) + ' ' + convert(varchar,BDG3.Jahr),
       FallBaPersonID$  = FAL.BaPersonID
from   KgBudget MST
       left  join FaLeistung LEI  ON LEI.FaLeistungID = MST.FaLeistungID
       left  join FaFall     FAL  ON FAL.FaFallID = LEI.FaFallID
       left  join vwPerson   PRS  ON PRS.BaPersonID = LEI.BaPersonID
       left  join vwUser     USR  ON USR.UserID = LEI.UserID
       left  join KgBudget   BDG1 ON BDG1.KgMasterBudgetID = MST.KgBudgetID and -- letztes grünes Budget
                                     BDG1.KgBewilligungCode = 5 and
                                     BDG1.KgBudgetID = (select top 1 KgBudgetID
                                                        from   KgBudget
                                                        where  KgMasterBudgetID = MST.KgBudgetID and
                                                               KgBewilligungCode = 5
                                                        order by Jahr desc,Monat desc)
       left  join KgBudget   BDG2 ON BDG2.KgMasterBudgetID = MST.KgBudgetID and -- erstes graues Budget
                                     BDG2.KgBewilligungCode = 1 and
                                     BDG2.KgBudgetID = (select top 1 KgBudgetID
                                                        from   KgBudget
                                                        where  KgMasterBudgetID = MST.KgBudgetID and
                                                               KgBewilligungCode = 1
                                                        order by Jahr,Monat)
       left  join KgBudget   BDG3 ON BDG3.KgMasterBudgetID = MST.KgBudgetID and -- erstes rotes Budget
                                     BDG3.KgBewilligungCode = 9 and
                                     BDG3.KgBudgetID = (select top 1 KgBudgetID
                                                        from   KgBudget
                                                        where  KgMasterBudgetID = MST.KgBudgetID and
                                                               KgBewilligungCode = 9
                                                        order by Jahr,Monat)
       left  join XLOVCode   BEW  ON BEW.LOVName = 'KgBewilligung' and BEW.Code = MST.KgBewilligungCode

    ---AND LEI.UserID = {ctlOrgUnitTeamUser.SucheUserID.LookupID}

where ((@OrgGruppeID IS NULL AND @TeamID IS NULL) OR USR.OrgUnitID IN (SELECT OrgUnitID FROM dbo.fnOrgUnitsOfTeam(@OrgGruppeID,@TeamID))) and
      MST.KgMasterBudgetID is null and 
      (@NurAktive = 0 OR LEI.DatumBis IS NULL) and 
      (@NurLaufende = 0 OR ( MST.KgBewilligungCode = 5 /*Bewilligung erteilt*/ AND GETDATE() &lt;= MST.BewilligtBis))
--- and USR.UserID =  {ctlOrgUnitTeamUser.SucheUserID.LookupID}
order by case when @NurLaufende &lt;&gt; 1 then USR.Sozialzentrum end,
         case when @NurLaufende &lt;&gt; 1 then USR.OrgUnit end,
         case when @NurLaufende &lt;&gt; 1 then USR.NameVorname end, 
         Person

</value>
