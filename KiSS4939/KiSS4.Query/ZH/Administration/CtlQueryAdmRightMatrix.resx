    <value>CREATE TABLE #Right (
  ClassName    varchar(255) COLLATE DATABASE_DEFAULT  NULL,
  RightID      int NOT NULL,
  DisplayText  varchar(300)
)

INSERT INTO #Right
  SELECT ClassName, 0, MOD.ShortName + ' - ' + CLS.ClassName
  FROM XClass          CLS
    INNER JOIN XModul  MOD ON MOD.ModulID = CLS.ModulID
  WHERE 1 = 1
---    AND MOD.ModulID = {edtModulID}

INSERT INTO #Right
  SELECT CLS.ClassName, RGT.RightID, IsNull(MOD.ShortName, 'UNB') + ' - ' + RGT.RightName
  FROM XRight         RGT
    LEFT JOIN XClass CLS   ON RGT.ClassName = CLS.ClassName
    LEFT JOIN XModul [MOD] ON MOD.ModulID = CLS.ModulID
  WHERE 1 = 1
---    AND MOD.ModulID = {edtModulID}

DECLARE @sqlAlterTable   nvarchar(4000),
        @sqlUpdate       nvarchar(4000)

DECLARE cUserGroup CURSOR FAST_FORWARD FOR
  SELECT
    'ALTER TABLE #Right ADD [' + UserGroupName + '] varchar(4) NOT NULL DEFAULT ('''')',
    'UPDATE RGT
  SET [' + UserGroupName + '] = ''R''
     + CASE WHEN UGR.MayInsert = 1 THEN ''E'' ELSE '' '' END
     + CASE WHEN UGR.MayUpdate = 1 THEN ''M'' ELSE '' '' END
     + CASE WHEN UGR.MayDelete = 1 THEN ''L'' ELSE '' '' END
FROM #Right                    RGT
  INNER JOIN XUserGroup_Right  UGR ON UGR.UserGroupID = ' + CONVERT(varchar, UserGroupID) + ' 
                                  AND (UGR.ClassName = RGT.ClassName
                                    OR UGR.ClassName IS NULL AND RGT.ClassName IS NULL)
                                  AND IsNull(UGR.RightID, 0) = RGT.RightID'
  FROM XUserGroup
  ORDER BY UserGroupName

OPEN cUserGroup
WHILE (1 = 1) BEGIN
  FETCH NEXT FROM cUserGroup INTO @sqlAlterTable, @sqlUpdate
  IF @@FETCH_STATUS &lt; 0 BREAK

  EXECUTE(@sqlAlterTable)
  EXECUTE(@sqlUpdate)
END
DEALLOCATE cUserGroup

SELECT *
FROM #Right
ORDER BY DisplayText

DROP TABLE #Right</value>
