  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>DECLARE @SucheElektronisch BIT;
DECLARE @SucheBarauszahlung BIT;
DECLARE @SucheTransferDatumVon DATETIME;
DECLARE @SucheTransferDatumBis DATETIME;
DECLARE @SucheBarbezugDatumVon DATETIME;
DECLARE @SucheBarbezugDatumBis DATETIME;

SET @SucheElektronisch = 0;
SET @SucheBarauszahlung = 0;
SET @SucheTransferDatumVon = '17530101';
SET @SucheTransferDatumBis = '99991231';
SET @SucheBarbezugDatumVon = '17530101';
SET @SucheBarbezugDatumBis = '99991231';

--- SET @SucheElektronisch = 1; -- {edtSucheTransferDatumVon}
--- SET @SucheElektronisch = 1; -- {edtSucheTransferDatumBis}

--- SET @SucheBarauszahlung = 1; -- {edtSucheBarbezugDatumVon}
--- SET @SucheBarauszahlung = 1; -- {edtSucheBarbezugDatumBis}

--- SET @SucheTransferDatumVon = {edtSucheTransferDatumVon};
--- SET @SucheTransferDatumBis = DATEADD(MS,-10,DATEADD(D,1,{edtSucheTransferDatumBis}));
--- SET @SucheBarbezugDatumVon = {edtSucheBarbezugDatumVon};
--- SET @SucheBarbezugDatumBis = DATEADD(MS,-10,DATEADD(D,1,{edtSucheBarbezugDatumBis}));

SELECT Transferdatum   = BUC.TransferDatum,
       Zeit            = CONVERT(VARCHAR,BUC.TransferDatum,108),
       Valuta          = BUC.ValutaDatum,
       Ausgleichsdatum = BUC.BarbezugDatum,
       BelegNr         = BUC.KgBuchungID,
       Mandant         = PRS.NameVorname,
       Betrag          = BUC.Betrag,
       Auszahlungsart  = AUS.ShortText,
       Kreditor        = BUC.BeguenstigtName + ISNULL(', ' + BUC.BeguenstigtName2,''),
       Zahlinfo        = CASE BUC.KgAuszahlungsArtCode
                         WHEN 101 THEN EIZ.ShortText +                                      
                                       ISNULL(', ' + dbo.fnTnToPc(ISNULL(BUC.PostKontoNummer,BNK.PCKontoNr)),'') + 
                                       ISNULL(', ' + BNK.Name,'') + 
                                       ISNULL(', ' + BUC.BankKontoNummer, '') +
                                       ISNULL(', ' + BUC.IBANNummer,'')
                         WHEN 103 THEN BUC.MitteilungZeile1 + 
                                       ISNULL(', ' + BUC.MitteilungZeile2,'') +
                                       ISNULL(', ' + BUC.MitteilungZeile3,'')
                         END,
       BaPersonID$     = FAL.BaPersonID
FROM dbo.KgBuchung         BUC 
  LEFT JOIN dbo.KgPeriode  PER ON PER.KgPeriodeID = BUC.KgPeriodeID
  LEFT JOIN dbo.FaLeistung LEI ON LEI.FaLeistungID = PER.FaLeistungID 
  LEFT JOIN dbo.FaFall     FAL ON FAL.FaFallID = LEI.FaFallID 
  LEFT JOIN dbo.vwPerson   PRS ON PRS.BaPersonID = LEI.BaPersonID 
  LEFT JOIN dbo.BaBank     BNK ON BNK.BaBankID = BUC.BaBankID
  LEFT JOIN dbo.XLOVCode   EIZ ON EIZ.LOVName = 'BgEinzahlungsschein' 
                              AND EIZ.Code = BUC.EinzahlungsscheinCode
  LEFT JOIN dbo.XLOVCode   AUS ON AUS.LOVName = 'KgAuszahlungsArt' 
                              AND AUS.Code = BUC.KgAuszahlungsArtCode
WHERE BUC.TransferDatum IS NOT NULL
  AND (
    -- elektronische Auszahlung
    (@SucheElektronisch = 1
      AND (BUC.KgAuszahlungsArtCode = 101 OR BUC.KgAuszahlungsArtCode IS NULL)
      AND BUC.TransferDatum BETWEEN @SucheTransferDatumVon AND @SucheTransferDatumBis)
    OR
    -- Barauszahlung
    (@SucheBarauszahlung = 1
      AND BUC.KgAuszahlungsArtCode = 103
      AND BUC.BarbezugDatum BETWEEN @SucheBarbezugDatumVon AND @SucheBarbezugDatumBis)
  )
ORDER BY CONVERT(DATETIME,CONVERT(VARCHAR(10),BUC.TransferDatum,104),104), BUC.ValutaDatum;</value>
