  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>select Typ              = 'Person',
       Nr               = PRS.BaPersonID,
       Name             = NameVorname,
       PLZ              = WohnsitzPLZ,
       Ort              = WohnsitzOrt,
       Land             = WohnsitzLand,
       Zahlweg          = KRE.Zahlungsweg,
       [Klient/in]      = case when exists(select 1 from BgFinanzplan_BaPerson where BaPersonID = PRS.BaPersonID) then 'W' else '' end +
                          case when exists(select 1 from IkGlaeubiger where BaPersonID = PRS.BaPersonID) then 'A' else '' end +
                          case when exists(select 1 from FaLeistung where BaPersonID = PRS.BaPersonID and FaProzessCode = 500) then 'K' else '' end,
       [Verw. Zahlinfo] = case when exists(select 1 from BgAuszahlungPerson where BaZahlungswegID = KRE.BaZahlungswegID) then 'W' else '' end +
                          case when exists(select 1 from IkGlaeubiger where BaZahlungswegID = KRE.BaZahlungswegID or
                                                                            _old_ZusatzBaZahlungswegID = KRE.BaZahlungswegID) then 'A' else '' end +
                          case when exists(select 1 from KgPosition where BaZahlungswegID = KRE.BaZahlungswegID) then 'K' else '' end,
       [Belege]         = convert(bit,case when exists(select 1 from KbBuchung
                                                     where BaZahlungswegID = KRE.BaZahlungswegID) or
                                              exists(select 1 from KgBuchung
                                                     where BaZahlungswegID = KRE.BaZahlungswegID) then 1 else 0 end),
       BaPersonID$ = PRS.BaPersonID
from   vwPerson PRS
       left join vwKreditor KRE on KRE.BaPersonID = PRS.BaPersonID
where  isNull(WohnsitzLand,'Schweiz') &lt;&gt; 'Schweiz'

union all

select Typ              = 'Institution',
       Nr               = INS.BaInstitutionID,
       Name             = INS.Name,
       PLZ              = PLZ,
       Ort              = Ort,
       Land             = Land,
       Zahlweg          = KRE.Zahlungsweg,
       [Klient/in]      = '',
       [Verw. Zahlinfo] = case when exists(select 1 from BgAuszahlungPerson where BaZahlungswegID = KRE.BaZahlungswegID) then 'W' else '' end +
                          case when exists(select 1 from IkGlaeubiger where BaZahlungswegID = KRE.BaZahlungswegID or
                                                                            _old_ZusatzBaZahlungswegID = KRE.BaZahlungswegID) then 'A' else '' end +
                          case when exists(select 1 from KgPosition where BaZahlungswegID = KRE.BaZahlungswegID) then 'K' else '' end,
       [Belege]         = convert(bit,case when exists(select 1 from KbBuchung
                                                       where BaZahlungswegID = KRE.BaZahlungswegID) or
                                                exists(select 1 from KgBuchung
                                                       where BaZahlungswegID = KRE.BaZahlungswegID) then 1 else 0 end),
       BaPersonID$ = null
from   vwInstitution INS
       left join vwKreditor KRE on KRE.BaInstitutionID = INS.BaInstitutionID
where  isNull(INS.Land,'Schweiz') &lt;&gt; 'Schweiz'

order by Typ desc, Name</value>
