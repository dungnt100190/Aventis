  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>DECLARE @DatumVon DATETIME
DECLARE @DatumBis DATETIME

SET @DatumVon = {edtDatumVon}
SET @DatumBis = {edtDatumBis}

DECLARE @Data Table(
   DatumVon               DATETIME,
   DatumBis               DATETIME,
   UHVDatum               DATETIME,
   SorgerechtVereinbDatum DATETIME,
   Unterhaltsvertrag      VARCHAR (100),
   SAR                    VARCHAR (100)
)

DECLARE @Result Table(
   [SAR/Grund]               VARCHAR (100),
   Errichtung        INT,
   Abschluss         INT,
   Unterhaltsvertrag INT,
   Sortkey$          INT
)

INSERT  @Data
SELECT FAL.DatumVon,
       FAL.DatumBis,
       VTR.UHVDatum,
       VTR.SorgerechtVereinbDatum,
       Unterhaltsvertrag = (SELECT [dbo].[fnLOVText]('VmVaterschaftAbschlussgrund', FAL.AbschlussgrundCode)),
       SAR = USR.LastName + isNull(', ' + USR.FirstName,'')
FROM dbo.FaLeistung            FAL WITH (READUNCOMMITTED)
  INNER JOIN dbo.XUser         USR WITH (READUNCOMMITTED) ON USR.UserID = FAL.UserID
  LEFT  JOIN dbo.VmVaterschaft VTR WITH (READUNCOMMITTED) ON FAL.FaLeistungID = VTR.FaLeistungID
WHERE FAL.ModulID = 5 --Vormundschaft
  AND FAL.FaProzessCode = 502 --Vaterschaftsabklärung
--- AND FAL.DatumVon BETWEEN @DatumVon AND @DatumBis

DECLARE @SAR               VARCHAR (100)
DECLARE @Errichtungen      INT
DECLARE @Abschlüsse        INT
DECLARE @Unterhaltsverträge INT

DECLARE cursorSAR CURSOR STATIC FOR
   SELECT USR.LastName + isNull(', ' + USR.FirstName,'')
   FROM dbo.XUser USR WITH (READUNCOMMITTED)

OPEN cursorSAR
   FETCH NEXT FROM cursorSAR INTO @SAR
   WHILE @@fetch_status = 0 BEGIN
      SET @Errichtungen = (SELECT COUNT(*)
                          FROM @DATA
                          WHERE DatumVon BETWEEN @DatumVon AND @DatumBis
                                AND SAR = @SAR)
      SET @Abschlüsse = (SELECT COUNT(*)
                         FROM @DATA
                         WHERE DatumBis BETWEEN @DatumVon AND @DatumBis
                               AND SAR = @SAR)
      SET @Unterhaltsverträge = (SELECT COUNT(*)
                                FROM @DATA
                                WHERE (UHVDatum BETWEEN @DatumVon AND @DatumBis
                                      OR SorgerechtVereinbDatum BETWEEN @DatumVon AND @DatumBis)
                                      AND SAR = @SAR)
      INSERT @Result
      SELECT @SAR,
             @Errichtungen,
             @Abschlüsse,
             @Unterhaltsverträge,
             1 --Sortkey$
             WHERE @Errichtungen &gt; 0 OR @Abschlüsse &gt; 0 OR @Unterhaltsverträge &gt; 0
      FETCH NEXT FROM cursorSAR INTO @SAR
   END
CLOSE cursorSAR
DEALLOCATE cursorSAR

INSERT @Result
SELECT 'Total', SUM(Errichtung),SUM(Abschluss), SUM(Unterhaltsvertrag), 3
FROM @Result

INSERT @Result SELECT NULL, NULL, NULL, NULL, 4 --Leerzeile

INSERT @Result SELECT 'Abschlussgründe', NULL, NULL, NULL, 4 --Titel

INSERT @Result
SELECT Unterhaltsvertrag,
       NULL,
       COUNT(*),
       NULL,
       5
FROM @Data
WHERE DatumBis IS NOT NULL
GROUP BY Unterhaltsvertrag

UPDATE @Result SET [SAR/Grund] = 'nicht angegeben' WHERE [SAR/Grund] = ' ' AND Sortkey$ = 5

SELECT [SAR/Grund], Errichtung, Abschluss, Unterhaltsvertrag 
FROM @Result
ORDER BY Sortkey$</value>
