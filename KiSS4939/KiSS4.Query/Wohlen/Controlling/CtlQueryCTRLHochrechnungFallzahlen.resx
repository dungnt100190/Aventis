  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>--Cockpit Hochrechnung Fallzahlen

DECLARE @DatumVon DATETIME
DECLARE @DatumBis DATETIME
DECLARE @Kennzahl INT

DECLARE @hochrechnungsFaktor FLOAT
DECLARE @laufendeFälle INT
DECLARE @aufnahmen INT
DECLARE @abschlüsse INT

SET @DatumVon = {edtDatumVon}
SET @DatumBis = {edtDatumBis}

IF DATEDIFF(DAY, @DatumVon, GETDATE())&lt;1
   SET @hochrechnungsFaktor = 0
ELSE
   SET @hochrechnungsFaktor = CONVERT(FLOAT, DATEDIFF(DAY, GETDATE(), @DatumBis)) / DATEDIFF(DAY, @DatumVon, GETDATE())

DECLARE @Hochrechnung TABLE
(
   Beschreibung VARCHAR (100),
   Kennzahl     INT,
   SortKey$     INT
)

SET @laufendeFälle =
-- Laufende Fälle per @DatumVon
           (SELECT COUNT(*)
                 FROM dbo.FaLeistung FAL WITH (READUNCOMMITTED)
                 WHERE FAL.ModulID = 2 and
                       FAL.DatumVon &lt; @DatumVon and
                      (FAL.DatumBis &gt;= @DatumVon or
                       FAL.DatumBis IS NULL)
            )
INSERT INTO @Hochrechnung
SELECT 'Laufende Fälle per '+ CONVERT(VARCHAR, @DatumVon, 104), @laufendeFälle, 1

-- Aufnahmen im Auswertungszeitraum (@DatumVon bis GETDATE())
SET @aufnahmen =
(SELECT COUNT(*)
                 FROM dbo.FaLeistung FAL WITH (READUNCOMMITTED)
                 WHERE ModulID = 2 AND
                       DatumVon BETWEEN @DatumVon AND GETDATE()
                 )
INSERT INTO @Hochrechnung
SELECT 'Aufnahmen bisher effektiv', @aufnahmen, 2
INSERT INTO @Hochrechnung
SELECT 'Aufnahmen hochgerechnet', @aufnahmen*@HochrechnungsFaktor, 10

-- Total im Auswertungszeitraum bearbeitete Fälle
SET @Kennzahl =
(SELECT COUNT(*)
                 FROM dbo.FaLeistung FAL WITH (READUNCOMMITTED)
                 WHERE ModulID = 2 AND
                     ( (FAL.DatumVon &lt;= @DatumVon AND (FAL.DatumBis &gt;= @DatumVon OR FAL.DatumBis IS NULL))
                                    OR
                      (FAL.DatumVon BETWEEN @DatumVon AND GETDATE()))
)
INSERT INTO @Hochrechnung
SELECT 'Total bearbeitete Fälle', @Kennzahl, 3
INSERT INTO @Hochrechnung
SELECT 'Total bearbeitete Fälle hochgerechnet', @laufendeFälle+@aufnahmen*@HochrechnungsFaktor, 11

--#Abschlüsse
SET @abschlüsse =
(SELECT COUNT(*) FROM dbo.FaLeistung WITH (READUNCOMMITTED) WHERE ModulID = 2 AND
              DatumBis BETWEEN @DatumVon AND GETDATE()
)
INSERT @Hochrechnung
SELECT 'Abschlüsse bisher effektiv', @abschlüsse, 4
INSERT @Hochrechnung
SELECT 'Abschlüsse hochgerechnet', @abschlüsse*@HochrechnungsFaktor, 12

-- Laufende Fälle per @DatumVon
SET @Kennzahl =
           (SELECT COUNT(*)
                 FROM dbo.FaLeistung FAL WITH (READUNCOMMITTED)
                 WHERE FAL.ModulID = 2 and
                       FAL.DatumVon &lt; GETDATE() and
                      (FAL.DatumBis &gt;= GETDATE() or
                       FAL.DatumBis IS NULL)
            )
INSERT @Hochrechnung
SELECT 'Laufende Fälle per '+ CONVERT(VARCHAR, GETDATE(), 104), @Kennzahl, 5
INSERT @Hochrechnung
SELECT 'Laufende Fälle hochgerechnet per '+ CONVERT(VARCHAR, @DatumBis, 104), @laufendeFälle+@HochrechnungsFaktor*(@aufnahmen-@abschlüsse), 13


SELECT Beschreibung, Kennzahl 
FROM @Hochrechnung
ORDER BY SortKey$</value>
