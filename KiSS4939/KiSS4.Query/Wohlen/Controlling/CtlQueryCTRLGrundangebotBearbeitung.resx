  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>DECLARE @DatumVon DATETIME
DECLARE @DatumBis DATETIME

SET @DatumVon = {edtDatumVon}--'20050101'--
SET @DatumBis = {edtDatumBis}--'20051231'--

DECLARE @GrundangebotBearbeitungFldID INT
DECLARE @GrundangebotBedarfFldID      INT
SET @GrundangebotBearbeitungFldID   = (SELECT DynaFieldID FROM dbo.DynaField WITH (READUNCOMMITTED) WHERE FieldName = 'FaBeratungsplanGrundangebot')
SET @GrundangebotBedarfFldID        = (SELECT DynaFieldID FROM dbo.DynaField WITH (READUNCOMMITTED) WHERE FieldName = 'FaGrundangebotBedarf')

DECLARE @BearbeitungVsBedarf TABLE(
   Bearbeitung VARCHAR (50),
   Bedarf      VARCHAR (50)
)

DECLARE @Kennzahlen TABLE(
   Grundangebot VARCHAR (50),
   Bearbeitung  INT,
   Bedarf       INT
)

INSERT @BearbeitungVsBedarf

SELECT GrundangebotBearbeitung = CASE when PHS.FaPhaseCode = 1 then 'Assessment'
                                      else ISNULL(XGBE.Text, 'Keine Angabe')
                                 end,
       GrundangebotBedarf = CASE when PHS.FaPhaseCode = 1 then 'Assessment'
                                 else ISNULL(XGBD.Text, 'Keine Angabe')
                            end
FROM dbo.FaPhase PHS WITH (READUNCOMMITTED)
LEFT JOIN dbo.DynaValue GBE WITH (READUNCOMMITTED) ON GBE.FaPhaseID = PHS.FaPhaseID                    AND
                           GBE.DynaFieldID = @GrundangebotBearbeitungFldID AND
                           GBE.GridRowID = 1                               AND
                           GBE.Value IS NOT NULL                           AND
                           SQL_VARIANT_PROPERTY(GBE.Value,'BaseType')='int'
LEFT JOIN dbo.XLOVCode XGBE WITH (READUNCOMMITTED) ON XGBE.LovName  = 'FaGrundangebot' AND XGBE.Code = GBE.Value
LEFT JOIN dbo.DynaValue GBD WITH (READUNCOMMITTED) ON GBD.FaPhaseID = PHS.FaPhaseID               AND
                           GBD.DynaFieldID = @GrundangebotBedarfFldID AND
                           GBD.GridRowID = 1                          AND
                           GBD.Value IS NOT NULL                      AND
                           SQL_VARIANT_PROPERTY(GBD.Value,'BaseType')='int'
LEFT JOIN dbo.XLOVCode XGBD WITH (READUNCOMMITTED) ON XGBD.LovName  = 'FaGrundangebot' AND XGBD.Code = GBD.Value
WHERE PHS.DatumBis BETWEEN @DatumVon AND @DatumBis

DECLARE @grundAngebot VARCHAR(200)
DECLARE @bearbeitung INT

DECLARE cursorGrundAngebot CURSOR STATIC FOR
   SELECT Bearbeitung, COUNT(*)
   FROM @BearbeitungVsBedarf
   GROUP BY Bearbeitung
   OPEN cursorGrundAngebot
   FETCH NEXT FROM cursorGrundAngebot INTO @grundAngebot, @bearbeitung
   WHILE @@fetch_status = 0 BEGIN
      INSERT INTO @Kennzahlen
      SELECT @grundAngebot, @bearbeitung, COUNT(*)
      FROM @BearbeitungVsBedarf
      WHERE Bedarf = @grundAngebot
      FETCH NEXT FROM cursorGrundAngebot INTO @grundAngebot, @bearbeitung
   END
CLOSE cursorGrundAngebot
DEALLOCATE cursorGrundAngebot


SELECT * 
FROM @Kennzahlen</value>
