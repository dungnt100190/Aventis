    <value>/*
* analog VM AHV-Mindestbeitäge
* Decisive Field:  @VersicherungsartFldID
*
*/
DECLARE @VersicherungsartFldID INT,
        @AdressatFldID         INT,
        @GrundprämieFldID       INT,
        @ZahlungsturnusFldID   INT,
        @LaufzeitBis           INT

EXEC dbo.spGetDynaFldIDfromTextmarke 'VmMfSachversVersicherungsart', @VersicherungsartFldID OUT
EXEC dbo.spGetDynaFldIDfromTextmarke 'VmMfSachversAdressat', @AdressatFldID OUT
EXEC dbo.spGetDynaFldIDfromTextmarke 'VmMfSachversGrundpämie', @GrundprämieFldID OUT
EXEC dbo.spGetDynaFldIDfromTextmarke 'VmMfSachversZahlungsturnus', @ZahlungsturnusFldID OUT
EXEC dbo.spGetDynaFldIDfromTextmarke 'VmMfSachversLaufzeitBis', @LaufzeitBis OUT

SELECT SAR              = BEN.LastName + ISNULL(', ' + BEN.FirstName,''),
       Kürzel           = BEN.ShortName,
       Person           = PRS.Name + ISNULL(', ' + PRS.Vorname,''),
       Versicherungsart = dbo.fnLOVText('vmVersicherungsartSachversicherung', CONVERT(INT, VSA.Value)),
       [Gesellschaft/Adressat] = CASE WHEN CONVERT(INT, ADR.Value) &gt; 1 THEN
                                           (SELECT Name + isNull(', ' + Vorname,'')
                                            FROM   dbo.BaPerson WITH (READUNCOMMITTED)
                                            WHERE  BaPersonID = CONVERT(INT, ADR.Value))
                                       ELSE
                                           (SELECT ORG.Name + isnull(', ' + ADR2.Ort,'')
                                            FROM   dbo.BaInstitution ORG WITH (READUNCOMMITTED)
                                            LEFT JOIN dbo.BaAdresse   ADR2 WITH (READUNCOMMITTED) ON ADR2.BaInstitutionID = ORG.BaInstitutionID
                                            WHERE  ORG.BaInstitutionID = - CONVERT(INT, ADR.Value))
                                  END,
       Prämie           = GPR.Value,
       Fälligkeit       = dbo.fnLOVText('vmZahlungsturnus', CONVERT(INT, ZTU.Value)),
       [Laufzeit bis]   = CONVERT(datetime, LZB.Value, 112),
       UserID$          = BEN.UserID,
       BaPersonID$     = PRS.BaPersonID
FROM dbo.BaPerson PRS WITH (READUNCOMMITTED)
INNER JOIN dbo.FaLeistung FAL WITH (READUNCOMMITTED) ON FAL.BaPersonID = PRS.BaPersonID AND
                         FAL.ModulID = 5 --Vormundschaft
INNER JOIN dbo.VmMassnahme MSN WITH (READUNCOMMITTED) ON MSN.FaLeistungID = FAL.FaLeistungID
INNER JOIN dbo.XUser       BEN WITH (READUNCOMMITTED) ON FAL.UserID = BEN.UserID
INNER JOIN dbo.DynaValue   VSA WITH (READUNCOMMITTED) ON VSA.FaLeistungID = FAL.FaLeistungID
                          AND VSA.DynaFieldID = @VersicherungsartFldID
LEFT JOIN dbo.DynaValue    ADR WITH (READUNCOMMITTED) ON ADR.FaLeistungID = FAL.FaLeistungID
                          AND ADR.DynaFieldID = @AdressatFldID
                          AND ADR.GridRowID = VSA.GridRowID
LEFT JOIN dbo.DynaValue    GPR WITH (READUNCOMMITTED) ON GPR.FaLeistungID = FAL.FaLeistungID
                          AND GPR.DynaFieldID = @GrundprämieFldID
                          AND GPR.GridRowID = VSA.GridRowID
LEFT JOIN dbo.DynaValue    ZTU WITH (READUNCOMMITTED) ON ZTU.FaLeistungID = FAL.FaLeistungID
                          AND ZTU.DynaFieldID = @ZahlungsturnusFldID
                          AND ZTU.GridRowID = VSA.GridRowID
LEFT JOIN dbo.DynaValue    LZB WITH (READUNCOMMITTED) ON LZB.FaLeistungID = FAL.FaLeistungID
                          AND LZB.DynaFieldID = @LaufzeitBis
                          AND LZB.GridRowID = VSA.GridRowID

WHERE 1=1
--- AND PRS.BaPersonID = {edtBaPersonID.LookupID}
--- AND BEN.UserID =  {edtUserID.LookupID}
--- AND ({edtNurAktive} = 0 or FAL.DatumBis is null)
ORDER BY SAR, Person</value>
