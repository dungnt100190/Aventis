  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>SELECT [Zust. PriMa]     = PMA.Name + isNull(', ' + PMA.Vorname,''),
       [Strasse PriMa]   = PRAD.Strasse,
       [StrasseNr PriMa] = PRAD.HausNr,
       [PLZ PriMa]       = PRAD.PLZ,
       [Wohnort PriMa]   = PRAD.Ort,
       [E-Mail PriMa]    = PMA.EMail,
       Errichtungsdatum  = ERN.Ernennung,
       Person            = PRS.NameVorname,
       Strasse           = PRS.WohnsitzStrasse,
       StrasseNr         = PRS.WohnsitzHausNr,
       PLZ               = PRS.WohnsitzPLZ,
       Wohnort           = PRS.WohnsitzOrt,
       [Aufenthaltsort]  = isNull(PRS.AufenthaltPLZOrt,'') +
                           isNull(' (' + PRS.AufenthaltKanton + ')','') +
                           case when ltrim(rtrim(isNull(PRS.AufenthaltStrasse,''))) &lt;&gt; ''
                           then isNull(', ' + PRS.AufenthaltStrasse,'') + isNull(' ' + PRS.AufenthaltHausNr,'')
                           else ''
                           end,
       BaPersonID$     = PRS.BaPersonID
FROM dbo.VmMassnahme MSN WITH (READUNCOMMITTED)
INNER JOIN dbo.VmErnennung ERN WITH (READUNCOMMITTED) ON ERN.VmMassnahmeID = MSN.VmMassnahmeID AND
                              ERN.VmPriMaID IS NOT NULL
INNER JOIN dbo.VmPriMa PMA WITH (READUNCOMMITTED) ON PMA.VmPriMaID = ERN.VmPriMaID
INNER JOIN dbo.FaLeistung FAL WITH (READUNCOMMITTED) ON FAL.FaLeistungID = MSN.FaLeistungID
INNER JOIN dbo.vwPerson PRS ON PRS.BaPersonID = FAL.BaPersonID
INNER JOIN dbo.BaAdresse PRAD WITH (READUNCOMMITTED) ON PRAD.VmPriMaID = PMA.VmPriMaID

WHERE   FAL.ModulID   = 5 
AND FAL.FaProzessCode = 501
AND MSN.IsDeleted = 0
AND ERN.IsDeleted = 0
--- AND PRS.BaPersonID = {edtBaPersonID.LookupID}
--- AND PMA.VmPriMaID = {edtVmPriMaID.LookupID}
--- AND ({edtNurAktive} = 0 or FAL.DatumBis is null)
--- AND PRS.Geburtsdatum &gt;= {edtGeburtVon}
--- AND PRS.Geburtsdatum &lt;= {edtGeburtBis}

ORDER BY [Zust. PriMa], Person</value>
