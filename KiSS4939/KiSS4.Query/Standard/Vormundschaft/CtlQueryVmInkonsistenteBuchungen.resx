  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>SELECT
  Klient = PRS.Name + IsNull(' ' + PRS.Vorname, ''),
  FBU.BelegNr, FBU.Buchungsdatum, FBU.ValutaDatum,
  SollKtoNr      = NullIf(DAU.SollKtoNr, FBU.SollKtoNr),
  HabenKtoNr     = NullIf(DAU.HabenKtoNr, FBU.HabenKtoNr),
  Betrag         = NullIf(DAU.Betrag, FBU.Betrag),
  Zahlungsgrund  = NullIf(DAU.Zahlungsgrund, FBU.Zahlungsgrund),

  Zahlungsweg    = CASE WHEN FBU.FbZahlungswegID &lt;&gt; FZW.FbZahlungswegID THEN 'x' ELSE '' END,
  PCKontoNr      = NullIf(FZW.PCKontoNr, FBU.PCKontoNr),
  BankKontoNr    = NullIf(FZW.BankKontoNr, FBU.BankKontoNr),
  ReferenzNummer = NullIf(DAU.ReferenzNummer, FBU.ReferenzNummer),
  BaPersonID$ = PRS.BaPersonID
FROM dbo.FbBuchung               FBU WITH (READUNCOMMITTED)
  INNER JOIN dbo.FbDauerauftrag  DAU WITH (READUNCOMMITTED) ON DAU.FbDauerauftragID = FBU.FbDauerauftragID
  INNER JOIN dbo.BaPerson       PRS WITH (READUNCOMMITTED) ON PRS.BaPersonID = DAU.BaPersonID
  INNER JOIN dbo.FbZahlungsweg   FZW WITH (READUNCOMMITTED) ON FZW.FbZahlungswegID = DAU.FbZahlungswegID
WHERE FBU.ValutaDatum &gt; DATEADD(m, -1, GetDate())
  AND (FBU.Betrag          &lt;&gt; DAU.Betrag
        OR FBU.SollKtoNr       &lt;&gt; DAU.SollKtoNr
        OR FBU.HabenKtoNr      &lt;&gt; DAU.HabenKtoNr
        OR FBU.ReferenzNummer  &lt;&gt; DAU.ReferenzNummer
        OR FBU.Zahlungsgrund   &lt;&gt; DAU.Zahlungsgrund

        OR FBU.FbZahlungswegID &lt;&gt; DAU.FbZahlungswegID
        OR FBU.PCKontoNr       &lt;&gt; FZW.PCKontoNr
        OR FBU.BankKontoNr     &lt;&gt; FZW.BankKontoNr)</value>
  </data>
