  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>select BaPersonID$          = PRS.BaPersonID,
       [Mandant/-in]         = PRS.Name + isNull(', ' + PRS.Vorname,''),
       Mutter                = (select top 1 MUT.Name + isNull(', ' + MUT.Vorname,'')
                                from   dbo.BaPerson_Relation DPR WITH (READUNCOMMITTED)
                                       left join dbo.BaPerson        MUT WITH (READUNCOMMITTED) on MUT.BaPersonID = DPR.BaPersonID_1 and
                                                                         MUT.GeschlechtCode = 2
                                where  DPR.BaPersonID_2 = PRS.BaPersonID and
                                       DPR.BaRelationID = 1),
       Vater                 = (select top 1 VAT.Name + isNull(', ' + VAT.Vorname,'')
                                from   dbo.BaPerson_Relation DPR WITH (READUNCOMMITTED)
                                       left join dbo.BaPerson        VAT WITH (READUNCOMMITTED) on VAT.BaPersonID = DPR.BaPersonID_1 and
                                                                         VAT.GeschlechtCode = 1
                                where  DPR.BaPersonID_2 = PRS.BaPersonID and
                                       DPR.BaRelationID = 1),
       Geburtsdatum          = PRS.Geburtsdatum,
       [Falleröffnung VM]    = FAL.DatumVon,
       [Fallabschluss VM]    = FAL.DatumBis,
       archiviert            = convert(bit,case when ARC.FaLeistungID is null then 0 else 1 end),
       [ZGB Art.]            = dbo.fnLOVTextListe('VmZGB',VVS.ZGBCodes),
       Anerkennung           = VVS.AnerkennungDatum,
       Ort                   = VVS.AnerkennungOrt,
       Unterhaltsvertrag     = VVS.UHVDatum,
       Betrag                = VVS.UHVBetrag,
       [Sorgerecht Vereinb.] = VVS.SorgerechtVereinbDatum,
       Vaterschaftsurteil    = VVS.VUrteilDatum,
       Unterhaltsurteil      = VVS.UnterhaltsurteilDatum,
       Betrag                = VVS.UnterhaltsurteilBetrag,
       Anfechtung            = VVS.VAnfechtungsurteil,
       [Schlussber. an EKSK] = VVS.SchlussberichtAnKommission,
       [Genehmigung EKSK]    = VVS.SchlussberichtGenehmigung,
       Abschlussgrund        = GRU.Text,
       [Zahlung Gebühr]      = VVS.GebuehrDatum,
       [Erlass der Gebühr]   = isNull(VVS.GebuehrErlass,convert(bit,0)),
       Mandatsträger         = USR.LastName + isNull(', ' + USR.FirstName,'')
from   dbo.BaPerson PRS WITH (READUNCOMMITTED)
       inner join dbo.FaLeistung      FAL WITH (READUNCOMMITTED) on FAL.BaPersonID = PRS.BaPersonID and
                                       FAL.ModulID = 5 and
                                       FAL.FaProzessCode = 502
       left  join dbo.VmVaterschaft VVS WITH (READUNCOMMITTED) on VVS.FaLeistungID = FAL.FaLeistungID
       left  join dbo.FaLeistungArchiv  ARC WITH (READUNCOMMITTED) on ARC.FaLeistungID = FAL.FaLeistungID and ARC.CheckOut is null
       inner join dbo.XUser         USR WITH (READUNCOMMITTED) on USR.UserID = FAL.UserID
       left  join dbo.XLOVCode      GRU WITH (READUNCOMMITTED) on GRU.LOVName = 'VmUHVAbschlussGrund' and
                                       GRU.Code = VVS.UHVAbschlussGrundCode
where 1 = 1
--- and USR.UserID =  {edtUserID.LookupID}
--- and PRS.BaPersonID = {edtBaPersonID.LookupID}
--- AND FAL.DatumVon &gt;= {edtEroeffnungsDatumVon}
--- AND FAL.DatumVon &lt;= {edtEroeffnungsDatumBis}
--- AND FAL.DatumBis &gt;= {edtAbschlussDatumVon}
--- AND FAL.DatumBis &lt;= {edtAbschlussDatumBis}
--- and case when FAL.DatumBis is null then 1 else case when ARC.FaLeistungID is null then 2 else 3 end end = {edtStatusCode}

order by Name,Vorname</value>
