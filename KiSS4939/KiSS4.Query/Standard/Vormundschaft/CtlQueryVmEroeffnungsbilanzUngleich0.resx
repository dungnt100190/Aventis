  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>select Mandant            = PRS.Name + isnull(', ' + PRS.Vorname,''),
       Eroeffnungssaldo   = sum(KTO.Eroeffnungssaldo),
       [Periode ID]       = KTO.fbperiodeid,
       Von                = max(PER.PeriodeVon),
       Bis                = max(PER.PeriodeBis),
       Status             = max(STA.Text),
       [Status Vorperiode]= max(STA2.Text),
       Team               = max(TEA.Name),
       [MT-Name]          = max(USR.LastName + isNull(', ' + USR.FirstName,'')),
       [MT-Kürzel]        = max(USR.LogonName),
       BaPersonID$        = PER.BaPersonID
from   dbo.FbKonto KTO WITH (READUNCOMMITTED)
       inner join dbo.FbPeriode PER WITH (READUNCOMMITTED) on PER.FbPeriodeID = KTO.FbPeriodeID
       inner join dbo.BaPerson PRS WITH (READUNCOMMITTED) on PRS.BaPersonID = PER.BaPersonID
       left  join dbo.FaLeistung    FAL WITH (READUNCOMMITTED) on FAL.BaPersonID = PER.BaPersonID and
                                   FAL.DatumVon = (select max(DatumVon) from dbo.FaLeistung WITH (READUNCOMMITTED)
                                                   where  BaPersonID = PER.BaPersonID and
                                                          ModulID = 5)
       left join dbo.XUser      USR WITH (READUNCOMMITTED)  on USR.UserID = FAL.UserID
       left join dbo.FbTeam     TEA WITH (READUNCOMMITTED)  on TEA.FbTeamID = PER.FbTeamID
       left join dbo.FbPeriode  PER2 WITH (READUNCOMMITTED) on PER2.BaPersonID = PER.BaPersonID and
                                    PER2.FbPeriodeID = (select top 1 FbPeriodeID
                                                        from   dbo.FbPeriode WITH (READUNCOMMITTED)
                                                        where  BaPersonID = PER.BaPersonID and
                                                               PeriodeVon &lt; PER.PeriodeVon
                                                        order by PeriodeVon desc)
       left join dbo.XLOVCode   STA WITH (READUNCOMMITTED)  on STA.LOVName = 'FbPeriodeStatus' and
                                    STA.Code = PER.PeriodeStatusCode

       left join dbo.XLOVCode   STA2 WITH (READUNCOMMITTED) on STA2.LOVName = 'FbPeriodeStatus' and
                                    STA2.Code = PER2.PeriodeStatusCode

--- where PER.FbTeamID = {edtFbTeamID}

group by KTO.fbperiodeid, PRS.Name, PRS.Vorname, PER.BaPersonID
having  abs(sum(Eroeffnungssaldo)) &gt; 0.099999
order by 1</value>
