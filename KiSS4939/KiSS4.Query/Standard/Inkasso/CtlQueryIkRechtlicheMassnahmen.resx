  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>-- grundselect IkRechtlicheMassnahmen:
SELECT
  [eröffnet am]      = ANZ.DatumVon,
  [Typ]              = dbo.fnLOVText('IkAnzeigeTyp', ANZ.IkAnzeigeTypCode),
  [abgeschlossen am] = ANZ.DatumBis,
  [Grund]            = dbo.fnLOVText('IkAnzeigeAbschlussGrund', ANZ.IkAnzeigeAbschlussGrundCode),
  [Schuldner]        = PRS.NameVorname,
  [Inkassofall]      = FAl.Bezeichnung,
  [SAR]              = USR.LastName + ', ' + USR.FirstName,
  BaPersonID$        = FAL.BaPersonID 
FROM  dbo.IkAnzeige                    ANZ WITH (READUNCOMMITTED)
  INNER JOIN dbo.XLOVCode              TYP WITH (READUNCOMMITTED)  ON ANZ.IkAnzeigeTypCode = TYP.Code and TYP.LOVName = 'IkAnzeigeTyp'
  INNER JOIN dbo.FaLeistung            FAL WITH (READUNCOMMITTED)  ON FAL.FaLeistungID = ANZ.FaLeistungID
  LEFT  JOIN dbo.FaLeistungArchiv      FFA WITH (READUNCOMMITTED)  ON FFA.FaLeistungID = FAL.FaLeistungID and FFA.CheckOut is NULL
  INNER JOIN dbo.vwPerson              PRS  ON PRS.BaPersonID = FAL.BaPersonID -- Schuldner
  INNER JOIN dbo.XUser                 USR WITH (READUNCOMMITTED)  ON USR.UserID = FAL.UserID
WHERE     FFA.FaLeistungID IS NULL -- nur NICHT archivierte
---   AND (     {edtVariante}=1 -- laufende rechtliche Massnahmen
---               AND (ANZ.DatumVon &lt;= {edtDatumBis} OR {edtDatumBis} IS NULL)
---               AND (ANZ.DatumBis &gt;= {edtDatumVon} OR ANZ.DatumBis IS NULL OR {edtDatumVon} IS NULL)
---         OR  {edtVariante}=2 -- neu eroeffnete rechtliche Massnahmen
---               AND (ANZ.DatumVon &gt;= {edtDatumVon} OR {edtDatumVon} IS NULL)
---               AND (ANZ.DatumVon &lt;= {edtDatumBis} OR {edtDatumBis} IS NULL)
---         OR  {edtVariante}=3 -- +
---               AND (ANZ.DatumBis &gt;= {edtDatumVon} OR {edtDatumVon} IS NULL)
---               AND (ANZ.DatumBis &lt;= {edtDatumBis} OR {edtDatumBis} IS NULL)
---       ) -- {edtVariante} closing bracket
---   AND ({edtVariante}!=3 OR ANZ.DatumBis IS NOT NULL) -- 3: nur abgeschlossene rechtliche Massnahmen
---   AND USR.UserID = {edtSAR_ID.LookupID}
ORDER BY ANZ.DatumVon</value>
