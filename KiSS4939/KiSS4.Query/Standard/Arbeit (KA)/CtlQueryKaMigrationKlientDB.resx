  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>;WITH cte (IsIntake, FaLeistungID, Datum, ModulCode, StatusDossierCode, DatumIntegration, IntegrBeurCode, GrundDossCode, KurserfassungID)
AS (
  SELECT 1,
         KAI.FaLeistungID,
         KAI.Datum,
         KAI.KaAbklaerungPhaseIntakeCode,
         KAI.KaAbklaerungStatusDossierCode,
         KAI.DatumIntegration,
         KAI.KaAbklaerungIntegrBeurCode,
         KAI.KaAbklaerungGrundDossCode,
         NULL
  FROM dbo.KaAbklaerungIntake KAI WITH (READUNCOMMITTED)
  
  UNION ALL
  
  SELECT 0,
         KAV.FaLeistungID,
         KAV.Datum,
         KAV.KaAbklaerungPhaseVertiefteAbklaerungenCode,
         KAV.KaAbklaerungStatusDossierCode,
         KAV.DatumIntegration,
         KAV.KaAbklaerungIntegrBeurCode,
         KAV.KaAbklaerungGrundDossCode,
         KAV.KaKurserfassungID
  FROM dbo.KaAbklaerungVertieft KAV WITH (READUNCOMMITTED)
)

SELECT BaPersonID$             = PRS.BaPersonID,
       Name                    = PRS.Name, 
       Vorname                 = PRS.Vorname,
       Geburtsdatum            = PRS.Geburtsdatum,
       [Datum Erfassung]       = AKZ.Erfassung,
       Anmeldung               = dbo.fnLOVText('KaAbklärungZuwAnmeld', AKZ.AnmeldungCode),
       Institution             = CASE WHEN AKZ.InstitutionID &lt; 0 THEN ORG1.ItemName ELSE ORG.Name END,
       Berater                 = CASE WHEN AKZ.BeraterID &lt; 0 THEN XUR.LastName + ISNULL(', ' + XUR.FirstName,'') ELSE OKO.Name + ISNULL(', ' + OKO.Vorname,'') END,
       Datum                   = cte.Datum,
       Phase                   = CASE WHEN cte.IsIntake = 1 THEN dbo.fnLOVText('KaAbklaerungPhaseIntake', cte.ModulCode) ELSE dbo.fnLOVText('KaAbklaerungPhaseVertiefteAbklaerungen', cte.ModulCode) END,
       [Status Warteliste]     = dbo.fnLOVText('KaAbklaerungStatusDossier', cte.StatusDossierCode),
       Kurs                    = KUE.KursNr + ISNULL(', ' + KUR.Name, ''),
       [Datum / Integr.beurt.] = CONVERT(VARCHAR, cte.DatumIntegration, 104) + ISNULL(': ' + dbo.fnLOVText('KaAbklaerungIntegrBeur', cte.IntegrBeurCode), ''),
       [Grund Dossierrückgabe] = dbo.fnLOVText('KaAbklärungGrundDoss', cte.GrundDossCode)
FROM cte
  LEFT JOIN dbo.BaPerson             PRS WITH (READUNCOMMITTED) ON PRS.BaPersonID = (SELECT FAL.BaPersonID 
                                                                                     FROM dbo.FaLeistung FAL WITH (READUNCOMMITTED)
                                                                                     WHERE FAL.FaLeistungID = cte.FaLeistungID)
  LEFT JOIN dbo.KaKurserfassung      KUE WITH (READUNCOMMITTED) ON KUE.KaKurserfassungID = cte.KurserfassungID
  LEFT JOIN dbo.KaKurs               KUR WITH (READUNCOMMITTED) ON KUR.KaKursID = KUE.KursID	
  LEFT JOIN dbo.KaAKZuweiser         AKZ WITH (READUNCOMMITTED) ON AKZ.FaLeistungID = cte.FaLeistungID
  LEFT JOIN dbo.BaInstitution        ORG WITH (READUNCOMMITTED) ON ORG.BaInstitutionID = AKZ.InstitutionID
  LEFT JOIN dbo.BaInstitutionKontakt OKO WITH (READUNCOMMITTED) ON OKO.BaInstitutionKontaktID = AKZ.BeraterID
  LEFT JOIN dbo.XUser                XUR WITH (READUNCOMMITTED) ON XUR.UserID = -AKZ.BeraterID
  LEFT JOIN dbo.XOrgUnit            ORG1 WITH (READUNCOMMITTED) ON ORG1.OrgUnitID = -AKZ.InstitutionID
ORDER BY Name ASC, Vorname ASC, Datum DESC</value>
