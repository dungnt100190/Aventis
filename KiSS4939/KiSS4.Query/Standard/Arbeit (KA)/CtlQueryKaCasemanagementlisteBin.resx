  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>DECLARE @SearchDate DATETIME;
--- SET @SearchDate = {edtSucheDatum};

IF (@SearchDate IS NULL)
BEGIN
  SET @SearchDate = GETDATE(); --SearchDate auf aktuelles Datum initialisieren, falls nicht bereits von Suchmaske initialisiert
END;

SELECT
  BaPersonID$         = PRS.BaPersonID,
  Anrede              = PRS.Titel,
  Name                = PRS.Name,
  Vorname             = PRS.Vorname,
  [Anweisung von]     = EIN.DatumVon,
  [Anweisung bis]     = ISNULL(AUS.AustrittDatum, EIN.DatumBis),
  BG                  = EIN.BeschGrad,
  Coach               = (SELECT TOP 1 USR.NameVorname
                         FROM dbo.KaZuteilFachbereich ZFB WITH (READUNCOMMITTED)
                           INNER JOIN dbo.vwUser      USR WITH (READUNCOMMITTED) ON USR.UserID = ZFB.FachleitungID
                         WHERE ZFB.BaPersonID = LEI.BaPersonID
                         ORDER BY ZFB.ZuteilungVon DESC),
  [Anrede PB]         = IKO.Anrede,
  [Vorname PB]        = IKO.Vorname,
  [Name PB]           = IKO.Name,
  [Telefonnummer PB]  = IKO.Telefon,
  [Mail PB]           = IKO.EMail,
  RAV                 = INS.Name
FROM dbo.FaLeistung                   LEI WITH (READUNCOMMITTED)
  INNER JOIN dbo.vwPerson             PRS WITH (READUNCOMMITTED) ON PRS.BaPersonID = LEI.BaPersonID
  INNER JOIN dbo.KaEinsatz            EIN WITH (READUNCOMMITTED) ON EIN.FaLeistungID = LEI.FaLeistungID
  LEFT  JOIN dbo.KaQEJobtimum         JOB WITH (READUNCOMMITTED) ON JOB.FaLeistungID = LEI.FaLeistungID
  LEFT  JOIN dbo.BaInstitutionKontakt IKO WITH (READUNCOMMITTED) ON IKO.BaInstitutionKontaktID = EIN.ZuweiserID
  LEFT  JOIN dbo.BaInstitution        INS WITH (READUNCOMMITTED) ON INS.BaInstitutionID = IKO.BaInstitutionID
  --
  OUTER APPLY dbo.fnKaGetAustrittDatumCode(LEI.FaLeistungID, EIN.KaEinsatzID) AUS
WHERE LEI.FaProzessCode = 704   -- 704: QE (Jobtimum + EPQ)
  AND LEI.KaEpqJob = 0          -- 0: Jobtimum
  AND LEI.DatumBis IS NULL
  AND @SearchDate BETWEEN EIN.DatumVon AND COALESCE(AUS.AustrittDatum, EIN.DatumBis, '99991231');</value>
