  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>SELECT 	BaPersonID$ = PRS.BaPersonID,
	Name = PRS.Name,
	Vorname = PRS.Vorname,
	Fachbereich = dbo.fnLOVText('KAFachbereich', ZFE.FachbereichID),
	Stagebetrieb = BET.FirmaName,
	Ansprechperson = BET.KontaktPerson,
	Adresse = ADR.Strasse + IsNull(' ' + ADR.HausNr, ''),
	PLZ = ADR.PLZ,
	Ort = ADR.Ort,
	Telefonnummer = BET.Telefon,
	Email = BET.Email,
	[Einsatz von] = EXT.EinsatzVon,
	[Einsatz bis] = EXT.EinsatzBis,
	[Einsatz als] = dbo.fnLOVText('KaQjExternEinsatzals', EXT.EinsatzCode)
FROM KaQJExtern EXT
   LEFT JOIN FaLeistung FAL ON FAL.FaLeistungID = EXT.FaLeistungID
   LEFT JOIN BaPerson PRS ON PRS.BaPersonID = FAL.BaPersonID
   LEFT JOIN KaBetrieb	BET on BET.KaBetriebID = EXT.BetriebID
   LEFT JOIN BaAdresse	ADR on ADR.KaBetriebID = BET.KaBetriebID
   LEFT JOIN KaZuteilFachbereich ZFE ON ZFE.BaPersonID = PRS.BaPersonID
                                         AND ZFE.KaZuteilFachbereichID = (SELECT TOP 1 KaZuteilFachbereichID
                                                                          FROM KaZuteilFachbereich
                                                                          WHERE BaPersonID = PRS.BaPersonID
                                                                          AND ZuteilungVon &lt;= EXT.EinsatzVon
                                                                          AND (ZuteilungBis is null or ZuteilungBis &gt;= EXT.EinsatzVon)
									  ORDER BY ZuteilungVon desc)
WHERE 1 = 1
AND (ZFE.FachbereichID IN (SELECT Code FROM XLOVCode WHERE LOVName like 'KAFachbereich' AND Text like 'Semo Ext%') OR ZFE.FachbereichID IS NULL)
---AND EXT.EinsatzCode = {edtEinsatzCode}
---AND PRS.BaPersonID = {edtBaPersonID.LookupID}
---AND ZFE.FachbereichID = {edtFachbereichID.LookupID}</value>
  </data>
