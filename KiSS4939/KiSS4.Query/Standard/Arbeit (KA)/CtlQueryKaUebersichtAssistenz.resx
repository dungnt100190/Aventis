  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>SELECT DISTINCT 
  BaPersonID$         = PRS.BaPersonID,
  [Anrede]            = PRS.Titel,
  [Name]              = PRS.Name,
  [Vorname]           = PRS.Vorname,
  [Eingang Zuweisung] = (SELECT TOP 1 isNull(DatumVon, '') 
                         FROM dbo.KaEinsatz WITH (READUNCOMMITTED)
                         WHERE FaLeistungID = FAL.FaLeistungID
                           AND AnweisungCode = 1
                         ORDER BY DatumVon DESC),
  [Eintritt Programm] = (SELECT TOP 1 isNull(DatumVon, '') 
                         FROM dbo.KaEinsatz WITH (READUNCOMMITTED) 
                         WHERE FaLeistungID = FAL.FaLeistungID
                           AND AnweisungCode = 2
                         ORDER BY DatumVon DESC),
  [Verlängerung]      = (SELECT TOP 1 isNull(DatumVon, '') 
                         FROM dbo.KaEinsatz WITH (READUNCOMMITTED) 
                         WHERE FaLeistungID = FAL.FaLeistungID
                           AND AnweisungCode = 3
                         ORDER BY DatumVon DESC),
  [Austritt Programm] = isNull(Convert(varchar, (SELECT MAX(DatumBis) 
                                                 FROM dbo.KaEinsatz WITH (READUNCOMMITTED)
                                                 WHERE FaLeistungID = FAL.FaLeistungID), 104), ''),
  [St%]               = KAE.BeschGrad,
  [Einsatzplatz]      = ASS.Einsatzplatz,
  [Stufe]             = ASS.Stufe,
  [Personalien]       = ASS.Personalien,
  [Vereinbarung]      = ASS.Vereinbarung,
  [Zielvereinbarung]  = ASS.Zielvereinbarung,
  [Schlussbericht]    = ASS.Schlussbericht,
  [Testat]            = ASS.Testat,
  [Bemerkung]         = ASS.Bemerkungen,
  [RAV]               = CASE WHEN KAE.ZuweiserID &lt; 0 THEN isNull(XOU.ItemName, '')
                             ELSE isNull(ORG1.Name, '')
                        END,
  [RAV-Berater]       = CASE WHEN KAE.ZuweiserID &lt; 0 THEN XUR1.LastName + isNull(', ' + XUR1.FirstName,'') 
                             ELSE OKO.Name + isNull(', ' + OKO.Vorname,'') 
                        END,
  [Telefon]           = CASE WHEN KAE.ZuweiserID &lt; 0 THEN isNull(XUR1.Phone, '')
                             ELSE isNull(OKO.Telefon, '')
                        END,
  [Adresse STES]      = PRS.WohnsitzStrasseHausNr,
  [PLZ/Ort STES]      = PRS.WohnsitzPLZOrt,
  [Mailadresse STES]  = PRS.Email,
  [Telefon STES]      = PRS.Telefon_P,
  [AHV-Nr.]           = PRS.AHVNummer,
  [Vers.-Nr.]         = PRS.Versichertennummer,
  [Geburtsdatum STES] = PRS.Geburtsdatum
FROM dbo.FaLeistung FAL WITH (READUNCOMMITTED)
  INNER JOIN dbo.KaAssistenz ASS WITH (READUNCOMMITTED) ON ASS.FaLeistungID = FAL.FaLeistungID
  INNER JOIN dbo.KaEinsatz KAE WITH (READUNCOMMITTED) ON KAE.FaLeistungID = FAL.FaLeistungID
  INNER JOIN dbo.vwPerson PRS ON PRS.BaPersonID = FAL.BaPersonID   
  LEFT  JOIN dbo.XUser XUR1 WITH (READUNCOMMITTED)	ON XUR1.UserID = -KAE.ZuweiserID		
  LEFT  JOIN dbo.BaInstitutionKontakt OKO WITH (READUNCOMMITTED) ON OKO.BaInstitutionKontaktID = KAE.ZuweiserID
  LEFT  JOIN dbo.BaInstitution ORG1 WITH (READUNCOMMITTED) ON ORG1.BaInstitutionID =  OKO.BaInstitutionID	
  LEFT  JOIN dbo.XOrgUnit_User OUU WITH (READUNCOMMITTED) ON OUU.UserID = -KAE.ZuweiserID
				AND (OUU.OrgUnitMemberCode = 1 OR OUU.OrgUnitMemberCode = 2)
  LEFT JOIN dbo.XOrgUnit XOU WITH (READUNCOMMITTED) ON XOU.OrgUnitID = OUU.OrgUnitID
WHERE FAL.ModulID = 7
  AND FAL.FaProzessCode = 707
  AND FAL.DatumBis is NULL
ORDER BY PRS.Name, PRS.Vorname
</value>
