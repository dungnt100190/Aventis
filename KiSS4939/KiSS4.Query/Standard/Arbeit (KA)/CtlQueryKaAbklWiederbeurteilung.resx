  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>DECLARE @tmp TABLE 
(
  ID              INT IDENTITY,
  Datum           DATETIME,
  IsIntake        BIT,
  IntegrationCode INT,
  StatusCode      INT
  PRIMARY KEY (ID)
);

;WITH AbklaerungCte (FaLeistungID, IsIntake, Datum, KaAbklaerungIntegrBeurCode, KaAbklaerungStatusDossierCode)
AS
(
  SELECT KAI.FaLeistungID,
         1,
         KAI.Datum,
         KAI.KaAbklaerungIntegrBeurCode,
         KAI.KaAbklaerungStatusDossierCode
  FROM dbo.KaAbklaerungIntake KAI WITH (READUNCOMMITTED)
  
  UNION ALL
  
  SELECT KAV.FaLeistungID,
         0,
         KAV.Datum,
         KAV.KaAbklaerungIntegrBeurCode,
         KAV.KaAbklaerungStatusDossierCode
  FROM dbo.KaAbklaerungVertieft KAV WITH (READUNCOMMITTED)
)

INSERT INTO @tmp
  SELECT CTE.Datum,
         CTE.IsIntake,
         CTE.KaAbklaerungIntegrBeurCode,
         CTE.KaAbklaerungStatusDossierCode 
  FROM AbklaerungCte CTE
    LEFT JOIN dbo.KaAKZuweiser AKZ WITH (READUNCOMMITTED) ON AKZ.FaLeistungID = CTE.FaLeistungID
  WHERE 1 = 1
  --- AND CTE.Datum &gt;= {edtDatumVon}
  --- AND CTE.Datum &lt;= {edtDatumBis}
  --- AND AKZ.BeraterID = {edtZuweiserID.LookupID}


-- Output
SELECT [Text] = 'davon vertiefte Abklärung', Anzahl = COUNT(1)
FROM @tmp
WHERE IsIntake = 0

UNION ALL 

SELECT [Text] = 'davon Vermittlung: ' + dbo.fnGetLOVMLText('KaAbklaerungIntegrBeur', 1, 1), Anzahl = COUNT(1)
FROM @tmp
WHERE IntegrationCode = 1 -- berufliche Integration BI/BIP

UNION ALL 

SELECT [Text] = 'davon Vermittlung: ' + dbo.fnGetLOVMLText('KaAbklaerungIntegrBeur', 2, 1), Anzahl = COUNT(1)
FROM @tmp
WHERE IntegrationCode = 2 -- soziale Integration SI

UNION ALL 

SELECT [Text] = 'davon ' + dbo.fnGetLOVMLText('KaAbklaerungIntegrBeur', 3, 1), Anzahl = COUNT(1)
FROM @tmp
WHERE IntegrationCode = 3 -- Qualifizierung Jugend

UNION ALL 

SELECT [Text] = 'davon ' + dbo.fnGetLOVMLText('KaAbklaerungIntegrBeur', 8, 1), Anzahl = COUNT(1)
FROM @tmp
WHERE IntegrationCode = 8 -- retour an ZuweiserIn mit Empfehlung

UNION ALL 

SELECT [Text] = 'davon ' + dbo.fnGetLOVMLText('KaAbklaerungIntegrBeur', 10, 1), Anzahl = COUNT(1)
FROM @tmp
WHERE IntegrationCode = 10 -- PvB EAF

UNION ALL 

SELECT [Text] = 'davon pendent (Rückfragen, Unklarheiten)', Anzahl = COUNT(1)
FROM @tmp
WHERE StatusCode = 5 -- Dossier pendent</value>
