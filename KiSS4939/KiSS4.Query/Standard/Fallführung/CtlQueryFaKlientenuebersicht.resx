  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>SELECT DISTINCT * FROM
(SELECT SAR            = BEN.LastName + ISNULL(', ' + BEN.FirstName,''),
       FT              = CONVERT(BIT, CASE WHEN EXISTS(SELECT 1 FROM FaLeistung WHERE BaPersonID = PRS.BaPersonID)
	                           THEN 1
                              ELSE 0
                         END),
       Person          = PRS.NameVorname,
       Strasse         = PRS.WohnsitzStrasseHausNr,
       PLZ             = PRS.WohnsitzPLZ,
       Ort             = PRS.WohnsitzOrt,
       [Aufenthaltsort] = PRS.Aufenthalt,
       [Alter]         = PRS.[Alter],
       Geburtsdatum    = PRS.Geburtsdatum,
       Geschlecht      = CASE PRS.GeschlechtCode
                         WHEN 1 THEN 'M'
                         WHEN 2 THEN 'F'
                         ELSE '?'
                         END,
       Nationalit√§t    = PRS.Nationalitaet,
       UserID$ = BEN.UserID,
       BaPersonID$ = PRS.BaPersonID
FROM  vwPerson PRS WITH (READUNCOMMITTED)
      LEFT JOIN dbo.FaLeistung    LEI WITH (READUNCOMMITTED) ON LEI.BaPersonID = PRS.BaPersonID
      LEFT JOIN dbo.XUser         BEN  WITH (READUNCOMMITTED)         ON BEN.UserID = LEI.UserID
WHERE 1 = 1
--- AND BEN.UserID =  {edtUserID.LookupID}
--- AND PRS.Name + isNull(', ' + PRS.Vorname,'') like {edtKlient} + '%'
--- AND LEI.ModulID = {edtModulID}
--- AND ({edtNurAktive} = 0 or LEI.DatumBis is null)
AND (LEI.ModulID &lt;&gt; 2 OR NOT EXISTS (SELECT * FROM dbo.FaLeistung LEI2 WITH (READUNCOMMITTED) WHERE LEI2.BaPersonID = LEI.BaPersonID AND LEI2.DatumBis IS NULL AND LEI2.ModulID &lt;&gt; 2))
) V
WHERE 1 = 1</value>
