  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>DECLARE @LanguageCode INT;
SET @LanguageCode = {1};

select SAR                = BEN.LastName + isNull(', ' + BEN.FirstName,''),
       Person             = PRS.Name + isNull(', ' + PRS.Vorname,''),
       [Eröffnet]         = FAL.DatumVon,
       Abgeschlossen      = FAL.DatumBis,
       Strasse            = PRS.WohnsitzStrasse,
       StrasseNr          = PRS.WohnsitzHausNr,
       PLZ                = PRS.WohnsitzPLZ,
       Ort                = PRS.WohnsitzOrt,
       Geburtsdatum       = PRS.Geburtsdatum,
       [Alter]            = dbo.fngetage(PRS.Geburtsdatum, GETDATE()),
       Altertyp           = case 
                              when dbo.fngetage(PRS.Geburtsdatum, GETDATE()) between 0 and 17 then '0-17'
                              when dbo.fngetage(PRS.Geburtsdatum, GETDATE()) between 18 and 25 then '18-25'
                              when dbo.fngetage(PRS.Geburtsdatum, GETDATE()) between 26 and 35 then '26-35'
                              when dbo.fngetage(PRS.Geburtsdatum, GETDATE()) between 36 and 50 then '36-50'
                              when dbo.fngetage(PRS.Geburtsdatum, GETDATE()) between 51 and 65 then '51-65'
                              else dbo.fnGetMLTextFromName('CtlQueryFaFalltraegerÜbersicht', 'Ab66', @LanguageCode) 
                            end,
       Geschlecht         = dbo.fnLOVMLText('Geschlecht',PRS.GeschlechtCode, @LanguageCode),
       Zivilstand         = dbo.fnLOVMLText('Zivilstand', PRS.ZivilstandCode, @LanguageCode),
       [Nationalität]     = CASE @LanguageCode
                              WHEN 1 THEN PRS.Nationalitaet
                              WHEN 2 THEN PRS.NationalitaetFR
                              WHEN 3 THEN PRS.NationalitaetIT
                              ELSE PRS.Nationalitaet
                            END,
       Aufenthaltstatus   = dbo.fnLOVMLText('Aufenthaltsstatus', PRS.AuslaenderStatusCode, @LanguageCode),
       Konfession         = dbo.fnLOVMLText('Konfession', PRS.KonfessionCode, @LanguageCode),
       [erlernter Beruf]  = dbo.fnLOVMLText('Beruf', ARB.ErlernterBerufCode, @LanguageCode),
       Branche            = dbo.fnLOVMLText('Branche',ARB.BrancheCode, @LanguageCode),
       Wohnstatus         = dbo.fnLOVMLText('Wohnstatus', ADR.WohnStatusCode, @LanguageCode),
       Wohnungsgrösse     = dbo.fnLOVMLText('Wohnungsgroesse', ADR.WohnungsgroesseCode, @LanguageCode),
       Erwerbssituation   = dbo.fnLOVMLText('Erwerbssituation', ARB.ErwerbssituationStatus1Code, @LanguageCode),
       [Letzte Tätigkeit] = dbo.fnLOVMLText('Beruf', ARB.BerufCode, @LanguageCode),
       UserID$            = BEN.UserID,
       BaPersonID$        = PRS.BaPersonID
       
from   XUser BEN
       inner join FaLeistung         FAL on FAL.UserID = BEN.UserID-- AND FAL.ModulCode = 3
       inner join vwPerson           PRS on PRS.BaPersonID = FAL.BaPersonID

       left  join BaArbeitAusbildung ARB on ARB.BaPersonID = PRS.BaPersonID
       left  join XOrgUnit_User      OUU on OUU.UserID = FAL.UserID and
                                            OUU.OrgUnitMemberCode = 2
       left  join XOrgUnit           ORG on ORG.OrgUnitID = OUU.OrgUnitID
       LEFT  JOIN BaAdresse          ADR ON ADR.BaPersonID = PRS.BaPersonID
where 1 = 1 
  AND (PRS.PersonSichtbarSDFlag = dbo.fnGetPersonSichtbarFlag({0}) or PRS.PersonSichtbarSDFlag = 1) 
--- AND BEN.UserID =  {edtUserID.LookupID} 
--- AND (ORG.OrgUnitID = {edtOrgUnitID} OR ORG.ParentID = {edtOrgUnitID})
--- AND FAL.DatumVon &gt;= {edtFallAufnahmeVon}
--- AND FAL.DatumVon &lt;= {edtFallAufnahmeBis}
--- AND FAL.DatumBis &gt;= {edtFallAbschlussVon}
--- AND FAL.DatumBis &lt;= {edtFallAbschlussBis}
--- AND (FAL.DatumBis IS NULL OR FAL.DatumBis &gt;= {edtFallAktivVon})
--- AND FAL.DatumVon &lt;= {edtFallAktivBis}
--- AND (       {edtAlterCode}=1 AND (dbo.fngetage(PRS.Geburtsdatum, GETDATE()) &gt;= 0  and  dbo.fngetage(PRS.Geburtsdatum, GETDATE()) &lt;=17)
---         OR  {edtAlterCode}=2 AND (dbo.fngetage(PRS.Geburtsdatum, GETDATE()) &gt;= 18  and  dbo.fngetage(PRS.Geburtsdatum, GETDATE()) &lt;=25)
---         OR  {edtAlterCode}=3 AND (dbo.fngetage(PRS.Geburtsdatum, GETDATE()) &gt;= 26  and  dbo.fngetage(PRS.Geburtsdatum, GETDATE()) &lt;=35)
---         OR  {edtAlterCode}=4 AND (dbo.fngetage(PRS.Geburtsdatum, GETDATE()) &gt;= 36  and  dbo.fngetage(PRS.Geburtsdatum, GETDATE()) &lt;=50)
---         OR  {edtAlterCode}=5 AND (dbo.fngetage(PRS.Geburtsdatum, GETDATE()) &gt;= 51  and  dbo.fngetage(PRS.Geburtsdatum, GETDATE()) &lt;=65)
---         OR  {edtAlterCode}=6 AND (dbo.fngetage(PRS.Geburtsdatum, GETDATE()) &gt;= 65)
---     ) -- {edtAlterCode} closing bracket

--- AND PRS.GeschlechtCode          = {edtGeschlechtCode}
--- AND PRS.ZivilstandCode          = {edtZivilstandCode}
--- AND PRS.NationalitaetCode       = {edtNationalitaetCode}
--- AND PRS.AuslaenderStatusCode    = {edtAufenthaltstatusCode}
--- AND PRS.KonfessionCode          = {edtKonfessionCode}
--- AND ARB.ErlernterBerufCode      = {edtBerufCode}
--- AND ARB.BrancheCode             = {edtBrancheCode}
--- AND ADR.WohnStatusCode          = {edtWohnstatus}
--- AND ADR.WohnungsgroesseCode     = {edtWohnungsgroesse}
order by SAR, Person
</value>
