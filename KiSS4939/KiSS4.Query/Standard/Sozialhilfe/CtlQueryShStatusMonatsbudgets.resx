  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>SELECT BaPersonID$  = FAL.BaPersonID,
       Sektion      = ORG.ItemName,
       SAR          = dbo.fnGetLastFirstName(NULL, USR.LastName, USR.FirstName),
       Person       = PRS.NameVorname,
       Monatsbudget = dbo.fnXKurzMonat(BDG.Monat) + ' ' + CONVERT(VARCHAR, BDG.Jahr),
       Status       = CASE Bdg.BgBewilligungStatusCode
                        WHEN 1 THEN 'in Vorbereitung (grau)'
                        WHEN 5 THEN 'ausgelöst (grün)'
                        WHEN 9 THEN 'gesperrt (rot)'
                      END
FROM dbo.BgBudget               BDG WITH (READUNCOMMITTED)
  INNER JOIN dbo.BgFinanzplan   SFP WITH (READUNCOMMITTED) ON SFP.BgFinanzPlanID = BDG.BgFinanzPlanID
  INNER JOIN dbo.FaLeistung     FAL WITH (READUNCOMMITTED) ON FAL.FaLeistungID = SFP.FaLeistungID
                                                          AND FAL.ModulID = 3 -- only SH
  INNER JOIN dbo.vwPersonSimple PRS WITH (READUNCOMMITTED) ON PRS.BaPersonID = FAL.BaPersonID
  INNER JOIN dbo.XUser          USR WITH (READUNCOMMITTED) ON USR.UserID = FAL.UserID
  LEFT  JOIN dbo.XOrgUnit_User  OUU WITH (READUNCOMMITTED) ON OUU.UserID = FAL.UserID
                                                          AND OUU.OrgUnitMemberCode = 2
  LEFT  JOIN dbo.XOrgUnit       ORG WITH (READUNCOMMITTED) ON ORG.OrgUnitID = OUU.OrgUnitID
WHERE BDG.Masterbudget = 0
--- AND (OUU.OrgUnitID = {edtOrgUnitID} OR ORG.ParentID = {edtOrgUnitID})
--- AND FAL.UserID = {edtUserID.LookupID} 
--- AND FAL.BaPersonID = {edtBaPersonID.LookupID}
--- AND (BDG.Jahr &gt; YEAR({edtDatumVon}) OR (BDG.Jahr = YEAR({edtDatumVon}) AND BDG.Monat &gt;= MONTH({edtDatumVon})))
--- AND (BDG.Jahr &lt; YEAR({edtDatumBis}) OR (BDG.Jahr = YEAR({edtDatumBis}) AND BDG.Monat &lt;= MONTH({edtDatumBis})))
--- AND BDG.BgBewilligungStatusCode = {edtStatus}
ORDER BY Sektion, SAR, Person;</value>
