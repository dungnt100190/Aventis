  <data name="edtUserID.LookupSQL" xml:space="preserve">
    <value>SELECT ID      = UserID, 
       SAR     = LastName + ISNULL(', ' + FirstName,''), 
       Kuerzel = LogonName
FROM dbo.vwUser USR WITH (READUNCOMMITTED)
WHERE USR.NameVorname LIKE '%' + {0} + '%' 
  OR  USR.LogonName LIKE '%' + {0} + '%' 
  OR  USR.ShortName LIKE '%' + {0} + '%'
--WHERE LastName + ISNULL(', ' + FirstName, '') LIKE ISNULL({0}, '') + '%' 
ORDER BY SAR;</value>
