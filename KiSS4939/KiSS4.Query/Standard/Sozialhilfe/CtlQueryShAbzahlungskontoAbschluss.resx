  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>DECLARE @LanguageCode INT;
SET @LanguageCode = {0};

SELECT 
  Person                 = PRS.NameVorname,
  [Vers.-Nr.]            = PRS.Versichertennummer,
  Sektion                = ORG.ItemName,
  SAR                    = USR.NameVorname,
  Abschlussgrund         = dbo.fnLOVMLText('AbzahlungskontoAbschlussgrund', SPK.AbschlussgrundCode, @LanguageCode),
  [Begründung Abschluss] = SPK.AbschlussBegruendung,
  Saldo                  = dbo.fnBgSpezkonto(SPK.BgSpezkontoID,3,default,default),
  [Rückerstattung]       = dbo.fnLOVMLText('AbzahlungskontoRueckerstattung', SPK.AbzahlungskontoRueckerstattungCode, @LanguageCode),
  Bezeichnung            = SPK.NameSpezkonto,
  [Abzahlungskonto mit Einzelzahlung] = dbo.fnLOVMLText('JaNein', SPK.OhneEinzelzahlung, @LanguageCode),
  
  BaPersonID$            = LEI.BaPersonID,
  Name$                  = PRS.Name,
  Vorname$               = PRS.Vorname,
  DatumVon$              = LEI.DatumVon,
  DatumBis$              = LEI.DatumBis,
  BgSpezkontoID$         = SPK.BgSpezkontoID,
  FaLeistungID$          = LEI.FaLeistungID,
  FaLeistungStatus$      = dbo.fnLOVMLText('FaLeistungStatus', 
                                           CASE
                                             WHEN LEA.FaLeistungID IS NOT NULL THEN 3
                                             WHEN LEI.DatumBis IS NOT NULL THEN 2
                                             WHEN LEI.DatumBis IS NULL THEN 1
                                           END, 
                                           @LanguageCode)
FROM dbo.FaLeistung                   LEI WITH (READUNCOMMITTED)
  LEFT  JOIN dbo.FaLeistungArchiv     LEA WITH (READUNCOMMITTED) ON LEA.FaLeistungID = LEI.FaLeistungID
  INNER JOIN dbo.vwPerson             PRS WITH (READUNCOMMITTED) ON PRS.BaPersonID = LEI.BaPersonID
  INNER JOIN dbo.vwUser               USR WITH (READUNCOMMITTED) ON USR.UserID = LEI.UserID
  LEFT  JOIN dbo.XOrgUnit_User        OUU WITH (READUNCOMMITTED) ON OUU.UserID = USR.UserID 
                                                                AND OUU.OrgUnitMemberCode = 2
  LEFT  JOIN dbo.XOrgUnit             ORG WITH (READUNCOMMITTED) ON ORG.OrgUnitID = OUU.OrgUnitID
  INNER JOIN dbo.BgSpezkonto          SPK WITH (READUNCOMMITTED) ON SPK.FaLeistungID = LEI.FaLeistungID 
                                                                AND SPK.BgSpezkontoTypCode = 3 -- Abzahlungskonto
  LEFT  JOIN dbo.BgSpezkontoAbschluss SKA WITH (READUNCOMMITTED) ON SKA.BgSpezkontoID = SPK.BgSpezkontoID
WHERE LEI.ModulID = 3 -- Sozialhilfe
---  AND (OUU.OrgUnitID = {edtOrgUnitID} OR ORG.ParentID = {edtOrgUnitID})
---  AND USR.UserID = {edtUserID.LookupID}
---  AND LEI.DatumVon &gt;= {edtDatumVon}
---  AND LEI.DatumVon &lt;= {edtDatumBis}
---  AND SPK.AbzahlungskontoRueckerstattungCode = {edtRueckerstattung}
---  AND ({edtStatus} &lt;&gt; 1 OR LEI.DatumBis IS NULL)         -- aktiv
---  AND ({edtStatus} &lt;&gt; 2 OR LEI.DatumBis IS NOT NULL)     -- geschlossen
---  AND ({edtStatus} &lt;&gt; 3 OR LEA.FaLeistungID IS NOT NULL) -- archiviert
---  AND (({edtAbschlussgrund} = 0 AND SPK.AbschlussgrundCode IS NULL) -- Spezkonto ohne Abschlussgrund
---    OR SPK.AbschlussgrundCode = {edtAbschlussgrund})
---  AND ({edtEinzelzahlungen} &lt;&gt; 1 OR SPK.OhneEinzelzahlung = 0) -- mit Einzelzahlung
---  AND ({edtEinzelzahlungen} &lt;&gt; 2 OR SPK.OhneEinzelzahlung = 1) -- ohne Einzelzahlung
ORDER BY Name$, Vorname$, DatumVon$;
</value>
