  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>-- Zuerst werden alle unterschiedlichen Personenids gesucht und in die Tabelle PersID gespeichert.
-- Die eigentliche Abfrage durchsucht dann die Daten anhand dieser IDs
DECLARE @LanguageCode INT;
SET @LanguageCode = {1};

DECLARE @PersID TABLE 
(
  BaPersonID INT
);

INSERT @PersID
  SELECT DISTINCT PRS.BaPersonID
  FROM dbo.BaPerson          PRS WITH (READUNCOMMITTED)
    LEFT JOIN dbo.FaLeistung FAL WITH (READUNCOMMITTED) ON FAL.BaPersonID = PRS.BaPersonID
    LEFT JOIN dbo.XUser      BEN WITH (READUNCOMMITTED) ON BEN.UserID = FAL.UserID
  WHERE 1=1
    AND (PRS.PersonSichtbarSDFlag = dbo.fnGetPersonSichtbarFlag({0}) or PRS.PersonSichtbarSDFlag = 1) 
--- AND ({edtNurAktive} = 0 OR (FAL.FaLeistungID IS NOT NULL AND FAL.DatumBis IS NULL))
--- AND BEN.UserID =  {edtUserID.LookupID}

SELECT ID            = PRS.BaPersonID,
       Anrede        = PRS.Titel,
       Name          = PRS.Name,
       Vorname       = PRS.Vorname,
       Geschlecht    = dbo.fnLOVMLText('Geschlecht', PRS.GeschlechtCode, @LanguageCode),
       Strasse       = PRS.WohnsitzStrasseHausNr,
       PLZ           = PRS.WohnsitzPLZ,
       Ort           = PRS.WohnsitzOrt,
       Kanton        = PRS.WohnsitzKanton,
       Heimatort     = PRS.Heimatort,
       Nationalitaet = CASE {1}
                         WHEN 1 THEN PRS.Nationalitaet
                         WHEN 2 THEN PRS.NationalitaetFR
                         WHEN 3 THEN PRS.NationalitaetIT
                         ELSE PRS.Nationalitaet
                       END,
       [Auf. Institution] = PRS.AufenthaltInstitutionName,
       [Auf. Str.]   = PRS.AufenthaltStrasseHausNr,
       [Auf. PLZ]    = PRS.AufenthaltPLZ,
       [Auf. Ort]    = PRS.AufenthaltOrt,
       [Auf. Kanton] = PRS.AufenthaltKanton,
       BaPersonID$   = PRS.BaPersonID
FROM @PersID              PID
  INNER JOIN dbo.vwPerson PRS ON PRS.BaPersonID = PID.BaPersonID
WHERE 1 = 1
--- AND PRS.Name LIKE {edtName} + '%'
--- AND PRS.Vorname LIKE {edtVorname} + '%'
--- AND PRS.WohnsitzStrasseHausNr LIKE {edtStrasse} + '%'
--- AND PRS.WohnsitzPLZ LIKE {edtPLZ} + '%'
--- AND PRS.WohnsitzOrt LIKE {edtOrt} + '%'
--- AND PRS.WohnsitzKanton LIKE {edtKanton} + '%'
--- AND PRS.Heimatort LIKE {edtHeimatort} + '%'
--- AND PRS.NationalitaetCode = {edtNationalitaetCode}
--- AND PRS.GeschlechtCode = {edtGeschlechtCode}
--- AND PRS.AufenthaltStrasseHausNr LIKE {edtAufStrasse} + '%'
--- AND PRS.AufenthaltPLZ LIKE {edtAufPLZ} + '%'
--- AND PRS.AufenthaltOrt LIKE {edtAufOrt} + '%'
--- AND PRS.AufenthaltKanton LIKE {edtAufKanton} + '%'</value>
