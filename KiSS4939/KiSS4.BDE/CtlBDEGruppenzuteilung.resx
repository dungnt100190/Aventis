  <data name="qryVerfuegbar.SelectStatement" xml:space="preserve">
    <value>select UGR.BDEUserGroupID, UGR.UserGroupName
					from   BDEUserGroup UGR
						   left join XUser_BDEUserGroup UUG on UUG.BDEUserGroupID = UGR.BDEUserGroupID and
															   UUG.UserID = {0}
					where  UUG.BDEUserGroupID is null
					order by UserGroupName</value>
  </data>
  <data name="qryUser.SelectStatement" xml:space="preserve">
    <value>SELECT USR.*,
	   Password = '***',
	   RetypePassword = '***',
	   Abteilung = ORG.ItemName
FROM   XUser USR
	   LEFT JOIN XOrgUnit_User OUU ON OUU.UserID = USR.UserID AND
									  OUU.OrgUnitMemberCode = 2 AND
									  OUU.OrgUnitID = (SELECT MAX(OrgUnitID) 
													   FROM XOrgUnit_User
													   WHERE UserID = USR.UserID AND OrgUnitMemberCode = 2)
	   LEFT JOIN XOrgUnit      ORG ON ORG.OrgUnitID = OUU.OrgUnitID
WHERE 1=1
--- AND USR.LastName LIKE {edtSucheName} + '%'
--- AND USR.FirstName LIKE {edtSucheVorname} + '%'
--- AND USR.LogonName LIKE {edtSucheLogon} + '%'
--- AND USR.UserID = {edtSucheUserID}
--- AND ORG.ItemName LIKE {edtSucheAbteilung} + '%'
--- AND USR.IsUserAdmin = CASE WHEN {chkSucheNurAdmin} = 1 THEN 1 ELSE USR.IsUserAdmin END
ORDER BY LastName</value>
