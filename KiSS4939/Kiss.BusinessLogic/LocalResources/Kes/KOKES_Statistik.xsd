<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="http://www.kokes.ch/schemas/KOKES_Statistik_2013_Transfer_v1.4.xsd"
    elementFormDefault="qualified"
    xmlns="http://www.kokes.ch/schemas/KOKES_Statistik_2013_Transfer_v1.4.xsd"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
>

  <xs:element name="Kokes" type="ctKokes">
    <xs:annotation>
      <xs:documentation xml:lang="de">Element für Übermittlung der Statistikdaten eines Jahres.</xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:complexType name="ctKokes">
    <xs:annotation>
      <xs:documentation xml:lang="de">Container für Übermittlung der Statistikdaten eines Jahres.</xs:documentation>
    </xs:annotation>

    <xs:sequence>
      <xs:element name="Behoerde" type="ctBehoerde" minOccurs="1" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation xml:lang="de">Element für Statistikdaten einer einzelnen Behörde.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>

    <xs:attribute name="Erstellt" type="xs:date" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="de">Erstellungsdatum der XML Datei.</xs:documentation>
      </xs:annotation>

    </xs:attribute>

    <xs:attribute name="StatistikJahr" type="xs:int" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="de">Statistikjahr für das die Daten in der der XML Datei enthalten sind.</xs:documentation>
      </xs:annotation>

    </xs:attribute>

  </xs:complexType>

  <xs:complexType name="ctBehoerde">
    <xs:annotation>
      <xs:documentation xml:lang="de">Container für Statistikdaten einer einzelnen Behörde.</xs:documentation>
    </xs:annotation>

    <xs:sequence>
      <xs:element name="Person" type="ctPerson" minOccurs="1" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation xml:lang="de">Element für Statistikdaten einer einzelnen Person.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>

    <xs:attribute name="Erstellt" type="xs:date" use="optional">
      <xs:annotation>
        <xs:documentation xml:lang="de">Erstellungsdatum der Statistikdaten für diese KESB.</xs:documentation>
      </xs:annotation>
    </xs:attribute>

    <xs:attribute name="BehoerdeId" type="KESBID" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="de">Eindeutige Identifikation der Behörde (gemäss Liste der KESB).</xs:documentation>
      </xs:annotation>
    </xs:attribute>

    <xs:attribute name="Generator" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="de">Fallführungssystem mit dem das XML erzeugt wurde.</xs:documentation>
      </xs:annotation>
    </xs:attribute>

    <xs:attribute name="GeneratorVersion" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="de">Version des Fallführungssystems mit dem das XML erzeugt wurde.</xs:documentation>
      </xs:annotation>
    </xs:attribute>

  </xs:complexType>

  <xs:complexType name="ctPerson">
    <xs:annotation>
      <xs:documentation xml:lang="de">Container für Statistikdaten einer einzelnen Person.</xs:documentation>
    </xs:annotation>

    <xs:sequence minOccurs="1" maxOccurs="1">

      <xs:choice minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xml:lang="de">Entweder bestehende Massnahmen per 01.01.xxxx oder Übernahme von Massnahmen. Eine Kombination ist nicht möglich.</xs:documentation>
        </xs:annotation>

        <xs:sequence minOccurs="1" maxOccurs="1">
          <xs:element name="BestehendeKinder_01.01.xxxx" type="ctBestehendeKinder_01.01.xxxx" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation xml:lang="de">Element für bestehende Massnahmen nach Kindesschutzrecht per 01.01.xxxx.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BestehendeErwachsene_01.01.xxxx" type="ctBestehendeErwachsene_01.01.xxxx" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation xml:lang="de">Element für bestehende Massnahmen nach Erwachsenenschutzrecht per 01.01.xxxx.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>

        <xs:sequence minOccurs="1" maxOccurs="1">
          <xs:element name="UebernahmeKinder" type="ctUebernahmeKinder" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation xml:lang="de">Element für Übernahme einer Massnahme nach Kindesschutzrecht von einer anderen KESB innerhalb des Statistikjahres.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="UebernahmeErwachsene" type="ctUebernahmeErwachsene" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation xml:lang="de">Element für Übernahme einer Massnahme nach Erwachsenenschutzrecht von einer anderen KESB innerhalb des Statistikjahres.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:choice>

      <xs:element name="ErrichtungKinder" type="ctErrichtungKinder" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation xml:lang="de">
            Element für Errichtung einer Massnahme nach Kindesschutzrecht innerhalb des Statistikjahres.
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="ErrichtungErwachsene" type="ctErrichtungErwachsene" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation xml:lang="de">
            Element für Errichtung einer Massnahme nach Erwachsenenschutzrecht innerhalb des Statistikjahres.
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="AenderungKinder" type="ctAenderungKinder" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation xml:lang="de">
            Container für Änderung bei einer Massnahme nach Kindesschutzrecht innerhalb des Statistikjahres.
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="AenderungErwachsene" type="ctAenderungErwachsene" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation xml:lang="de">
            Element für Änderung bei einer Massnahme nach Erwachsenenschutzrecht innerhalb des Statistikjahres.
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="UmwandlungErwachsene" type="ctUmwandlungErwachsene" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation xml:lang="de">
            Element für Umwandlung einer altrechtlichen Massnahme in eine neurechtliche (Erwachsenenschutzrecht) innerhalb des Statistikjahres.
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="AufhebungKinder" type="ctAufhebungKinder" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation xml:lang="de">
            Element für Aufhebung einer Massnahme nach Kindesschutzrecht innerhalb des Statistikjahres.
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="AufhebungErwachsene" type="ctAufhebungErwachsene" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation xml:lang="de">
            Element für Aufhebung einer Massnahme nach Erwachsenenschutzrecht innerhalb des Statistikjahres.
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:choice minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xml:lang="de">Entweder bestehende Massnahmen per 31.12.xxxx oder Übertragung von Massnahmen. Eine Kombination ist nicht möglich.</xs:documentation>
        </xs:annotation>

        <xs:sequence minOccurs="1" maxOccurs="1">
          <xs:element name="BestehendeKinder_31.12.xxxx" type="ctBestehendeKinder_31.12.xxxx" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation xml:lang="de">
                Element für bestehende Massnahmen nach Kindesschutzrecht per 31.12.xxxx.
              </xs:documentation>
            </xs:annotation>
          </xs:element>

          <xs:element name="BestehendeErwachsene_31.12.xxxx" type="ctBestehendeErwachsene_31.12.xxxx" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation xml:lang="de">
                Element für bestehende Massnahmen nach Erwachsenenschutzrecht per 31.12.xxxx.
              </xs:documentation>
            </xs:annotation>
          </xs:element>

          <xs:element name="BestehendeErwachseneAlt_31.12.xxxx" type="ctBestehendeErwachseneAlt_31.12.xxxx" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation xml:lang="de">
                Element für bestehende Massnahmen nach altrechtlichem Erwachsenenschutzrecht per 31.12.xxxx.
              </xs:documentation>
            </xs:annotation>
          </xs:element>

        </xs:sequence>

        <xs:sequence minOccurs="1" maxOccurs="1">

          <xs:element name="UebertragungKinder" type="ctUebertragungKinder" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation xml:lang="de">
                Element für Übertragung einer Massnahme nach Kindesschutzrecht an eine andere KESB innerhalb des Statistikjahres.
              </xs:documentation>
            </xs:annotation>
          </xs:element>

          <xs:element name="UebertragungErwachsene" type="ctUebertragungErwachsene" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation xml:lang="de">
                Element für Übertragung einer Massnahme nach Erwachsenenschutzrecht an eine andere KESB innerhalb des Statistikjahres.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:choice>

      <xs:element name="NichtMassnahmengebundenesGeschaeftKinder" type="ctNichtMassnahmengebundenesGeschaeftKinder" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation xml:lang="de">
            Element für nicht massnahmengebundenes Geschäft nach Kindesschutzrecht innerhalb des Statistikjahres.
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="NichtMassnahmengebundenesGeschaeftErwachsene" type="ctNichtMassnahmengebundenesGeschaeftErwachsene" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation xml:lang="de">
            Element für nicht massnahmengebundenes Geschäft nach Erwachsenenschutzrecht innerhalb des Statistikjahres.
          </xs:documentation>
        </xs:annotation>
      </xs:element>

    </xs:sequence>

    <xs:attribute name="Jahrgang" type="xs:int" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="de">Geburtsjahr der Person.</xs:documentation>
      </xs:annotation>
    </xs:attribute>

    <xs:attribute name="Geschlecht" type="CodeGeschlecht" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="de">Geschlecht der Person.</xs:documentation>
      </xs:annotation>
    </xs:attribute>

    <xs:attribute name="Erfassung" type="xs:date" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="de">Erfassung (Errichtung erste Massnahme).</xs:documentation>
      </xs:annotation>
    </xs:attribute>

    <xs:attribute name="Abschluss" type="xs:date" use="optional">
      <xs:annotation>
        <xs:documentation xml:lang="de">Abschluss (Aufhebung letzte Massnahme).</xs:documentation>
      </xs:annotation>
    </xs:attribute>

  </xs:complexType>

  <xs:complexType name="ctMassnahmeKinder">
    <xs:annotation>
      <xs:documentation xml:lang="de">Container für Massnahme nach Kindesschutzrecht.</xs:documentation>
    </xs:annotation>

    <xs:sequence minOccurs="1" maxOccurs="1">

      <xs:element name="Mandatstraeger" type="CodeMandatstraeger" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xml:lang="de">
            Element für Mandatsträger. Der Mandatsträger wird in der Regel bei folgenden Massnahmen angegeben:

            • MAK010 Art. 306 Abs. 2 ZGB (Interessenkollision/Verhinderung der Eltern) - Errichtung Vertretungsbeistandschaft
            • MAK050 Art. 308 Abs. 1 ZGB (Erziehungsbeistandschaft: Beratung)
            • MAK360 Art. 308 Abs. 2 ZGB (Beistandschaft mit besonderen Befugnissen) – Feststellung Vaterschaft
            • MAK060 Art. 308 Abs. 2 ZGB (Beistandschaft mit besonderen Befugnissen) - Unterhalt
            • MAK070 Art. 308 Abs. 2 ZGB (Beistandschaft mit besonderen Befugnissen) - persönlicher Verkehr
            • MAK080 Art. 308 Abs. 2 ZGB (Beistandschaft mit besonderen Befugnissen) - medizinische Behandlung/Betreuung
            • MAK090 Art. 308 Abs. 2 ZGB (Beistandschaft mit besonderen Befugnissen) - Schule, Berufslehre, etc.
            • MAK100 Art. 308 Abs. 2 ZGB (Beistandschaft mit besonderen Befugnissen) - Anderes
            • MAK160 Art. 309 ZGB (Paternitätsbeistandschaft)
            • MAK270 Art. 314a bis ZGB (Verfahrensbeistandschaft)
            • MAK300 Art. 325 ZGB (Kindesvermögensverwaltungsbeistandschaft, Entziehung der Verwaltung)
            • MAK310 Art. 327a ZGB (Vormundschaft bei Kind ohne elterliche Sorge)
            • MAK320 Art. 544 Abs. 1 bis ZGB (Vertretungsbeistandschaft zur Interessenwahrung des ungeborenen Kindes)
            • MAK330 Art. 17 BG HAÜ (Beistandschaft)
            • MAK340 Art. 18 BG HAÜ (Vormundschaft)
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="HauptIndikation" minOccurs="0" maxOccurs="1" type="CodeIndikationKinder">
        <xs:annotation>
          <xs:documentation xml:lang="de">
            Element für Hauptindikation (zum Zeitpunkt der Errichtung). Die Hauptindikation wird in der Regel bei folgenden Massnahmen angegeben:

            • MAK350 Art. 307 Abs. 1 ZGB (geeignete Massnahme)
            • MAK030 Art. 307 Abs. 3 ZGB (Weisung oder Ermahnung)
            • MAK040 Art. 307 Abs. 3 ZGB (Erziehungsaufsicht: Person/Stelle mit Einblick und Auskunft)
            • MAK050 Art. 308 Abs. 1 ZGB (Erziehungsbeistandschaft: Beratung)
            • MAK360 Art. 308 Abs. 2 ZGB (Beistandschaft mit besonderen Befugnissen) – Feststellung Vaterschaft
            • MAK060 Art. 308 Abs. 2 ZGB (Beistandschaft mit besonderen Befugnissen) - Unterhalt
            • MAK070 Art. 308 Abs. 2 ZGB (Beistandschaft mit besonderen Befugnissen) - persönlicher Verkehr
            • MAK080 Art. 308 Abs. 2 ZGB (Beistandschaft mit besonderen Befugnissen) - medizinische Behandlung/Betreuung
            • MAK090 Art. 308 Abs. 2 ZGB (Beistandschaft mit besonderen Befugnissen) - Schule, Berufslehre, etc.
            • MAK100 Art. 308 Abs. 2 ZGB (Beistandschaft mit besonderen Befugnissen) - Anderes
            • MAK370 Art. 308 Abs. 3 ZGB (Beistandschaft mit besonderen Befugnissen und Beschränkung der elterlichen Sorge) – Feststellung Vaterschaft
            • MAK110 Art. 308 Abs. 3 ZGB (Beistandschaft mit besonderen Befugnissen und Beschränkung der elterlichen Sorge) - Unterhalt
            • MAK120 Art. 308 Abs. 3 ZGB (Beistandschaft mit besonderen Befugnissen und Beschränkung der elterlichen Sorge) - persönlicher Verkehr
            • MAK130 Art. 308 Abs. 3 ZGB (Beistandschaft mit besonderen Befugnissen und Beschränkung der elterlichen Sorge) - medizinische Behandlung/Betreuung
            • MAK140 Art. 308 Abs. 3 ZGB (Beistandschaft mit besonderen Befugnissen und Beschränkung der elterlichen Sorge) - Schule, Berufslehre, etc.
            • MAK150 Art. 308 Abs. 3 ZGB (Beistandschaft mit besonderen Befugnissen und Beschränkung der elterlichen Sorge) - Anderes
            • MAK170 Art. 310 Abs. 1 ZGB (Aufhebung des Obhutsrechts und Unterbringung - Wegnahme und Unterbringung von Amtes wegen)
            • MAK180 Art. 310 Abs. 1 ZGB i.V.m. Art. 314b ZGB (Aufhebung des Obhutsrechts und Unterbringung - Wegnahme und Unterbringung von Amtes wegen - Unterbringung in geschlossener Einrichtung oder psychiatrischer Klinik)
            • MAK190 Art. 310 Abs. 2 ZGB (Aufhebung des Obhutsrechts und Unterbringung - Wegnahme und Unterbringung auf Begehren des Kindes oder der Eltern)
            • MAK200 Art. 310 Abs. 2 ZGB i.V.m. Art. 314b ZGB (Aufhebung des Obhutsrechts und Unterbringung - Wegnahme und Unterbringung auf Begehren des Kindes oder der Eltern - Unterbringung in geschlossener Einrichtung oder psychiatrischer Klinik)
            • MAK210 Art. 310 Abs. 3 ZGB (Aufhebung des Obhutsrechts und Unterbringung - Verbot der Rücknahme)
            • MAK220 Art. 310 Abs. 3 ZGB i.V.m. Art. 314b ZGB (Aufhebung des Obhutsrechts und Unterbringung - Verbot der Rücknahme - Unterbringung in geschlossener Einrichtung oder psychiatrischer Klinik)
            • MAK240 Art. 311 Abs. 1 Ziff. 2 ZGB (Entziehung der elterlichen Sorge von Amtes wegen - Eltern haben sich nicht ernstlich gekümmert oder Pflichten verletzt)
            • MAK230 Art. 311 Abs. 1 Ziff. 1 ZGB (Entziehung der elterlichen Sorge von Amtes wegen - Eltern sind ausserstande)
            • MAK250 Art. 312 Ziff. 1 ZGB (Entziehung der elterlichen Sorge mit Einverständnis der Eltern - auf Antrag der Eltern)
            • MAK260 Art. 312 Ziff. 2 ZGB (Entziehung der elterlichen Sorge mit Einverständnis der Eltern - Einwilligung der Eltern in Adoption)
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="NebenIndikation" minOccurs="0" maxOccurs="unbounded" type="CodeIndikationKinder">
        <xs:annotation>
          <xs:documentation xml:lang="de">
            Element für Nebenindikation (zum Zeitpunkt der Errichtung). Die Nebenindikation wird in der Regel (*) bei folgenden Massnahmen angegeben:
            • MAK350 Art. 307 Abs. 1 ZGB (geeignete Massnahme)
            • MAK030 Art. 307 Abs. 3 ZGB (Weisung oder Ermahnung)
            • MAK040 Art. 307 Abs. 3 ZGB (Erziehungsaufsicht: Person/Stelle mit Einblick und Auskunft)
            • MAK050 Art. 308 Abs. 1 ZGB (Erziehungsbeistandschaft: Beratung)
            • MAK360 Art. 308 Abs. 2 ZGB (Beistandschaft mit besonderen Befugnissen) – Feststellung Vaterschaft
            • MAK060 Art. 308 Abs. 2 ZGB (Beistandschaft mit besonderen Befugnissen) - Unterhalt
            • MAK070 Art. 308 Abs. 2 ZGB (Beistandschaft mit besonderen Befugnissen) - persönlicher Verkehr
            • MAK080 Art. 308 Abs. 2 ZGB (Beistandschaft mit besonderen Befugnissen) - medizinische Behandlung/Betreuung
            • MAK090 Art. 308 Abs. 2 ZGB (Beistandschaft mit besonderen Befugnissen) - Schule, Berufslehre, etc.
            • MAK100 Art. 308 Abs. 2 ZGB (Beistandschaft mit besonderen Befugnissen) - Anderes
            • MAK370 Art. 308 Abs. 3 ZGB (Beistandschaft mit besonderen Befugnissen und Beschränkung der elterlichen Sorge) – Feststellung Vaterschaft
            • MAK110 Art. 308 Abs. 3 ZGB (Beistandschaft mit besonderen Befugnissen und Beschränkung der elterlichen Sorge) - Unterhalt
            • MAK120 Art. 308 Abs. 3 ZGB (Beistandschaft mit besonderen Befugnissen und Beschränkung der elterlichen Sorge) - persönlicher Verkehr
            • MAK130 Art. 308 Abs. 3 ZGB (Beistandschaft mit besonderen Befugnissen und Beschränkung der elterlichen Sorge) - medizinische Behandlung/Betreuung
            • MAK140 Art. 308 Abs. 3 ZGB (Beistandschaft mit besonderen Befugnissen und Beschränkung der elterlichen Sorge) - Schule, Berufslehre, etc.
            • MAK150 Art. 308 Abs. 3 ZGB (Beistandschaft mit besonderen Befugnissen und Beschränkung der elterlichen Sorge) - Anderes
            • MAK170 Art. 310 Abs. 1 ZGB (Aufhebung des Obhutsrechts und Unterbringung - Wegnahme und Unterbringung von Amtes wegen)
            • MAK180 Art. 310 Abs. 1 ZGB i.V.m. Art. 314b ZGB (Aufhebung des Obhutsrechts und Unterbringung - Wegnahme und Unterbringung von Amtes wegen - Unterbringung in geschlossener Einrichtung oder psychiatrischer Klinik)
            • MAK190 Art. 310 Abs. 2 ZGB (Aufhebung des Obhutsrechts und Unterbringung - Wegnahme und Unterbringung auf Begehren des Kindes oder der Eltern)
            • MAK200 Art. 310 Abs. 2 ZGB i.V.m. Art. 314b ZGB (Aufhebung des Obhutsrechts und Unterbringung - Wegnahme und Unterbringung auf Begehren des Kindes oder der Eltern - Unterbringung in geschlossener Einrichtung oder psychiatrischer Klinik)
            • MAK210 Art. 310 Abs. 3 ZGB (Aufhebung des Obhutsrechts und Unterbringung - Verbot der Rücknahme)
            • MAK220 Art. 310 Abs. 3 ZGB i.V.m. Art. 314b ZGB (Aufhebung des Obhutsrechts und Unterbringung - Verbot der Rücknahme - Unterbringung in geschlossener Einrichtung oder psychiatrischer Klinik)
            • MAK240 Art. 311 Abs. 1 Ziff. 2 ZGB (Entziehung der elterlichen Sorge von Amtes wegen - Eltern haben sich nicht ernstlich gekümmert oder Pflichten verletzt)
            • MAK230 Art. 311 Abs. 1 Ziff. 1 ZGB (Entziehung der elterlichen Sorge von Amtes wegen - Eltern sind ausserstande)
            • MAK250 Art. 312 Ziff. 1 ZGB (Entziehung der elterlichen Sorge mit Einverständnis der Eltern - auf Antrag der Eltern)
            • MAK260 Art. 312 Ziff. 2 ZGB (Entziehung der elterlichen Sorge mit Einverständnis der Eltern - Einwilligung der Eltern in Adoption)

            * Die Angabe der Nebenindikation ist grundsätzlich optional.
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="Gefaehrdungsmeldung" type="CodeGefaehrdungsmeldungKinder" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xml:lang="de">Element für (erste) Gefährdungsmeldung (zum Zeitpunkt der Errichtung). Die Angabe der Gefährdungsmeldung ist grundsätzlich optional.</xs:documentation>
        </xs:annotation>
      </xs:element>

    </xs:sequence>

    <xs:attribute name="MassnahmeArt" type="CodeMassnahmeartKinder" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="de">Art der Massnahme nach Kindesschutzrecht.</xs:documentation>
      </xs:annotation>
    </xs:attribute>

    <xs:attribute name="Errichtung" type="xs:date" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="de">Datum der Errichtung der Massnahme.</xs:documentation>
      </xs:annotation>
    </xs:attribute>

    <xs:attribute name="Urteil" type="xs:date" use="optional">
      <xs:annotation>
        <xs:documentation xml:lang="de">Datum des eherechtlichen Urteils (falls Massnahme aufgrund Vollzug eherechtlichem Urteil)</xs:documentation>
      </xs:annotation>
    </xs:attribute>

  </xs:complexType>

  <xs:complexType name="ctMassnahmeErwachsene">
    <xs:annotation>
      <xs:documentation xml:lang="de">Container für Massnahme nach Erwachsenenschutzrecht.</xs:documentation>
    </xs:annotation>

    <xs:sequence minOccurs="1" maxOccurs="1">

      <xs:choice minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation xml:lang="de">Entweder Mandatsträger oder Aufgabenbereich(e). Eine Kombination ist nicht möglich.</xs:documentation>
        </xs:annotation>

        <xs:element name="Aufgabenbereich" type="ctAufgabenbereich" minOccurs="1" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation xml:lang="de">
              Element für Aufgabenbereich. Die Aufgabenbereiche werden in der Regel bei folgenden Massnahmen angegeben:

              • MAE040 Art. 393 ZGB (Begleitbeistandschaft)
              • MAE050 Art. 394 Abs. 1 ZGB (Vertretungsbeistandschaft - allgemein)
              • MAE060 Art. 394 Abs. 2 ZGB (Vertretungsbeistandschaft - Einschränkung der Handlungsfähigkeit)
              • MAE070 Art. 395 Abs. 1 ZGB (Vertretungsbeistandschaft - Vermögensverwaltung)
              • MAE080 Art. 395 Abs. 3 ZGB (Vertretungsbeistandschaft - Entziehung Zugriff auf einzelne oder alle Vermögenswerte/Einkommensquellen)
