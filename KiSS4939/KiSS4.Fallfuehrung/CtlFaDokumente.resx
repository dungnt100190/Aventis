  <data name="qryFaDokumente.SelectStatement" xml:space="preserve">
    <value>SELECT
  FDO.FaDokumenteID,
  FDO.FaLeistungID,
  FDO.BaPersonID_LT,
  FDO.BaPersonID_Adressat,
  FDO.BaInstitutionID_Adressat,
  FDO.VmPriMaID_Adressat,
  FDO.UserID_Absender,
  FDO.UserID_VisumBeantragtDurch,
  FDO.UserID_VisumBeantragtBei,
  FDO.UserID_VisiertDurch,
  FDO.UserID_EkVisumUser,
  FDO.DocumentID,
  FDO.DocumentID_Merkblatt,
  FDO.DatumErstellung,
  FDO.StatusCode,
  FDO.PendenzDatum,
  FDO.PendenzErledigt,
  FDO.Stichwort,
  FDO.EingangAusgang,
  FDO.ThemaCode,
  FDO.VisumBeantragtDatum,
  FDO.VisiertDatum,
  FDO.EkStatusCode,
  FDO.EkLaufNr,
  FDO.EkKW,
  FDO.EkJahr,
  FDO.EkVisumDatum,
  FDO.Bemerkung,
  FDO.Vertraulich,
  FDO.IsDeleted,
  FDO.FaDauerCode,
  FDO.VmErbDienstCode,
  FDO.FaThemaCodes,
  FaThemaCodesText = dbo.fnLOVMLColumnListe('FaThema', FDO.FaThemaCodes, NULL, {5}),
  FDO.IstBericht,
  FDO.Creator,
  FDO.Created,
  FDO.Modifier,
  FDO.Modified,
  FDO.FaDokumenteTS,
  --  
  AbsenderName = dbo.fnGetLastFirstName(NULL, USR.LastName, USR.FirstName),
  AdressatName = CASE 
                   WHEN FDO.BaInstitutionID_Adressat IS NOT NULL THEN dbo.fnGetLastFirstName(NULL, INS.Name, INS.Vorname)
                   WHEN FDO.BaPersonID_Adressat IS NOT NULL THEN dbo.fnGetLastFirstName(NULL, PER.Name, PER.Vorname)
                   WHEN FDO.VmPriMaID_Adressat IS NOT NULL THEN dbo.fnGetLastFirstName(NULL, PRM.Name, PRM.Vorname)
                   ELSE ''
                 END
FROM dbo.FaDokumente          FDO WITH (READUNCOMMITTED)
  LEFT JOIN dbo.XUser         USR WITH (READUNCOMMITTED) ON USR.UserID = FDO.UserID_Absender
  LEFT JOIN dbo.BaPerson      PER WITH (READUNCOMMITTED) ON PER.BaPersonID = FDO.BaPersonID_Adressat
  LEFT JOIN dbo.BaInstitution INS WITH (READUNCOMMITTED) ON INS.BaInstitutionID = FDO.BaInstitutionID_Adressat
  LEFT  JOIN dbo.VmPriMa       PRM WITH (READUNCOMMITTED) ON PRM.VmPriMaID = FDO.VmPriMaID_Adressat 
WHERE FDO.FaLeistungID = {0}
  AND (FDO.IsDeleted = {1} OR FDO.IsDeleted = {2})
  AND (dbo.fnXCodeListsHaveMatch(FDO.FaThemaCodes, {3}) = 1 or {4} = 1)
--- AND FDO.DatumErstellung &gt;= {edtFaDokKorreSucheDatumVon}
--- AND FDO.DatumErstellung &lt;= {edtFaDokKorreSucheDatumBis}
--- AND FDO.Stichwort LIKE '%'+{edtFaDokKorreSucheStichwort}+'%'
--- AND dbo.fnGetLastFirstName(NULL, USR.LastName, USR.FirstName) LIKE '%' + {edtFaDokKorreSucheAbsender} + '%'
--- AND (INS.Name LIKE '%' + {edtFaDokKorreSucheAdressat} + '%' OR PER.Name LIKE '%'+{edtFaDokKorreSucheAdressat}+'%')
ORDER BY FDO.DatumErstellung ASC;</value>
  </data>
