  <data name="qryPopupMenu.SelectStatement" xml:space="preserve">
    <value>SELECT
  UserMayReadFall     = dbo.fnUserMayReadFall({2}, {1}),
  CaseIsClosed        = CASE WHEN FAL.DatumBis IS NULL THEN 0 ELSE 1 END,
  ServiceIsClosed     = CASE WHEN FLE.DatumBis IS NULL THEN 0 ELSE 1 END,
  IstAlim             = CASE WHEN FLE.FaProzessCode = 201 THEN 1 ELSE 0 END,
  IstVM               = CASE WHEN FLE.FaProzessCode = 210 THEN 1 ELSE 0 END,
  HatF                = IsNull((SELECT TOP 1 1 FROM FaLeistung
                                WHERE FaFallID = FAL.FaFallID AND ModulID = 2 AND FaProzessCode = 200), 0),
  HatFAlim            = IsNull((SELECT TOP 1 1 FROM FaLeistung
                                WHERE FaFallID = FAL.FaFallID AND ModulID = 2 AND FaProzessCode = 201), 0),
  HatVm               = IsNull((SELECT TOP 1 1 FROM FaLeistung
                                WHERE FaFallID = FAL.FaFallID AND ModulID = 2 AND FaProzessCode = 210), 0),
  HatVoranmeldung     = IsNull((SELECT TOP 1 1 FROM FaVoranmeldung V
                                LEFT JOIN FaLeistung L on L.FaLeistungID = V.FaLeistungID
                                WHERE L.FaFallID = {0} 
                                AND V.FaVoranmeldungAbschlussgrundCode is NULL), 0),
  HatCheckin          = IsNull((SELECT TOP 1 1 FROM FaCheckin C          
                                LEFT JOIN FaLeistung L on L.FaLeistungID = C.FaLeistungID
                                WHERE L.FaFallID = {0}
                                AND (
                                 C.FallabschlussGrundCode is NULL OR 
                                 (C.FallabschlussGrundCode = 7 AND C.QTUebergabeDatum IS NULL)
                                )), 0),
  HatAssessment       = IsNull((SELECT TOP 1 1 FROM FaAssessment A      
                                LEFT JOIN FaLeistung L on L.FaLeistungID = A.FaLeistungID
                                WHERE L.FaFallID = {0}
                                AND (
                                 A.FaAssessmentAbschlussGrundCode is NULL)), 0),
  HatCheckinAlim      = IsNull((SELECT TOP 1 1 FROM FaCheckinAlim      WHERE FaLeistungID = FLE.FaLeistungID), 0),
  HatRessourcenpaket  = IsNull((SELECT TOP 1 1 FROM FaRessourcenpaket  WHERE FaLeistungID = FLE.FaLeistungID), 0),
  HatZielvereinbarung = IsNull((SELECT TOP 1 1 FROM FaZielvereinbarung WHERE FaLeistungID = FLE.FaLeistungID), 0),
  HatAbklaerung       = IsNull((SELECT TOP 1 1 FROM FaAbklaerung       WHERE FaLeistungID = FLE.FaLeistungID), 0)
FROM FaFall FAL
LEFT  JOIN FaLeistung FLE ON FLE.FaLeistungID = {1}
WHERE FAL.FaFallID = {0}  -- 51042</value>
