  <data name="qryFaAktennotiz.SelectStatement" xml:space="preserve">
    <value>SELECT 
  AKT.FaAktennotizID,
  AKT.FaLeistungID,
  AKT.UserID,
  AKT.Datum,
  AKT.Zeit,
  AKT.FaThemaCodes,
  FaThemaCodesText = dbo.fnLOVMLColumnListe('FaThema', AKT.FaThemaCodes, NULL, {5}),
  AKT.FaDauerCode,
  AKT.FaGespraechsStatusCode,
  AKT.FaGespraechstypCode,
  AKT.Kontaktpartner,
  AKT.AlimentenstelleTypCode,
  AKT.Stichwort,
  AKT.InhaltRTF,
  AKT.Vertraulich,
  AKT.BesprechungThema1,
  AKT.BesprechungThema2,
  AKT.BesprechungThema3,
  AKT.BesprechungThema4,
  AKT.BesprechungThemaText1,
  AKT.BesprechungThemaText2,
  AKT.BesprechungThemaText3,
  AKT.BesprechungThemaText4,
  AKT.BesprechungZiel1,
  AKT.BesprechungZiel2,
  AKT.BesprechungZiel3,
  AKT.BesprechungZiel4,
  AKT.BesprechungZielGrad1,
  AKT.BesprechungZielGrad2,
  AKT.BesprechungZielGrad3,
  AKT.BesprechungZielGrad4,
  AKT.FaKontaktartCode,
  AKT.Pendenz1,
  AKT.Pendenz2,
  AKT.Pendenz3,
  AKT.Pendenz4,
  AKT.PendenzErledigt1,
  AKT.PendenzErledigt2,
  AKT.PendenzErledigt3,
  AKT.PendenzErledigt4,
  AKT.IsDeleted,
  AKT.Creator,
  AKT.Created,
  AKT.Modifier,
  AKT.Modified,
  AKT.FaAktennotizTS,
  [User] = USR.LastName + ISNULL(', ' + USR.FirstName, '')
FROM dbo.FaAktennotizen AKT WITH(READUNCOMMITTED)
  LEFT JOIN dbo.XUser   USR WITH(READUNCOMMITTED) ON USR.UserID = AKT.UserID
WHERE AKT.FaLeistungID = {0}
  AND (AKT.IsDeleted = {1} OR AKT.IsDeleted = {2})
  AND (dbo.fnXCodeListsHaveMatch(AKT.FaThemaCodes, {3}) = 1 or {4} = 1)
---  AND AKT.Datum &gt;= {edtFaDokSucheDatumVon}
---  AND AKT.Datum &lt;= {edtFaDokSucheDatumBis}
---  AND AKT.Stichwort LIKE '%'+{edtFaDokSucheStichwort}+'%'
---  AND AKT.InhaltRTF LIKE '%'+{edtFaDokSucheInhalt}+'%'
---  AND AKT.UserID = {edtFaDokSucheSAR.LookupID}
---  AND AKT.FaKontaktartCode = {edtFaDokSucheKontaktart}
ORDER BY AKT.Datum ASC</value>
  </data>
  <data name="edtFaDokSucheSAR.LookupSQL" xml:space="preserve">
    <value>select ID = UserID, SAR = LastName + isNull(', ' + FirstName,''), [Kürzel] = LogonName from   XUser where LastName + isNull(', ' + FirstName,'') like {0} + '%' order by SAR ---- select  LastName + isNull(', ' + FirstName,'') from XUser where UserID = {0} </value>
  </data>
