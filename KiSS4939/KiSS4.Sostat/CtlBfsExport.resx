  <data name="qryDossier.SelectStatement" xml:space="preserve">
    <value>SELECT Jahr,
       DOS.BFSDossierID,
       DOS.BFSLeistungsartCode,
       DOS.Stichtag,
       DOS.PlausibilisierungDatum,
       PRS.PersonName,
       AnzahlFehler = (SELECT COUNT(*) 
                       FROM dbo.BFSWert WITH (READUNCOMMITTED)
                       WHERE BFSDossierID = DOS.BFSDossierID
                         AND PlausiFehler IS NOT NULL),
       BaPersonID$ = PRS.BaPersonID
FROM dbo.BFSDossier               DOS WITH (READUNCOMMITTED)
  INNER JOIN dbo.BFSDossierPerson PRS WITH (READUNCOMMITTED)
                               ON PRS.BFSDossierID = DOS.BFSDossierID 
                              AND PRS.BFSPersonCode = 1 -- Antragsteller
  LEFT  JOIN dbo.FaLeistung       LST WITH (READUNCOMMITTED)
                               ON LST.FaLeistungID = DOS.FaLeistungID
  LEFT  JOIN dbo.vwUser       USR WITH (READUNCOMMITTED)
                           ON USR.UserID = LST.UserID
WHERE 1 = 1
--- AND Jahr = {edtErhebungsjahrX}
--- AND DOS.UserID = {edtSARX.LookupID}
--- AND PersonName like {edtKlientX} + '%'
--- AND DOS.BFSLeistungsartCode IN (SELECT * FROM dbo.fnSplit({edtLeistungsartX}, ','))
--- AND DOS.BFSDossierID = {edtBFSDossierID}
--- AND USR.OrgUnitID = {edtSektionX}
</value>
  </data>
  <data name="edtSARX.LookupSQL" xml:space="preserve">
    <value>select ID = UserID, SAR = LastName + isNull(', ' + FirstName,''), [Kürzel] = LogonName from   XUser where LastName + isNull(', ' + FirstName,'') like {0} + '%' order by SAR ---- select  LastName + isNull(', ' + FirstName,'') from XUser where UserID = {0} </value>
  </data>
