  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>DECLARE @NurStichtag BIT
DECLARE @NurAnfangszustand BIT
--- SET @NurStichtag ={edtNurStichtagX}
--- SET @NurAnfangszustand ={edtNurAnfangszustandX}

SELECT 
  SAR          = USR.NameVorname,
  Person       = ISNULL(PRS.NameVorname, DOP.PersonName),
  PlausiFehler = WRT.PlausiFehler,
  Variable     = FRG.Variable,
  [Dossier Art]= CASE WHEN DOS.Stichtag = 1 THEN 'Stichtag' ELSE 'Anfangszustand' END,
  [Dossier-ID] = DOS.BFSDossierID,
  BaPersonID$  = PRS.BaPersonID
FROM BFSWert                  WRT WITH(READUNCOMMITTED)
  INNER JOIN BFSFrage         FRG WITH(READUNCOMMITTED) ON FRG.BFSFrageID = WRT.BFSFrageID
  INNER JOIN BFSDossier       DOS WITH(READUNCOMMITTED) ON DOS.BFSDossierID = WRT.BFSDossierID
  INNER JOIN BFSDossierPerson DOP WITH(READUNCOMMITTED) ON DOP.BFSDossierPersonID = WRT.BFSDossierPersonID OR 
                                                           -- falls der Wert keiner DossierPerson zugeordnet ist -&gt; dem Antragssteller zuordnen
                                                           (WRT.BFSDossierPersonID IS NULL AND 
                                                            DOP.BFSDossierID  = DOS.BFSDossierID AND 
                                                            DOP.BFSPersonCode = 1 /*Antragssteller*/)
  INNER JOIN vwUser           USR                       ON USR.UserID = DOS.UserID
  LEFT  JOIN vwPerson         PRS                       ON PRS.BaPersonID = DOP.BaPersonID
WHERE PlausiFehler IS NOT NULL AND PlausiFehler &lt;&gt; '' -- wegen Migration der Tabelle von text zu varchar(max) kann IS NOT NULL trotzdem NULLs zur√ºckgeben
  AND Jahr = {edtErhebungsjahr}
  AND (DOS.Stichtag IN (
    CASE 
        WHEN (@NurAnfangszustand  = 1 AND @NurStichtag = 0) THEN 0
        WHEN (@NurAnfangszustand  = 0 AND @NurStichtag = 1) THEN 1
        ELSE DOS.Stichtag
    END
  ))
--- AND USR.UserID = {edtSARX.LookupID}
-- AND WRT.BFSDossierPersonID = {edtBaPersonID.LookupID}
--- and PRS.NameVorname = {edtBaPersonID}
ORDER BY SAR, Variable

</value>
