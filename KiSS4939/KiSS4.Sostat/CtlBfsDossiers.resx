  <data name="qryAnswersOfDossier.SelectStatement" xml:space="preserve">
    <value>SELECT BFSWertID, BFSDossierID, BFSDossierPersonID, Wert, BFW.BFSFrageID, WertManuell, 
       ManuellMutiert, PlausiFehler, BFSWertTS, 
       Variable, ExportNode,ExportAttribute, BFSFeldCode
FROM dbo.BFSWert BFW WITH (READUNCOMMITTED)
   INNER JOIN dbo.BFSFrage BFF WITH (READUNCOMMITTED) ON BFW.BFSFrageID = BFF.BFSFrageID
WHERE (BFSDossierID = {0})</value>
