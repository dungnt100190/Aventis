  <data name="qryFrageWert.SelectStatement" xml:space="preserve">
    <value>SELECT 
  DOS.BFSDossierID, DOS.BFSLeistungsartCode,
  PRS.BFSDossierPersonID,
  FRA.BfsFrageID, FRA.BFSKategorieCode, FRA.BfsFeldCode, FRA.BfsFormat, FRA.BFSLOVName,
  FRA.Variable, FRA.Frage, FRA.HilfeText, FRA.ExportNode, FRA.ExportAttribute,
  FRA.HerkunftBeschreibung, FRA.Editierbar, FRA.FilterRegel, ExcelFarbCode = IsNull(COL.ExcelFarbCode, 0),
  WRT.BFSWertID, WRT.BFSDossierID, WRT.BFSDossierPersonID, WRT.BFSFrageID,
  Wert = IsNull(WRT.Wert, FRA.Vorgabewert),
  WRT.PlausiFehler, LST.BaPersonID
FROM dbo.BFSDossier                DOS WITH (READUNCOMMITTED)
  INNER JOIN dbo.BFSDossierPerson  PRS WITH (READUNCOMMITTED) ON PRS.BFSDossierID = DOS.BFSDossierID
  INNER JOIN dbo.BFSLOVCode        FLT WITH (READUNCOMMITTED) ON FLT.LOVName = 'BFSLeistungsart' AND FLT.Code = DOS.BfsLeistungsartCode
  INNER JOIN dbo.BFSFrage          FRA WITH (READUNCOMMITTED) ON FRA.BFSKatalogVersionID = DOS.BFSKatalogVersionID 
                                       AND ',' + FRA.BFSLeistungsfilterCodes + ',' LIKE '%,' + CONVERT(VARCHAR, FLT.Filter) + ',%'
                                       AND FRA.BFSPersonCode = PRS.BFSPersonCode AND FRA.PersonIndex = PRS.PersonIndex
  LEFT  JOIN dbo.BFSFarbCode       COL WITH (READUNCOMMITTED) ON COL.BFSFrageID = FRA.BFSFrageID AND COL.Leistungsfilter = FLT.Filter
  LEFT  JOIN dbo.BFSWert           WRT WITH (READUNCOMMITTED) ON WRT.BFSDossierID = DOS.BFSDossierID
                                       AND WRT.BFSFrageID = FRA.BFSFrageID
  LEFT  JOIN dbo.FaLeistung        LST WITH (READUNCOMMITTED) ON LST.FaLeistungID = DOS.FaLeistungID
WHERE DOS.BFSDossierID = {0}
ORDER BY CASE WHEN FRA.Variable LIKE '_.%' THEN '0' + FRA.Variable ELSE FRA.Variable END</value>
  </data>
