  <data name="qryAssignedUsers.SelectStatement" xml:space="preserve">
    <value>SELECT UDT.XUser_XDocTemplateID,
       UDT.UserID,
       UDT.DocTemplateID,
       UDT.Creator,
       UDT.Created,
       UDT.Modifier,
       UDT.Modified,
       UDT.XUser_XDocTemplateTS
FROM dbo.XUser_XDocTemplate UDT
INNER JOIN dbo.vwUser USR ON UDT.UserID = USR.UserID
WHERE UDT.DocTemplateID = {0}
  AND USR.IsLocked = 0</value>
