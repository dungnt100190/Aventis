  <data name="qryAssignedXOrgUnits.SelectStatement" xml:space="preserve">
    <value>SELECT ODT.XOrgUnit_XDocTemplateID,
       ODT.OrgUnitID,
       ODT.DocTemplateID,
       ODT.Creator,
       ODT.Created,
       ODT.Modifier,
       ODT.Modified,
       ODT.XOrgUnit_XDocTemplateTS
FROM dbo.XOrgUnit_XDocTemplate ODT
WHERE ODT.DocTemplateID = {0}</value>
