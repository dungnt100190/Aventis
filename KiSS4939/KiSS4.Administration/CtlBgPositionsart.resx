  <data name="qryBgKostenart.SelectStatement" xml:space="preserve">
    <value>SELECT BgKostenartID = NULL, NrName = NULL, SortKey=NULL
UNION ALL
  SELECT BgKostenartID, 
         NrName=IsNull(KontoNr + ' ', '') + Name, 
	    SortKey=CASE WHEN IsNumeric(KontoNr) = 1 
                      THEN CONVERT(decimal, KontoNr) 
                      ELSE NULL 
                      END
  FROM   BgKostenart
  WHERE ModulID = {0}
  ORDER BY SortKey</value>
  </data>
