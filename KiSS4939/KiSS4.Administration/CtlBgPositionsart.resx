  <data name="qryBgPositionsart.SelectStatement" xml:space="preserve">
    <value>SELECT BPA.BgPositionsartID, BPA.BgKategorieCode, BPA.BgGruppeCode, BPA.Name, BPA.HilfeText, BPA.SortKey, 
BPA.BgKostenartID, BPA.NrmKontoCode, BPA.VerwaltungSD_Default, BPA.Spezkonto, BPA.ProPerson, BPA.ProUE, 
BPA.Masterbudget_EditMask, BPA.Monatsbudget_EditMask, BPA.ModulID, BPA.sqlRichtlinie, BPA.VarName, 
BPA.Verrechenbar, BPA.Bemerkung, BPA.NameTID, BPA.DatumVon, BPA.DatumBis, BPA.BgPositionsartCode, 
BPA.BgPositionsartID_CopyOf, BPA.System, BPA.KreditorEingeschraenkt, BPA.ErzeugtBfsDossier
FROM dbo.BgPositionsart        BPA WITH (READUNCOMMITTED)
  LEFT JOIN dbo.XLOVCode       KAT WITH (READUNCOMMITTED)           ON KAT.LOVName = 'BgKategorie' AND 
                                                                       KAT.Code = BPA.BgKategorieCode
  LEFT JOIN dbo.XLOVCode       GRP WITH (READUNCOMMITTED)           ON GRP.LOVName = 'BgGruppe' AND 
                                                                       GRP.Code = BPA.BgGruppeCode
WHERE BPA.ModulID = {0}
--- AND BPA.BgKategorieCode = {edtBgKategorieCodeX}  
--- AND BPA.BgGruppeCode  = {edtBgGruppeCodeX}  
--- AND BPA.Name LIKE '%' + {edtNameX} + '%'  
--- AND BPA.BgKostenartID = {edtBgKostenartIDX}  
--- AND ({chkNurAktive} = 0 OR (IsNull(BPA.DatumBis, dbo.fnDateOf(GetDate())) &gt;= dbo.fnDateOf(GetDate())))
ORDER BY KAT.SortKey, GRP.SortKey, BPA.SortKey</value>
  </data>
