  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>DECLARE @LanguageCode INT;
SET @LanguageCode = {0};

-- ALBV und ZuDe Konti holen
DECLARE @ALBVKonti VARCHAR(100);
DECLARE @ZuDeKonti VARCHAR(100);

SELECT @ALBVKonti = ';' + CONVERT(VARCHAR, dbo.fnXConfig('System\KliBu\Sozialhilferechnung\ALBVKonti', GETDATE())) + ';';
SELECT @ZuDeKonti = ';' + CONVERT(VARCHAR, dbo.fnXConfig('System\KliBu\Sozialhilferechnung\ZuDeKonti', GETDATE())) + ';';

-- Aktuelle und letzte KbPeriodeID holen
DECLARE @KbPeriodeID_Now INT;
DECLARE @KbPeriodeID_Last INT;

SELECT @KbPeriodeID_Now = KbPeriodeID
FROM dbo.KbPeriode WITH (READUNCOMMITTED)
WHERE KbMandantID = 1 -- Klibu
  AND GETDATE() BETWEEN PeriodeVon AND PeriodeBis;

SELECT @KbPeriodeID_Last = KbPeriodeID
FROM dbo.KbPeriode WITH (READUNCOMMITTED)
WHERE KbPeriodeID = (SELECT TOP 1 KbPeriodeID
                     FROM dbo.KbPeriode WITH (READUNCOMMITTED)
                     WHERE KbMandantID = 1
                     ORDER BY PeriodeVon DESC);

-- Output
SELECT [BgKostenartID]                = KOA.BgKostenartID,
       [KontoNr]                      = KOA.KontoNr,
       [Name]                         = KOA.Name,
       [Kostenart]                    = ISNULL(KOA.KontoNr + ' ', '') + KOA.Name,
       [ErfolgAktuellerKontoplan]     = dbo.fnLOVMLText('KbKontoklasse', KTO1.KbKontoklasseCode, @LanguageCode),
       [ErfolgLetzterKontoplan]       = dbo.fnLOVMLText('KbKontoklasse', KTO2.KbKontoklasseCode, @LanguageCode),
       [BgKostenart_WhGefKategorieID] = KGK.BgKostenart_WhGefKategorieID,
       [WhGefKategorieID]             = KGK.WhGefKategorieID,
       [WhGefKategorieCode]           = GEF.WhGefKategorieCode,
       [WhGefKategorieTypCode]        = GEF.WhGefKategorieTypCode,
       [WhGefKategorie]               = dbo.fnLOVMLText('WhGefKategorie', GEF.WhGefKategorieCode, @LanguageCode),
       [WhGefKategorieTyp]            = dbo.fnLOVMLText('WhGefKategorieTyp', GEF.WhGefKategorieTypCode, @LanguageCode),
       [IsALBV]                       = CONVERT(BIT,
                                          CASE
                                            WHEN @ALBVKonti LIKE '%;' + KOA.KontoNr + ';%' THEN 1
                                            ELSE 0
                                          END),
       [IsZuDe]                       = CONVERT(BIT,
                                          CASE
                                            WHEN @ZuDeKonti LIKE '%;' + KOA.KontoNr + ';%' THEN 1
                                            ELSE 0
                                          END),
       [IsInkassoprovision]           = ISNULL(KGK.IsInkassoprovision, 0),
       [Creator]                      = KGK.Creator,
       [Created]                      = KGK.Created,
       [Modifier]                     = KGK.Modifier,
       [Modified]                     = KGK.Modified
FROM dbo.BgKostenart                       KOA  WITH (READUNCOMMITTED)
  LEFT JOIN dbo.BgKostenart_WhGefKategorie KGK  WITH (READUNCOMMITTED) ON KGK.BgKostenartID = KOA.BgKostenartID
  LEFT JOIN dbo.WhGefKategorie             GEF  WITH (READUNCOMMITTED) ON GEF.WhGefKategorieID = KGK.WhGefKategorieID
  LEFT JOIN dbo.KbKonto                    KTO1 WITH (READUNCOMMITTED) ON KTO1.KbPeriodeID = @KbPeriodeID_Now -- Kontoplan aktueller Periode
                                                                      AND KTO1.KontoNr = KOA.KontoNr
  LEFT JOIN dbo.KbKonto                    KTO2 WITH (READUNCOMMITTED) ON KTO2.KbPeriodeID = @KbPeriodeID_Last -- Kontoplan letzter Periode
                                                                      AND KTO2.KontoNr = KOA.KontoNr
WHERE ModulID = 3
---  AND KOA.BgKostenartID = {edtKostenartX.LookupID}
---  AND GEF.WhGefKategorieID = {edtGefKategorieX.LookupID};</value>
