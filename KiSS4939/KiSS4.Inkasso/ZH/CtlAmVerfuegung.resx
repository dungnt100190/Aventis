  <data name="qryAnspruch.SelectStatement" xml:space="preserve">
    <value>select 
  A.FaLeistungID,
  A.AmAnspruchsberechnungID,
  A.Bezeichnung,
  A.BerechnungDatumAb,
  Status = dbo.fnLOVText('AmBerechnungsStatus', VER.AmVerfuegungStatusCode),
  BerechnungTyp = dbo.fnLOVText('AmBerechnungTyp', A.AmBerechnungTypCode),
  Einkommen = dbo.fnLOVText('AmEinkommensVariante', A.AmEinkommensVarianteCode),
  AnzKinder = (
    select Count(*) from dbo.AmAbKind K with(readuncommitted)
    where K.AmAnspruchsberechnungID = A.AmAnspruchsberechnungID
    and K.BerechtigtCode in (1,2,3)
  ),
  IsSelected = convert(bit, case when A.AmVerfuegungID Is NULL then 0 else 1 end)
from dbo.AmAnspruchsberechnung A with(readuncommitted)
left join dbo.FaLeistung L with(readuncommitted) on L.FaLeistungID = A.FaLeistungID
left join dbo.AmVerfuegung VER on VER.AmVerfuegungID = A.AmVerfuegungID
where A.FaLeistungID = {0}  -- 7006661
  and (A.AmBerechnungsStatusCode != 5 OR A.AmBerechnungsStatusCode IS NULL)
  and (
    (A.AmVerfuegungID IS NULL AND NOT EXISTS(
      select top 1 G.AmAbKindID from dbo.AmAbKind G with(readuncommitted) 
      where G.AmAnspruchsberechnungID = A.AmAnspruchsberechnungID
      and G.IkRechtstitelID IS NULL
      and G.BerechtigtCode in (1,2,3)
    )) OR
    A.AmVerfuegungID = {1}
  )
order by IsSelected desc, BerechnungDatumAb asc, A.AmAnspruchsberechnungID asc</value>
  </data>
