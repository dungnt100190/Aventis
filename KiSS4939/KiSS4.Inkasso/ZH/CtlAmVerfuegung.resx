  <data name="qryAmVerfuegung.SelectStatement" xml:space="preserve">
    <value>select V.*, 
  LEI.FaFallID,
  UserName = U.DisplayText,
  LeistungUserID = LEI.UserID,
  FallBaPersonID = FAL.BaPersonID,
  LeistungBaPersonID = LEI.BaPersonID
from dbo.AmVerfuegung V with(readuncommitted)
  left outer join dbo.vwUser U on U.UserID = V.UserID
  inner join dbo.FaLeistung LEI with(readuncommitted) on LEI.FaLeistungID = V.FaLeistungID
  inner join dbo.FaFall FAL with(readuncommitted) on FAL.FaFallID = LEI.FaFallID
where V.FaLeistungID = {0}</value>
