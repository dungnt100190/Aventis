  <data name="qryDatenKontrolle.SelectStatement" xml:space="preserve">
    <value>select top 1 ID = IkPositionID from dbo.IkPosition P with(readuncommitted)
where P.FaLeistungID = {0}
union all
select top 1 ID = IkRechtstitelID from dbo.IkRechtstitel R with(readuncommitted)
where R.FaLeistungID = {0}
union all
select top 1 ID = IkForderungID from dbo.IkForderung R with(readuncommitted)
where R.FaLeistungID = {0}</value>
