  <data name="qryAdresse.SelectStatement" xml:space="preserve">
    <value>SELECT TOP 1
  Adresse = 
    IsNull(P.Name + IsNull(' '+P.Vorname, ''), '')  + 
    IsNull(char(10) + A.Zusatz, '') + 
    IsNull(char(10) + A.Strasse + IsNull(' '+A.HausNr, ''), '') + 
    IsNull(char(10) + A.Postfach, '') +
    IsNull(char(10) + A.PLZ + IsNull(' '+A.Ort, ''), '')
FROM dbo.BaPerson P with(readuncommitted)
LEFT OUTER JOIN dbo.BaAdresse A with(readuncommitted) ON A.BaPersonID = P.BaPersonID
WHERE P.BaPersonID = {0}
  and GetDate() between IsNull(A.DatumVon, GetDate()) and IsNull(A.DatumBis, GetDate())
  and IsNull(A.AdresseCode, 1) = 1
order by IsNull(A.AdresseCode, 0) desc,IsNull(A.DatumVon, GetDate()) desc
</value>
  </data>
