  <data name="qryFaLeistung.SelectStatement" xml:space="preserve">
    <value>SELECT LST.*,
  USR.LogonName, 
  UserName = USR.LastName + USR.FirstName,
  VUFallName = VUP.NameVorname
    +IsNull(', geb. '+Convert(varchar, VUP.Geburtsdatum, 104), '')
    +IsNull(', Fall '+Convert(varchar, VUF.FaFallID), '')
FROM dbo.FaLeistung LST with(readuncommitted)
LEFT JOIN dbo.XUser USR with(readuncommitted) ON USR.UserID = LST.UserID
LEFT JOIN dbo.FaFall VUF with(readuncommitted) ON VUF.FaFallID = LST.VUFaFallID
LEFT JOIN dbo.vwPerson VUP ON VUP.BaPersonID = VUF.BaPersonID
WHERE LST.FaLeistungID = {0}</value>
