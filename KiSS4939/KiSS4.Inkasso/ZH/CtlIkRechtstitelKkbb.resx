  <data name="qryPerson.SelectStatement" xml:space="preserve">
    <value>select 
  P.BaPersonID, 
  Text = P.Name + IsNull(' ' + P.Vorname,'')+IsNull(', '+Convert(varchar(10), P.Geburtsdatum, 104), ''),
  P.Geburtsdatum
from dbo.FaFallPerson F WITH(READUNCOMMITTED)
left join dbo.BaPerson P WITH(READUNCOMMITTED) on P.BaPersonID = F.BaPersonID
where F.FafallID = {0} and 
  (
    ( dbo.fnGetAge(P.Geburtsdatum, GetDate()) &lt; 18 and 
      not P.Geburtsdatum IS NULL
    ) OR P.BaPersonID in (
      select Q.BaPersonID from dbo.IkRechtstitel Q WITH(READUNCOMMITTED)
      where IkRechtstitelID = {1}
    )
  )
  AND NOT P.BaPersonID IN ( 
    SELECT L.SchuldnerBaPersonID 
    FROM dbo.FaLeistung L WITH(READUNCOMMITTED)
    WHERE L.FaLeistungID = {2} 
      AND NOT L.SchuldnerBaPersonID IS NULL
  )
order by 2</value>
  </data>
  <data name="qryIkRechtstitel.SelectStatement" xml:space="preserve">
    <value>SELECT 
  RTT.IkRechtstitelID, 
  RTT.Bezeichnung, 
  RTT.BaPersonID, 
  RTT.IkRechtstitelGueltigVon,
  RTT.IkRechtstitelGueltigBis,
  RTT.Bemerkung,
  RTT.BeantragtAm,
  RTT.WohnhaftImKantonSeit,
  RTT.IkRechtstitelTS
FROM dbo.IkRechtstitel RTT WITH(READUNCOMMITTED)
WHERE RTT.IkRechtstitelID = {0}</value>
  </data>
