  <data name="qryCheck.SelectStatement" xml:space="preserve">
    <value>select Anzahl = count(*) from dbo.IkRechtstitel R WITH(READUNCOMMITTED)
left join dbo.FaLeistung L WITH(READUNCOMMITTED) on L.FaLeistungID = R.FaLeistungID
where L.FaFallID = {0}
and L.FaProzessCode = 407
and R.BaPersonID = {1}
and R.IkRechtstitelID &lt;&gt; {2}</value>
  </data>
  <data name="qryIkRechtstitel.TrayLocation" type="System.Drawing.Point, System.Drawing">
    <value>116, 17</value>
  </data>
  <data name="qryPosition.SelectStatement" xml:space="preserve">
    <value>select 
  AnzahlDaten = (
    select Count(*) from dbo.IkPosition WITH(READUNCOMMITTED) 
    where IkRechtstitelID = {0}),
  AnzahlSollst = (
    select Count(*) from dbo.IkPosition P WITH(READUNCOMMITTED)
    where P.IkRechtstitelID = {0} and Exists(
      select top 1 BUC.KbBuchungID from dbo.KbBuchung BUC WITH(READUNCOMMITTED) 
      where BUC.IkPositionID = P.IkPositionID
        and BUC.KbBuchungStatusCode != 8
    ) and P.Einmalig = 0),
  BaPersonID = (
    select BaPersonID from dbo.IkGlaeubiger WITH(READUNCOMMITTED)
    where IkRechtstitelID = {0})</value>
  </data>
