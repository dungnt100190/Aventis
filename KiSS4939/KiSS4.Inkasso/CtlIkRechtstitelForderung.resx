  <data name="qryIkForderung.SelectStatement" xml:space="preserve">
    <value>SELECT 
  FRD.IkForderungID,
  FRD.FaLeistungID,
  FRD.IkRechtstitelID,
  FRD.BaPersonID,
  FRD.IkForderungPeriodischCode,
  FRD.DatumAnpassenAb,
  FRD.DatumGueltigBis,
  FRD.Betrag,
  FRD.IkAnpassungsGrundCode,
  FRD.IkAnpassungsRegelCode,
  FRD.IndexStandBeiBerechnung,
  FRD.IndexStandDatum,
  FRD.Bemerkung,
  FRD.Sollgestellt,
  FRD.ALBVBerechtigt,
  FRD.Teuerungseinsprache,
  FRD.Unterstuetzungsfall,
  FRD.TeilALBV,
  FRD.TeilALBVBetrag,
  FRD.IkForderungTS,
  [Alter] = dbo.fnGetAge(PRS.Geburtsdatum, ISNULL(PRS.Sterbedatum, GETDATE())),
  Person  = PRS.NameVorname + ISNULL(', ' + CONVERT(VARCHAR(10), PRS.Geburtsdatum, 104), ''),
  [ALV]   = Betrag - ISNULL(TeilALBVBetrag, $0.0)
FROM dbo.IkForderung     FRD WITH (READUNCOMMITTED)
  LEFT JOIN dbo.vwPerson PRS ON PRS.BaPersonID = FRD.BaPersonID
WHERE FRD.IkRechtstitelID = {0}
ORDER BY dbo.fnGetAge(PRS.Geburtsdatum, GETDATE()) DESC,
         PRS.NameVorname ASC, 
         FRD.DatumAnpassenAb ASC</value>
  </data>
