  <data name="qryPerson.SelectStatement" xml:space="preserve">
    <value>SELECT 
  PRS.BaPersonID, 
  [Text]  = PRS.Name + IsNull(' ' + PRS.Vorname,'') + ISNULL(', ' + CONVERT(VARCHAR(10), PRS.Geburtsdatum, 104), ''),
  [Alter] = dbo.fnGetAge(PRS.Geburtsdatum, ISNULL(PRS.Sterbedatum, GETDATE()))
FROM dbo.IkGlaeubiger    GLB WITH (READUNCOMMITTED)
  LEFT JOIN dbo.BaPerson PRS WITH (READUNCOMMITTED) ON PRS.BaPersonID = GLB.BaPersonID
WHERE GLB.IkRechtstitelID = {0}
ORDER BY 2;</value>
