  <data name="qrySaldo.SelectStatement" xml:space="preserve">
    <value>select 
  P.IkPositionID,
  P.Datum, P.Monat, P.Jahr,
  Titel = dbo.fnLOVText('IkForderungEinmalig', P.IkForderungCode),
  P.ErledigterMonat,
  P.Unterstuetzungsfall,
  BetragVerrechg = IsNull(P.BetragALBVverrechnung,0), 
  BetragALBVIst = IsNull(P.BetragALBV,0),
  BetragALBVSoll = IsNull(P.BetragALBVSoll,0),
  Auszahlung = IsNull(P.BetragAuszahlung,0),
  ALBVDiff = IsNull(P.BetragALBVSoll,0) - IsNull(P.BetragALBV,0),
  Guthaben = Convert(money, 0.0),
  Saldo = Convert(money, 0.0),
  P.Bemerkung
from IkPosition P
left join IkGlaeubiger G on G.IkRechtstitelID = P.IkRechtstitelID 
  and G.BaPersonID = P.BaPersonID
where P.IkRechtstitelID = {0}
  and P.Einmalig = 0
  and P.BaPersonID = {1}
  and P.Datum &lt;= {2}
order by P.Datum asc</value>
  </data>
  <data name="qryGlaeubiger.SelectStatement" xml:space="preserve">
    <value>SELECT 
  GLB.BaPersonID,
  Name        = PRS.NameVorname + isNULL(', '+Convert(varchar, PRS.Geburtsdatum, 104) + ' ', ''),
  [Alter]     = dbo.fnGetAge(PRS.Geburtsdatum, getDate()),
  BetragALBV  = (SELECT TOP 1 POS.BetragALBV
                 FROM IkPosition POS 
                 WHERE POS.IkRechtstitelID = RTT.IkRechtstitelID
                   AND POS.BaPersonID = GLB.BaPersonID 
                   AND POS.Einmalig = 0
/* Original:                   AND POS.Datum &lt; dbo.fnDateSerial(YEAR({1}), MONTH({1})+1, 1)
 19.11.2008 Thomas Berger:*/
                   AND POS.Datum &lt; dateadd(d,1,dbo.fnLastDayOf({1}))
                 ORDER BY Datum DESC),
  BetragSaldo = (SELECT SUM(IsNull(POS.BetragAuszahlung,0))
                 FROM IkPosition POS 
                 WHERE POS.IkRechtstitelID = RTT.IkRechtstitelID
                   AND POS.BaPersonID = GLB.BaPersonID 
                   AND POS.Einmalig = 0
/* Original:                   AND POS.Datum &lt; dbo.fnDateSerial(YEAR({1}), MONTH({1})+1, 1))
 19.11.2008 Thomas Berger:*/
                   AND POS.Datum &lt; dateadd(d,1,dbo.fnLastDayOf({1})))
FROM IkGlaeubiger          GLB
  LEFT  JOIN IkRechtstitel RTT ON RTT.IkRechtstitelID = GLB.IkRechtstitelID
  LEFT  JOIN FaLeistung    LST ON LST.FaLeistungID = RTT.FaLeistungID
  LEFT  JOIN vwPerson      PRS ON PRS.BaPersonID = GLB.BaPersonID
WHERE GLB.IkRechtstitelID = {0}</value>
  </data>
