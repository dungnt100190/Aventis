  <data name="qryMonatszahlen.SelectStatement" xml:space="preserve">
    <value>SELECT IkPositionID            = IPO.IkPositionID,
       FaLeistungID            = IPO.FaLeistungID,
       IkRechtstitelID         = IPO.IkRechtstitelID,
       BetragALBV              = IPO.BetragALBV,
       Einmalig                = IPO.Einmalig,
       Datum                   = IPO.Datum,
       Monat                   = IPO.Monat,
       Jahr                    = IPO.Jahr,
       BaPersonID              = IPO.BaPersonID,
       TotalAliment            = IPO.TotalAliment,
       TotalAlimentSoll        = IPO.TotalAlimentSoll,
       BetragALBVSoll          = IPO.BetragALBVSoll,
       BetragKiZulage          = IPO.BetragKiZulage,
       BetragKiZulageSoll      = IPO.BetragKiZulageSoll,
       BetragALBVverrechnung   = IPO.BetragALBVverrechnung,
       BetragEinmalig          = IPO.BetragEinmalig,
       IkForderungEinmaligCode = IPO.IkForderungCode,
       IndexStandBeiBerechnung = IPO.IndexStandBeiBerechnung,
       IndexStandDatum         = IPO.IndexStandDatum,
       ErledigterMonat         = IPO.ErledigterMonat,
       Unterstuetzungsfall     = IPO.Unterstuetzungsfall,
       Bemerkung               = IPO.Bemerkung,
       -- BerechneteFelder
       BetragALBV_Calc         = CASE 
                                     WHEN IPO.Unterstuetzungsfall = 0 THEN IPO.BetragALBV
                                     ELSE 0
                                 END,
       BetragVermittelt_Calc   = CASE 
                                     WHEN IPO.Unterstuetzungsfall = 0 THEN IPO.TotalAliment - IPO.BetragALBV 
                                     ELSE 0
                                 END,
       BetragDiffALBV_Calc     = CASE 
                                     WHEN IPO.Unterstuetzungsfall = 1 THEN 0
                                     WHEN IsNull(IPO.BetragALBV, 0) &lt; IsNull(IPO.BetragALBVverrechnung, 0) THEN 0 
                                     ELSE IsNull(IPO.BetragALBV, 0) + IsNull(IPO.BetragALBVverrechnung, 0)
                                 END,
       BetragSozialhilfe_Calc  = CASE 
                                     WHEN IPO.Unterstuetzungsfall = 1 THEN  IPO.TotalAliment
                                     ELSE 0
                                 END,
       Person                  = PRS.NameVorname,
       JahrMonat               = dbo.fnDateSerial(IPO.Jahr, IPO.Monat, 1)
FROM IkPosition       IPO
  LEFT  JOIN vwPerson PRS ON PRS.BaPersonID = IPO.BaPersonID
WHERE IPO.IkRechtstitelID = {0}
  AND IPO.Einmalig = 0
ORDER BY IPO.Jahr desc, IPO.Monat DESC,
  PRS.NameVorname ASC</value>
