  <data name="qryBgPosition.SelectStatement" xml:space="preserve">
    <value>SELECT BPO.*,
  -- SKOS
  GrundbedarfI_Anpassung  = - BPO.Reduktion,
  Betrag_SKOS             = dbo.fnShGrundbedarfI(BBG.BgFinanzplanID),
  GrundbedarfZusatz       = IsNull(GBZ.Betrag, dbo.fnShGrundbedarfIZuschlag(BBG.BgFinanzplanID)),
  GrundbedarfII           = GB2.Betrag,
  GBII_AsOnePerson        = CONVERT(bit, CASE WHEN GB2.BgPositionsartID = 32014
                                         THEN 1 ELSE 0 END),
  GrundbedarfII_Anpassung = IsNull($0.00 - GB2.Reduktion, $0.00),
  GrundbedarfTotal        = IsNull((SELECT SUM(Betrag - Reduktion)
                                    FROM BgPosition
                                    WHERE BgBudgetID = BPO.BgBudgetID
                                      AND BgPositionsartID IN (32011, 32012, 32013, 32014)
                                      AND DatumVon IS NULL), $0.00),

  -- SKOS 2005
  SKOS2005_HG             = CONVERT(money, NULL),
  SKOS2005_UE             = CONVERT(money, NULL),
  SKOS2005_Anpassung      = - BPO.Reduktion,
  SKOS2005_Total          = - BPO.Reduktion,

  -- Andere
  ELSE_Betrag     = CONVERT(money, CASE WHEN BPO.BgPositionsartID IN (32011, 32015) THEN 0 ELSE BPO.Betrag END),
  ELSE_Anpassung  = CONVERT(money, CASE WHEN BPO.BgPositionsartID IN (32011, 32015) THEN 0 ELSE - BPO.Reduktion END),
  ELSE_Total      = CONVERT(money, CASE WHEN BPO.BgPositionsartID IN (32011, 32015) THEN 0 ELSE BPO.Betrag - BPO.Reduktion END),

  GBI_Hg                  = dbo.fnShGrundbedarfI_Hg(BBG.BgFinanzplanID),
  GBIZuschlag_Hg          = dbo.fnShGrundbedarfIZuschlag_Hg(BBG.BgFinanzplanID),
  GBII_Einzel             = dbo.fnShGrundbedarfII(BBG.BgFinanzplanID, 0),
  GBII_Gemeinschaft       = dbo.fnShGrundbedarfII(BBG.BgFinanzplanID, 1),
  GBII_Hg_Einzel          = dbo.fnShGrundbedarfII_Hg(BBG.BgFinanzplanID, 0),
  GBII_Hg_Gemeinschaft    = dbo.fnShGrundbedarfII_Hg(BBG.BgFinanzplanID, 1),
  GBII_Hg                 = dbo.fnShGrundbedarfII_Hg(BBG.BgFinanzplanID, CASE WHEN GB2.BgPositionsartID = 32014 THEN 0 ELSE 1 END)
FROM BgBudget              BBG
  LEFT  JOIN BgPosition    BPO ON BBG.BgBudgetID = BPO.BgBudgetID AND BPO.BgPositionsartID IN (SELECT Code FROM XLOVCode WHERE LOVName = CONVERT(varchar(100), {1}))
  LEFT  JOIN BgPosition    GBZ ON GBZ.BgBudgetID = BPO.BgBudgetID AND GBZ.BgPositionsartID = 32012
  LEFT  JOIN BgPosition    GB2 ON GB2.BgBudgetID = BPO.BgBudgetID AND GB2.BgPositionsartID IN (32013, 32014)
WHERE BPO.BgBudgetID = {0}
 AND (BPO.DatumVon IS NULL OR ISNULL(BPO.FPPosition,0) = 1)</value>
