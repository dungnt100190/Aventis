  <data name="qryMonatsbudget.SelectStatement" xml:space="preserve">
    <value>SELECT Status       = CASE WHEN BDG.BgBewilligungStatusCode = 5 THEN 2
                      ELSE CASE BPO.BgBewilligungStatusCode
                           WHEN 1 THEN 1   -- grau
                           WHEN 2 THEN 15  -- abgelehnt
                           WHEN 3 THEN 12  -- angefragt
                           WHEN 5 THEN 14  -- bewilligt
                           WHEN 9 THEN 7   -- gesperrt
                           END
                      END,
       Jahr         = BDG.Jahr,
       Monat        = dbo.fnXMonat(BDG.Monat),
       KOA          = BKA.KontoNr,
       Buchungstext = BPO.Buchungstext,
       Person       = PRS.NameVorname,
       Betrag       = BPO.Betrag
FROM   BgPosition BPO
       INNER JOIN BgBudget        BDG ON BDG.BgBudgetID = BPO.BgBudgetID
       INNER JOIN BgPositionsart  BPA ON BPA.BgPositionsartID = BPO.BgPositionsartID
       INNER JOIN BgKostenart     BKA ON BKA.BgKostenartID = BPA.BgKostenartID
       LEFT  JOIN vwPerson        PRS ON PRS.BaPersonID = BPO.BaPersonID
       LEFT  JOIN vwInstitution   INS ON INS.BaInstitutionID = BPO.BaInstitutionID
       LEFT  JOIN XLOVCode        GES ON GES.LOVName = 'BaGeschlecht' AND GES.Code = PRS.GeschlechtCode
WHERE BPO.BgPositionID_CopyOf = {0} AND
      BDG.Masterbudget = 0
ORDER BY BDG.Jahr, BDG.Monat</value>
