  <data name="qryBgPosition.SelectStatement" xml:space="preserve">
    <value>SELECT BPO.*,
       KOA                = BKA.KontoNr,
       Kostenart          = BKA.KontoNr + ' ' + BPA.Name,
       Institution        = INS.Name,
       Adresse            = INS.AdresseMehrzeilig,
       F                  = CASE WHEN BPO.FPPosition = 1 THEN 'LE' END,
       FaLeistungID       = LEI.FaLeistungID,
       FaFallID           = LEI.FaFallID,
       LeistungBaPersonID = LEI.BaPersonID,
       FallBaPersonID     = FAL.BaPersonID,
       Gruppe             = GRP.Text,
       ProPerson          = ISNULL(BPA.ProPerson,CONVERT(BIT,0)),
       ProUE              = ISNULL(BPA.ProUE,CONVERT(BIT,0))
FROM   BgPosition BPO
       INNER JOIN BgPositionsart  BPA ON BPA.BgPositionsartID = BPO.BgPositionsartID
       INNER JOIN BgKostenart     BKA ON BKA.BgKostenartID = BPA.BgKostenartID
       LEFT  JOIN vwPerson        PRS ON PRS.BaPersonID = BPO.BaPersonID
       LEFT  JOIN vwInstitution   INS ON INS.BaInstitutionID = BPO.BaInstitutionID
       LEFT  JOIN XLOVCode        GES ON GES.LOVName = 'BaGeschlecht' AND GES.Code = PRS.GeschlechtCode
       LEFT  JOIN XLOVCode        GRP ON GRP.LOVName = 'BgGruppe' AND GRP.Code = BPA.BgGruppeCode
       INNER JOIN BgBudget        BDG ON BDG.BgBudgetID = BPO.BgBudgetID
       INNER JOIN BgFinanzplan    FPL ON FPL.BgFinanzplanID = BDG.BgFinanzplanID
       INNER JOIN FaLeistung      LEI ON LEI.FaLeistungID = FPL.FaLeistungID
       INNER JOIN FaFall          FAL ON FAL.FaFallID = LEI.FaFallID
WHERE BPO.BgBudgetID = {0} AND BPA.BgGruppeCode = {1} AND
      (ISNULL(BPO.FPPosition,0) = 0 OR BPO.Betrag &lt;&gt; 0)
ORDER BY BKA.KontoNr, PRS.NameVorname, BPO.DatumVon</value>
  </data>
