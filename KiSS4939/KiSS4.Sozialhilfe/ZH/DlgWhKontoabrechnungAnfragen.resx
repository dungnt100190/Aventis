  <data name="qryAnfrage.SelectStatement" xml:space="preserve">
    <value>SELECT
 Stellenleiter = USR_AN.NameVorname,
 ABR.WhAbrechnungID,
 ABR.Bemerkung,
 ABR.WhAbrechnungTS,
 ABR.UserID_AnfrageVon,
 ABR.UserID_AnfrageAn,
 ABR.WhAbrechnungTS,
 ABR.DatumAnfrage,
 ABR.WhAbrechnungVisumCode
FROM WhAbrechnung ABR
  LEFT JOIN VwUser USR_AN ON USR_AN.UserID = ABR.UserID_AnfrageAn
WHERE WhAbrechnungID = {0}</value>
  </data>
  <metadata name="qryStellenleiter.TrayLocation" type="System.Drawing.Point, System.Drawing, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a">
