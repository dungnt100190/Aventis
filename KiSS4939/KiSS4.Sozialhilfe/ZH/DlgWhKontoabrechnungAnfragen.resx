  <data name="qryStellenleiter.SelectStatement" xml:space="preserve">
    <value>SELECT
  UserID_An = T.ID,
  Text = T.LogonName + ' - ' + T.Name + ' (' + T.Funktion + ' ' + OE + ')',
  T.OE,
  T.Funktion,
  T.Name,
  istDirekterStellenleiter = CASE WHEN T.OrgUnitID = USR.OrgUnitID AND T.Funktion = 'SL' THEN 1 ELSE 0 END
FROM vwUser USR
  INNER JOIN 
  (
    SELECT DISTINCT Funktion = 'SL', OE = ORG.ItemName, ID = STL.UserID, Name = STL.NameVorname,
      STL.LogonName, STL.Sozialzentrum, ORG.OrgUnitID
    FROM dbo.XOrgUnit ORG
      INNER JOIN dbo.vwUser STL ON STL.UserID = ORG.ChiefID
    UNION 
    SELECT DISTINCT 'SL Stv', ORG.ItemName, STV.UserID, Name = STV.NameVorname,
      STV.LogonName, STV.Sozialzentrum, ORG.OrgUnitID
    FROM dbo.XOrgUnit ORG
      INNER JOIN dbo.vwUser STV ON STV.UserID = ORG.StellvertreterID
  ) T ON T.Sozialzentrum = USR.Sozialzentrum
WHERE USR.UserID = {0}
ORDER BY T.OE, T.Funktion</value>
