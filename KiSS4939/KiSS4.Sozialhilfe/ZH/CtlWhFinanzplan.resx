    <value>SELECT Code,
       Text,
       SortKey,
       IsActive = CASE WHEN EXISTS(SELECT TOP 1 1 
                                   FROM BgPositionsart 
                                   WHERE BgKategorieCode = 2 
                                     AND BgPositionsartID = Code) THEN 1 
                       ELSE 0 
                  END
FROM dbo.XLOVCode 
