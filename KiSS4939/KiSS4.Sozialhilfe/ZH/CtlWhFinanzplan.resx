  <data name="qryIstBewilligt.SelectStatement" xml:space="preserve">
    <value>BEGIN
DECLARE @DatumBewilligt datetime
DECLARE @DatumAnnuliert datetime

-- Wann wurde der Finanzplan bewilligt
SELECT Top 1 @DatumBewilligt = BBW.Datum
FROM   BgBewilligung BBW
WHERE  BBW.BgFinanzplanID = {0}
AND BBW.BgBudgetID IS NULL  
AND BBW.BgBewilligungCode = 2
ORDER BY BBW.Datum desc

-- Wann wurde der Finanzplan annuliert
SELECT Top 1 @DatumAnnuliert= BBW.Datum
FROM   BgBewilligung BBW
WHERE  BBW.BgFinanzplanID = {0}
AND BBW.BgBudgetID IS NULL  
AND BBW.BgBewilligungCode = 9
ORDER BY BBW.Datum desc

-- Wenn der Finanzplan noch nie annuliert wurde, dass Datum auf MinDate setzten
if (@DatumAnnuliert is null) Set @DatumAnnuliert = ''

-- Falls der Finanzplan bewilligt wurde und nicht wieder annuliert, dann wird das Datum der Bewilligung zurückgegeben
SELECT @DatumBewilligt 
where (@DatumBewilligt &gt; @DatumAnnuliert)

END</value>
  </data>
