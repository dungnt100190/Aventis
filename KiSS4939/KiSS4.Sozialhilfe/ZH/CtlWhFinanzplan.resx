  <data name="qryBgFinanzplan.SelectStatement" xml:space="preserve">
    <value>SELECT SFP.*,
  FinanzPlanVon = IsNull(SFP.DatumVon, SFP.GeplantVon),
  FinanzPlanBis = IsNull(SFP.DatumBis, SFP.GeplantBis),
  ShStatusText  = dbo.fnLOVText('BgBewilligungStatus', SFP.BgBewilligungStatusCode),
  FallDatumVon  = FAL.DatumVon,
  FallDatumBis  = FAL.DatumBis,
  USR.LogonName,
  FaFallID$     = FAL.FaFallID,
  UserID$       = FAL.UserID
FROM BgFinanzplan        SFP
  INNER JOIN FaLeistung  FAL ON FAL.FaLeistungID = SFP.FaLeistungID
  LEFT  JOIN XUser       USR ON USR.UserID = FAL.UserID
WHERE BgFinanzplanID = {0}</value>
