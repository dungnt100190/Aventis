  <data name="qryKbBuchung.SelectStatement" xml:space="preserve">
    <value>DECLARE @OrgGruppeID int
SET @OrgGruppeID = NULL
--- SET @OrgGruppeID = {ctlOrgUnitTeamUser.SucheGruppe}

DECLARE @TeamID int
SET @TeamID = NULL
--- SET @TeamID = {ctlOrgUnitTeamUser.SucheTeam}

SELECT DISTINCT
       BUC.*,
       LEI.BaPersonID,
       LEI.FaFallID, 
       FPL.BgFinanzplanID,
       PRS.Geburtsdatum, 
       PRS.AHVNummer,
       Klient      = PRS.NameVorname + char(13) + char(10) + PRS.WohnsitzMehrzeilig,
       KlientLinie = PRS.NameVorname,
       MA          = USR.Displaytext,
       MADruck     = DUSR.DisplayText,
       ZahlbarAn   = IsNull(BUC.BeguenstigtName + CHAR(13) + CHAR(10),'') +
                     IsNull(BUC.BeguenstigtName2 + char(13) + char(10),'') +
                     IsNull(BUC.BeguenstigtPostfach + char(13) + char(10), '') +
                     IsNull(BUC.BeguenstigtStrasse + IsNull(' ' + BUC.BeguenstigtHausNr, '') + char(13) + char(10), '') +
                     IsNull(IsNull(BUC.BeguenstigtPLZ + ' ', '') + IsNull(BUC.BeguenstigtOrt, ''),'')
FROM   FaLeistung LEI
       inner join vwPerson                PRS  ON PRS.BaPersonID = LEI.BaPersonID
       inner join BgFinanzplan            FPL  ON FPL.FaLeistungID = LEI.FaLeistungID 
       inner join BgFinanzplan_BaPerson   FPP  ON FPP.BgFinanzplanID = FPL.BgFinanzplanID AND FPP.IstUnterstuetzt = 1
       inner join BgBudget                BGG  ON BGG.BgFinanzplanID = FPL.BgFinanzplanID
       inner join KbBuchung               BUC  ON BUC.BgBudgetID = BGG.BgBudgetID
       inner join vwUser                  USR  ON USR.UserID = LEI.UserId
       left  join vwUser                  DUSR ON DUSR.UserID = BUC.BarbelegUserID
WHERE ((@OrgGruppeID IS NULL AND @TeamID IS NULL) OR USR.OrgUnitID IN (SELECT OrgUnitID FROM dbo.fnOrgUnitsOfTeam(@OrgGruppeID,@TeamID)))
AND LEI.ModulID = 3 
AND BUC.KbAuszahlungsartCode = 103
AND BUC.KbBuchungStatusCode &lt;&gt; 8	-- stornierte Barbelege dürfen nicht dargestellt werden
--- AND LEI.UserID = {ctlOrgUnitTeamUser.SucheUserID.LookupID}
--- AND FPP.BaPersonID = {edtSucheKlient.LookupID}
--- AND BUC.ValutaDatum &gt;= {edtSucheValutaVon}
--- AND BUC.ValutaDatum &lt;= {edtSucheValutaBis}
--- AND LEI.FaFallID = {edtSucheFaFallID}
--- AND BUC.KbBuchungStatusCode = {edtSucheStatus}
--- AND (BUC.Betrag is not null and BUC.Betrag &lt;&gt; 0)

ORDER BY Klient, BUC.ValutaDatum</value>
  </data>
  <data name="edtSucheKlient.LookupSQL" xml:space="preserve">
    <value>select distinct
       [P-Nr.] = PRS.BaPersonID, 
       [unterstützte Person] = PRS.NameVorname,
       Adresse = PRS.Wohnsitz
from   BgFinanzplan_BaPerson FPP 
       inner join vwPerson PRS on PRS.BaPersonID = FPP.BaPersonID
where  PRS.NameVorname like '%' + {0} + '%' and
       FPP.IstUnterstuetzt = 1
order by PRS.NameVorname</value>
  </data>
