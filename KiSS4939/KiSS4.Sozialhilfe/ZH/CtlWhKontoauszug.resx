  <data name="qryBgDokumente.SelectStatement" xml:space="preserve">
    <value>DECLARE @KbBuchungBruttoID INT;
DECLARE @BaPersonID INT;
SET @KbBuchungBruttoID = {0};
SET @BaPersonID = {1};

;WITH DOC_CTE AS
(
  -- Budget-Position
  SELECT DISTINCT BDO.*, DOC.DateLastSave, DOC.DateCreation, POS.BaPersonID 
  FROM KbBuchungBruttoPerson BUP
    INNER JOIN BgPosition    POS ON POS.BgPositionID = BUP.BgPositionID
    INNER JOIN BgDokument    BDO ON BDO.BgPositionID = BUP.BgPositionID
    LEFT  JOIN XDocument     DOC ON DOC.DocumentID = BDO.DocumentID
  WHERE BUP.KbBuchungBruttoID = dbo.fnGetOriginalKbBuchungBruttoID(@KbBuchungBruttoID)
  UNION
  -- Monatsbudget
  SELECT DISTINCT BDO.*, DOC.DateLastSave, DOC.DateCreation, BaPersonID = NULL
  FROM KbBuchungBruttoPerson BUP
    INNER JOIN BgPosition    POS ON POS.BgPositionID = BUP.BgPositionID
    INNER JOIN BgDokument    BDO ON BDO.BgBudgetID = POS.BgBudgetID
    LEFT  JOIN XDocument     DOC ON DOC.DocumentID = BDO.DocumentID
  WHERE BUP.KbBuchungBruttoID = @KbBuchungBruttoID
  UNION
  -- Finanzplan
  SELECT DISTINCT BDO.*, DOC.DateLastSave, DOC.DateCreation, BaPersonID = NULL
  FROM KbBuchungBruttoPerson BUP
    INNER JOIN BgPosition    POS ON POS.BgPositionID = BUP.BgPositionID
    INNER JOIN BgBudget      BDG ON BDG.BgBudgetID = POS.BgBudgetID
    INNER JOIN BgDokument    BDO ON BDO.BgFinanzplanID = BDG.BgFinanzplanID
    LEFT  JOIN XDocument     DOC ON DOC.DocumentID = BDO.DocumentID
  WHERE BUP.KbBuchungBruttoID = @KbBuchungBruttoID
  UNION
  -- Masterbudget-Position
  SELECT DISTINCT BDO.*, DOC.DateLastSave, DOC.DateCreation, POM.BaPersonID 
  FROM KbBuchungBruttoPerson BUP
    INNER JOIN BgPosition    POS ON POS.BgPositionID = BUP.BgPositionID
    INNER JOIN BgPosition    POM ON POM.BgPositionID = POS.BgPositionID_CopyOf
    INNER JOIN BgDokument    BDO ON BDO.BgPositionID = POM.BgPositionID
    LEFT  JOIN XDocument     DOC ON DOC.DocumentID = BDO.DocumentID
  WHERE BUP.KbBuchungBruttoID = @KbBuchungBruttoID
    AND BDO.BgDokumentTypCode = 4 -- Masterbudgetposition
 
)

SELECT * 
FROM DOC_CTE 
WHERE ISNULL(BaPersonID, @BaPersonID) = @BaPersonID OR @BaPersonID IS NULL
ORDER BY BgDokumentTypCode, Stichwort;

</value>
