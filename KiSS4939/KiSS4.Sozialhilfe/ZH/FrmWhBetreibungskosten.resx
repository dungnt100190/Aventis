  <data name="qryKbDokumente.SelectStatement" xml:space="preserve">
    <value>SELECT KBD.*, DOC.*
FROM   KbBuchung BUC
       INNER JOIN KbDokument KBD ON KBD.KbBuchungID = BUC.KbBuchungID
       LEFT JOIN XDocument   DOC ON DOC.DocumentID = KBD.DocumentID
WHERE  BUC.KbBuchungID = {0}
ORDER BY Stichwort</value>
  </data>
