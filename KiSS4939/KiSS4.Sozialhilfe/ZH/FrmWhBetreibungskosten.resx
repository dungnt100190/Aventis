  <data name="qryValuta.SelectStatement" xml:space="preserve">
    <value>SELECT TOP 1
  Datum =
    CASE WHEN ZLV.Datum1 &gt; GetDate() THEN ZLV.Datum1
         WHEN ZLV.Datum2 &gt; GetDate() THEN ZLV.Datum2
         WHEN ZLV.Datum3 &gt; GetDate() THEN ZLV.Datum3
         WHEN ZLV.Datum4 &gt; GetDate() THEN ZLV.Datum4
         WHEN ZLV.Datum5 &gt; GetDate() THEN ZLV.Datum5
         WHEN ZLV.Datum6 &gt; GetDate() THEN ZLV.Datum6
         WHEN ZLV.Datum7 &gt; GetDate() THEN ZLV.Datum7
         WHEN ZLV.Datum8 &gt; GetDate() THEN ZLV.Datum8
         WHEN ZLV.Datum9 &gt; GetDate() THEN ZLV.Datum9
         WHEN ZLV.Datum10 &gt; GetDate() THEN ZLV.Datum10
    END
FROM dbo.IkZahlungslaufValuta ZLV WITH(READUNCOMMITTED)
WHERE
 ZLV.Datum1 &gt; GetDate() OR ZLV.Datum2 &gt; GetDate() OR ZLV.Datum3 &gt; GetDate() OR ZLV.Datum4 &gt; GetDate() OR
 ZLV.Datum5 &gt; GetDate() OR ZLV.Datum6 &gt; GetDate() OR ZLV.Datum7 &gt; GetDate() OR ZLV.Datum8 &gt; GetDate() OR
 ZLV.Datum9 &gt; GetDate() OR ZLV.Datum10 &gt; GetDate()
ORDER BY ZLV.Datum1 ASC</value>
  </data>
