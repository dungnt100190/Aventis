  <data name="qryKbBuchung.SelectStatement" xml:space="preserve">
    <value>SELECT BUC.*,
       BUK.BgKostenartID,
       BUK.BaPersonID,
       BUK.KontoNr,
       BUK.PositionImBeleg,
       BUK.Hauptvorgang,
       BUK.Teilvorgang,
       BUK.Belegart,
       BUK.VerwPeriodeVon,
       BUK.VerwPeriodeBis,
       Doc                   = (SELECT CASE WHEN COUNT(*) &gt; 0 THEN COUNT(*) END FROM KbDokument WHERE KbBuchungID = BUC.KbBuchungID),
       KOA                   = BKA.KontoNr,
       Kreditor              = KRE.Kreditor,
       ZusatzInfo            = KRE.ZusatzInfo,
       EinzahlungsscheinCode = KRE.EinzahlungsscheinCode,
       Schuldner             = PRS.NameVorname,
       Adresse               = PRS.Wohnsitz,
       MA                    = USR.LogonName,
       FallBaPersonID        = FAL.BaPersonID,
       FaFallID              = LEI.FaFallID,
       FaProzessCode         = LEI.FaProzessCode,
       BgSplittingartCode    = BKA.BgSplittingartCode,
       Kostenart             = BKA.KontoNr + ' ' + BKA.Name,
       Leistung              = PRO.Text + isnull(', VG ' + convert(varchar,LEI.PscdVertragsgegenstandID),''),
       Sel                   = CONVERT(bit,CASE WHEN BUC.KbBuchungStatusCode in (2,5) then 0 end)
FROM   KbBuchung BUC
       INNER JOIN KbBuchungKostenart       BUK  ON BUK.KbBuchungID = BUC.KbBuchungID
       INNER JOIN FaLeistung               LEI  ON LEI.FaLeistungID = BUC.FaLeistungID
       INNER JOIN FaFall                   FAL  ON FAL.FaFallID = LEI.FaFallID
       LEFT  JOIN vwPerson                 PRS  ON PRS.BaPersonID = LEI.BaPersonID
       LEFT  JOIN BgKostenart              BKA  ON BKA.BgKostenartID = BUK.BgKostenartID
       LEFT  JOIN vwKreditor               KRE  ON KRE.BaZahlungswegID = BUC.BaZahlungswegID
       LEFT  JOIN XLOVCode                 AZA  ON AZA.LOVName = 'KbAuszahlungsart' and AZA.Code = BUC.KbAuszahlungsArtCode
       LEFT  JOIN XLOVCode                 PRO on PRO.LOVName = 'FaProzess' AND PRO.Code = LEI.FaProzessCode
       LEFT  JOIN XUser                    USR  ON USR.UserID = BUC.ErstelltUserID 
where 1 = 1 
--- and (({edtSucheModulW} = 1 and LEI.FaProzessCode in (301,302,304) and BKA.KontoNr in ('365','366')) OR
---      ({edtSucheModulA} = 1 and LEI.FaProzessCode between 405 and 410 and BKA.KontoNr = '612'))
--- and BUC.ValutaDatum &gt;= {edtSucheValutaVon}
--- and BUC.ValutaDatum &lt;= {edtSucheValutaBis}
--- and BUC.KbBuchungID = {edtSucheBelegNr}
--- and BUC.BelegNr = {edtSuchePSCDBelegNr}
--- and BUC.KbBuchungStatusCode = {edtSucheStatus}
--- and LEI.FaFallID = {edtSucheFallNr}
--- and LEI.BaPersonID = {edtSucheSchuldner.LookupID}
--- and BUC.ErstelltUserID = {edtSucheErfassungMA.LookupID}
--- and BUC.ErstelltDatum &gt;= {edtSucheErfassungVon}
--- and BUC.ErstelltDatum &lt;= dateadd(ms,-10,dateadd(d,1,{edtSucheErfassungBis}))  
--- and BUC.MutiertUserID = {edtSucheMutationMA.LookupID}
--- and BUC.MutiertDatum &gt;= {edtSucheMutationVon}
--- and BUC.MutiertDatum &lt;= dateadd(ms,-10,dateadd(d,1,{edtSucheMutationBis}))  


order by BUC.ValutaDatum, Schuldner</value>
  </data>
