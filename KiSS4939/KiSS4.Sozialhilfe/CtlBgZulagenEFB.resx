  <data name="qryBgPosition.SelectStatement" xml:space="preserve">
    <value>SELECT BPO.*,
  BetragSum         = CASE WHEN BPO.BgKategorieCode = 4 THEN -BPO.Betrag ELSE BPO.Betrag END,
  BgGruppeCode      = CASE              WHEN SPT.BgGruppeCode IN (39110, 39130,        39910) THEN SPT.BgGruppeCode - 10 ELSE SPT.BgGruppeCode END,
  Alleinerziehend   = CONVERT(bit, CASE WHEN SPT.BgGruppeCode IN (39110, 39130, 39210, 39910) THEN 1 ELSE 0 END),
  Anteil            = CASE WHEN SPT.BgPositionsartCode % 10 BETWEEN 1 AND 9 THEN SPT.BgPositionsartCode % 10 ELSE NULL END,
  BaPersonID_NEW   = UST.BaPersonID, PRS.Geburtsdatum,
  PRS.NameVorname
FROM (SELECT 
         BPO.*
      FROM BgPosition            BPO
       INNER JOIN BgPositionsart  BPT ON BPT.BgPositionsartID = BPO.BgPositionsartID
      WHERE BPO.BgBudgetID = {0} AND BaPersonID IS NOT NULL
          AND IsNull(BPO.DatumVon, '19000101') &lt;&gt; '19000102'                      -- Im Masterbudget erstellte/veränderte Position
          AND NOT (BPO.DatumVon IS NOT NULL AND BPO.BgPositionID_CopyOf IS NULL)  -- Im Monatsbudget erstellte Position
          AND ({1} = 0 OR GETDATE() BETWEEN ISNULL(BPO.DatumVon,dbo.fnDateSerial(1753, 01, 01)) AND ISNULL(BPO.DatumBis,dbo.fnDateSerial(9999, 12, 31))
                  OR BPO.DatumBis = '19000101') -- Finanzplan-Position, welche im Masterbudget verändert wurde --&gt; ursprüngliche Position soll dargestellt werden
          AND IsNull(BPO.DatumVon, '17530101') &lt; IsNull(BPO.DatumBis, '99991231') -- Wird die gleiche Position mehrmals per 01.01.2011 editiert, resultieren mehrere Positionen mit DatumVon=01.01.2011, DatumBis=31.12.2010. Solche Positionen einfach filtern.
          AND BgGruppeCode BETWEEN 39000 AND 39999) BPO
  FULL  JOIN (SELECT SPP.BaPersonID
              FROM BgBudget                       BBG
                INNER JOIN BgFinanzplan           SFP ON SFP.BgFinanzplanID = BBG.BgFinanzplanID
                INNER JOIN BgFinanzplan_BaPerson SPP ON SPP.BgFinanzplanID = SFP.BgFinanzplanID
                INNER JOIN BaPerson              PRS ON PRS.BaPersonID    = SPP.BaPersonID
              WHERE SPP.IstUnterstuetzt = 1 AND dbo.fnGetAge(PRS.Geburtsdatum, IsNull(SFP.DatumVon, SFP.GeplantVon)) &gt;= 15
                AND BBG.BgBudgetID = {0})  UST ON UST.BaPersonID = BPO.BaPersonID
  INNER JOIN vwPerson      PRS ON PRS.BaPersonID = IsNull(UST.BaPersonID, BPO.BaPersonID)
  LEFT  JOIN WhPositionsart  SPT ON SPT.BgPositionsartID = BPO.BgPositionsartID
ORDER BY PRS.Name, PRS.Vorname</value>
