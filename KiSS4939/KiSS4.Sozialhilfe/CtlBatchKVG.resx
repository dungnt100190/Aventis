  <data name="qryAuszahlweg.SelectStatement" xml:space="preserve">
    <value>SELECT KbAuszahlungsartCode  = BAP.KbAuszahlungsArtCode,
       BgKategorieCode       = BPO.BgKategorieCode,
       Valuta                = BAT.Datum,
       Rechnungdatum         = BPO.RechnungDatum,
       BaZahlungswegID       = BAP.BaZahlungswegID,
       Kreditor              = KRE.Kreditor,
       ZusatzInfo            = KRE.ZusatzInfo,
       EinzahlungsscheinCode = KRE.EinzahlungsscheinCode,
       ReferenzNummer        = BAP.ReferenzNummer,
       Totalbetrag           = (SELECT SUM(SUB.Betrag)
                                FROM dbo.BgPosition SUB
                                WHERE SUB.BgPositionID = BPO.BgPositionID OR
                                      SUB.BgPositionID_Parent = BPO.BgPositionID),
       Restbetrag            = CONVERT(MONEY, 0.0),	-- must be 0 due to validation
       MitteilungZeile1      = BAP.MitteilungZeile1,
       MitteilungZeile2      = BAP.MitteilungZeile2,
       MitteilungZeile3      = BAP.MitteilungZeile3,
       Buchungstext          = BPO.Buchungstext,
       VerwPeriodeVon        = BPO.VerwPeriodeVon,
       VerwPeriodeBis        = BPO.VerwPeriodeBis,
       BgSplittingArtCode    = {1},
       BelegID               = BPO.BgPositionID,
       BelegNr               = KBB.BelegNr,
       BelegDatum            = KBB.BelegDatum,
       ErstelltUserID        = BPO.ErstelltUserID,
       ErstelltDatum         = BPO.ErstelltDatum,
       MutiertUserID         = BPO.MutiertUserID,
       MutiertDatum          = BPO.MutiertDatum
FROM dbo.BgPosition                       BPO
  INNER JOIN dbo.BgAuszahlungPerson       BAP WITH (READUNCOMMITTED) ON BAP.BgPositionID = BPO.BgPositionID
  LEFT JOIN  dbo.BgAuszahlungPersonTermin BAT WITH (READUNCOMMITTED) ON BAT.BgAuszahlungPersonID = BAP.BgAuszahlungPersonID
  LEFT JOIN  dbo.vwKreditor               KRE WITH (READUNCOMMITTED) ON KRE.BaZahlungswegID = BAP.BaZahlungswegID
  
  LEFT JOIN  dbo.KbBuchungKostenart       KBK                        ON KBK.BgPositionID = BPO.BgPositionID
  LEFT JOIN  dbo.KbBuchung                KBB                        ON KBB.KbBuchungID = KBK.KbBuchungID
WHERE BPO.BgPositionID = {0};</value>
