  <data name="qryFall.SelectStatement" xml:space="preserve">
    <value>-- get data
SELECT DISTINCT 
  FAL.FaLeistungID, 
  BBG.BgFinanzplanID,
  Person        = PRS.NameVorname + ' (' + CONVERT(VARCHAR, PRS.BaPersonID) + ')',
  Adresse       = ISNULL(PRS.WohnsitzStrasseHausNr + CHAR(13) + CHAR(10), '')
                + ISNULL(PRS.WohnsitzPLZOrt, '') + ISNULL(' ' + PRS.WohnsitzKanton, ''),
  SAR           = USR.LastName + ISNULL(', ' + USR.FirstName, '') + ' (' + USR.LogonName + ')',
  /*
  BetragKVG     = ISNULL((SELECT SUM(POS.Betrag - POS.Reduktion) 
                          FROM dbo.BgPosition POS WITH (READUNCOMMITTED)
                          INNER JOIN dbo.BgPositionsart POA WITH (READUNCOMMITTED) ON POA.BgPositionsartID = POS.BgPositionsartID
                          WHERE POS.BgBudgetID = BBG.BgBudgetID AND 
                                (POA.BgPositionsartCode IN (32020, 32023, 32024) OR POA.BgPositionsartCode BETWEEN 32121 AND 32130)) , $0), -- KVG+KVGMax
  BetragVVG     = ISNULL((SELECT SUM(POS.Betrag - POS.Reduktion) 
                          FROM dbo.BgPosition POS WITH (READUNCOMMITTED)
                          INNER JOIN dbo.BgPositionsart POA WITH (READUNCOMMITTED) ON POA.BgPositionsartID = POS.BgPositionsartID
                          WHERE POS.BgBudgetID = BBG.BgBudgetID AND 
                                POA.BgPositionsartCode = 32021) , $0),
  LetzteZahlung = (SELECT MAX(BPT.Datum)
                   FROM dbo.BgFinanzplan BFP2 WITH (READUNCOMMITTED)
                     INNER JOIN dbo.BgBudget                 BBG2 WITH (READUNCOMMITTED) ON BBG2.BgFinanzplanID = BFP2.BgFinanzplanID
                     INNER JOIN dbo.BgPosition               BPO  WITH (READUNCOMMITTED) ON BPO.BgBudgetID = BBG2.BgBudgetID
                     INNER JOIN dbo.BgPositionsart					 POA  WITH (READUNCOMMITTED) ON POA.BgPositionsartID = BPO.BgPositionsartID
                     INNER JOIN dbo.BgAuszahlungPerson       BAP  WITH (READUNCOMMITTED) ON BAP.BgPositionID = BPO.BgPositionID
                     INNER JOIN dbo.BgAuszahlungPersonTermin BPT  WITH (READUNCOMMITTED) ON BPT.BgAuszahlungPersonID = BAP.BgAuszahlungPersonID
                   WHERE BFP2.FaLeistungID = FAL.FaLeistungID AND 
                         (POA.BgPositionsartCode IN (32020, 32023, 32024, 32021) OR POA.BgPositionsartCode BETWEEN 32121 AND 32130)), -- KVG+KVGMax+VVG
  */
  BaZahlungswegID = CONVERT(INT, NULL),
  Betrag          = CONVERT(MONEY, NULL),
  ValutaDatum     = CONVERT(DATETIME, NULL),
  ReferenzNummer  = CONVERT(VARCHAR(50), NULL),
  FAL.BaPersonID,
  FAL.FaFallID,
  FAL.Modifier,
  FAL.Modified
FROM dbo.FaLeistung                    FAL WITH (READUNCOMMITTED)
  INNER JOIN (SELECT BFP.FaLeistungID, BFP.BgFinanzplanID, BBG.BgBudgetID
              FROM dbo.BgFinanzplan BFP WITH (READUNCOMMITTED)
                INNER JOIN dbo.BgBudget BBG WITH (READUNCOMMITTED) ON BBG.BgFinanzplanID = BFP.BgFinanzplanID
              WHERE BFP.BgBewilligungStatusCode &gt;= 5 AND -- bewilligt oder gesperrt
                    BBG.MasterBudget = 0 AND -- darf kein masterbudget sein
                    NOT EXISTS (SELECT 1
                                FROM dbo.BgFinanzplan BFP2 WITH (READUNCOMMITTED)
                                  INNER JOIN dbo.BgBudget BBG2 WITH (READUNCOMMITTED) ON BBG2.BgFinanzplanID = BFP2.BgFinanzplanID
                                WHERE BFP2.FaLeistungID = BFP.FaLeistungID AND 
                                      BFP2.BgBewilligungStatusCode &gt;= 5 AND 
                                      BBG2.MasterBudget = 0 AND 
                                      (BBG2.Jahr &gt; BBG.Jahr OR (BBG2.Jahr = BBG.Jahr AND BBG2.Monat &gt; BBG.Monat))
                               )
				     ) BBG ON BBG.FaLeistungID = FAL.FaLeistungID
  INNER JOIN dbo.BgFinanzplan_BaPerson BFP  WITH (READUNCOMMITTED) ON BFP.BgFinanzplanID = BBG.BgFinanzplanID 
                                                                  AND BFP.IstUnterstuetzt = 1
  INNER JOIN dbo.vwPerson              PRS  WITH (READUNCOMMITTED) ON PRS.BaPersonID = FAL.BaPersonID
  INNER JOIN dbo.vwPerson              PRS2 WITH (READUNCOMMITTED) ON PRS2.BaPersonID = BFP.BaPersonID
  LEFT  JOIN dbo.XUser                 USR  WITH (READUNCOMMITTED) ON USR.UserID = FAL.UserID
WHERE FAL.DatumBis IS NULL -- see Mantis: 3725
--- AND PRS.BaPersonID = (SELECT BaPersonID FROM BgPosition WHERE BgPositionID = {edtSuchePositionID})
--- AND PRS.BaPersonID = {edtSucheFalltraeger.LookupID}
--- AND PRS2.NameVorname LIKE dbo.fnReplaceWildcard({edtSucheName})
--- AND USR.UserID = {edtSucheSAR.LookupID}
ORDER BY Person, FAL.FaLeistungID;</value>
