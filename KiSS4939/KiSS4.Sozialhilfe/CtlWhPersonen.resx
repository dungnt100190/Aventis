  <data name="qryKlientensystem.SelectStatement" xml:space="preserve">
    <value>DECLARE @BaPersonID int
SET @BaPersonID = {1}

DECLARE @tblKlientensystem TABLE (BaPersonID int)

INSERT @tblKlientensystem
  SELECT DISTINCT
    CASE WHEN BaPersonID_1 = @BaPersonID
      THEN BaPersonID_2
      ELSE BaPersonID_1
    END
  FROM BaPerson_Relation
  WHERE BaPersonID_1 = @BaPersonID OR BaPersonID_2 = @BaPersonID
  UNION
  SELECT  @BaPersonID

SELECT
  PRS.BaPersonID,
  PRS.NameVorname,
  PRS.Geburtsdatum,
  [Alter]      = dbo.fnGetAge(PRS.Geburtsdatum, IsNull(SFP.DatumVon, SFP.GeplantVon)),
  Beziehung    = CASE WHEN PRS.BaPersonID = @BaPersonID
                   THEN 'Fallträger/-in'
                   ELSE dbo.fnBaRelation(@BaPersonID, PRS.BaPersonID)
                 END,
  Unterstuetzt = SPP.IstUnterstuetzt
FROM @tblKlientensystem             KLS
  INNER JOIN vwPerson               PRS on PRS.BaPersonID = KLS.BaPersonID
  LEFT  JOIN BgFinanzplan_BaPerson  SPP on SPP.BgFinanzplanID = {0} AND SPP.BaPersonID = KLS.BaPersonID
  LEFT  JOIN BgFinanzplan           SFP on SFP.BgFinanzplanID = {0}
WHERE SPP.BaPersonID IS NULL</value>
  </data>
