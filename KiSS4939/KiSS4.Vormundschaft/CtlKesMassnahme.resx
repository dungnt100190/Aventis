  <data name="qryKesMassnahme.SelectStatement" xml:space="preserve">
    <value>;WITH 
MassnahmenCTE AS
(
  SELECT
    GueltigAb       = COALESCE(MAS.AenderungVom, MAS.UebernahmeVom, MAS.ErrichtungVom),
    GueltigBis      = COALESCE(MAS.Beistandswechsel, MAS.UebertragungVom, MAS.AufhebungVom, MAS.BerichtsgenehmigungVom),
    Artikel_M       = ART_M.Artikel,
    Absatz_M        = ART_M.Absatz,
    Ziffer_M        = ART_M.Ziffer,
    ArtikelText_M   = ART_M.Artikel + ISNULL('.' + ART_M.Absatz, '') + ISNULL('.' + ART_M.Ziffer, '') + ' ' + ART_M.Gesetz,
    Bezeichnung_M   = ART_M.Bezeichnung,
    Artikel_N       = ART_N.Artikel,
    Absatz_N        = ART_N.Absatz,
    Ziffer_N        = ART_N.Ziffer,
    ArtikelText_N   = ART_N.Artikel + ISNULL('.' + ART_N.Absatz, '') + ISNULL('.' + ART_N.Ziffer, '') + ' ' + ART_N.Gesetz,
    Bezeichnung_N   = ART_N.Bezeichnung,
    Platzierung_BaInstitutionName = INP.Name,
    Aufgabenbereich = STUFF((SELECT ', ' + [Text]
                             FROM dbo.XLOVCode LOC WITH (READUNCOMMITTED)
                             WHERE LOC.LOVName = 'KesAufgabenbereichES'
                               AND LOC.Code IN (SELECT SplitValue
                                                FROM dbo.fnSplitStringToValues(MAS.KesAufgabenbereichCodes, ',', 1))
                             ORDER BY SortKey, Code
                             FOR XML PATH('')),
                                 1,
                                 2, 
                                 ''),
    Beistand        = COALESCE(USR.NameVorname, VPM.Name + ISNULL(', ' + VPM.Vorname, ''), INS.NameVorname),
    MAS.KesMassnahmeBSSID,
    MAS.FaLeistungID,
    MAS.UserID,
    MAS.VmPriMaID,
    MAS.BaInstitutionID,
    MAS.Platzierung_BaInstitutionID, 
    MAS.KesMassnahmeTypCode,
    MAS.ErrichtungVom,
    MAS.AenderungVom,
    MAS.UebernahmeVom,
    MAS.Beistandswechsel,
    MAS.UebertragungVom,
    MAS.AufhebungVom,
    MAS.KesBeistandsartCode,
    MAS.KesGefaehrdungsmeldungDurchCode,
    MAS.KesAenderungsgrundCode,
    MAS.PLZ,
    MAS.Ort,
    MAS.Kanton,
    MAS.OrtschaftCode,
    MAS.KesAufhebungsgrundCode,
    MAS.KESBDocumentID,
    MAS.DocumentID_Urkunde,
    MAS.KesArtikelID_MassnahmegebundeneGeschaefte,
    MAS.KesArtikelID_NichtMassnahmegebundeneGeschaefte,
    MAS.KesAufgabenbereichCodes,
    MAS.KesIndikationCodes,
    MAS.Bemerkung,
    MAS.KesElterlicheSorgeObhutCode_ElterlicheSorge,
    MAS.KesElterlicheSorgeObhutCode_Obhut,
    MAS.KesGrundlageCode,
    MAS.FuersorgerischeUnterbringung, 
    MAS.BerichtsgenehmigungVom,	
    MAS.Creator,
    MAS.Created,
    MAS.Modifier,
    MAS.Modified,
    MAS.IsDeleted,
    MAS.KesMassnahmeBSSTS
  FROM dbo.KesMassnahmeBSS      MAS   WITH (READUNCOMMITTED)
    LEFT JOIN dbo.vwUser        USR   ON USR.UserID = MAS.UserID
    LEFT JOIN dbo.VmPriMa       VPM   ON VPM.VmPriMaID = MAS.VmPriMaID
    LEFT JOIN dbo.vwInstitution INS   ON INS.BaInstitutionID = MAS.BaInstitutionID
    LEFT JOIN dbo.vwInstitution INP   ON INP.BaInstitutionID = MAS.Platzierung_BaInstitutionID
    LEFT JOIN dbo.KesArtikel    ART_M WITH (READUNCOMMITTED) ON ART_M.KesArtikelID = MAS.KesArtikelID_MassnahmegebundeneGeschaefte
    LEFT JOIN dbo.KesArtikel    ART_N WITH (READUNCOMMITTED) ON ART_N.KesArtikelID = MAS.KesArtikelID_NichtMassnahmegebundeneGeschaefte
  WHERE MAS.FaLeistungID = {0}
    AND ({1} = 0 --0: alle
         OR ({1} = 1 AND MAS.IsDeleted = 0) --1: nur aktive
         OR ({1} = 2 AND MAS.IsDeleted = 1) --2: nur gelöschte
        )
    AND ({2} IS NULL
         OR MAS.KesArtikelID_MassnahmegebundeneGeschaefte = {2}
         OR MAS.KesArtikelID_NichtMassnahmegebundeneGeschaefte = {2})
        )
SELECT
  GueltigAb,
  GueltigBis,
  Artikel_M,
  Bezeichnung_M,
  ArtikelBezeichnung_M = ArtikelText_M + ' ' + Bezeichnung_M,
  Artikel_N,
  Bezeichnung_N,
  ArtikelBezeichnung_N = ArtikelText_N + ' ' + Bezeichnung_N,
  Artikel     = ISNULL(Artikel_M, Artikel_N),
  Absatz      = ISNULL(Absatz_M, Absatz_N),
  Ziffer      = ISNULL(Ziffer_M, Ziffer_N),
  Bezeichnung = ISNULL(Bezeichnung_M, Bezeichnung_N),
  Aufgabenbereich,
  Beistand,
  KesMassnahmeBSSID,
  FaLeistungID,
  UserID,
  VmPriMaID,
  BaInstitutionID,
  Platzierung_BaInstitutionID, 
  Platzierung_BaInstitutionName,
  KesMassnahmeTypCode,
  ErrichtungVom,
  AenderungVom,
  UebernahmeVom,
  Beistandswechsel,
  UebertragungVom,
  AufhebungVom,
  KesBeistandsartCode,
  KesGefaehrdungsmeldungDurchCode,
  KesAenderungsgrundCode,
  PLZ,
  Ort,
  Kanton,
  OrtschaftCode,
  KesAufhebungsgrundCode,
  KESBDocumentID,
  DocumentID_Urkunde,
  KesArtikelID_MassnahmegebundeneGeschaefte,
  KesArtikelID_NichtMassnahmegebundeneGeschaefte,
  KesAufgabenbereichCodes,
  KesIndikationCodes,
  Bemerkung,
  KesElterlicheSorgeObhutCode_ElterlicheSorge,
  KesElterlicheSorgeObhutCode_Obhut,
  KesGrundlageCode,
  FuersorgerischeUnterbringung, 
  BerichtsgenehmigungVom,
  Creator,
  Created,
  Modifier,
  Modified,
  IsDeleted,
  KesMassnahmeBSSTS,
  SpecialOrder = CASE
                   WHEN GueltigBis IS NULL THEN 1 
                   ELSE 2 
                 END
FROM MassnahmenCTE
ORDER BY
  SpecialOrder,
  GueltigBis DESC,
  GueltigAb DESC;
  
  </value>
  </data>
</root>