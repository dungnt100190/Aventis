  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>DECLARE @OrgGruppeID int
SET @OrgGruppeID = NULL
--- SET @OrgGruppeID = {ctlOrgUnitTeamUser.SucheGruppe}

DECLARE @TeamID int
SET @TeamID = NULL
--- SET @TeamID = {ctlOrgUnitTeamUser.SucheTeam}

SELECT 
  BEW.Datum,
  Typ = 'Budgetbewilligung',
  BEW.Bemerkung,
  BEW.KgBudgetID,
  LEI.FaFallID,
  LEI.FaLeistungID,
  LEI.UserID,
  SA = SAR.LastName + IsNull(', ' + SAR.FirstName, ''), 
  FAL.BaPersonID, PRS.NameVorname,
  BDG.BewilligtVon, BDG.BewilligtBis,
  BDG.KgBewilligungCode,
  Absender   = ABN.LastName + IsNull(', ' + ABN.FirstName, ''),
  Empfaenger = EMP.LastName + IsNull(', ' + EMP.FirstName, ''),
  KgPositionID = convert(int,NULL),
  Betrag       = convert(money,NULL)
FROM KgBewilligung           BEW
  INNER JOIN XUser           ABN ON ABN.UserID       = BEW.UserID
  LEFT  JOIN vwUser          EMP ON EMP.UserID       = BEW.UserID_An
  LEFT  JOIN KgBudget        BDG ON BDG.KgBudgetID   = BEW.KgBudgetID
  LEFT  JOIN FaLeistung      LEI ON LEI.FaLeistungID = BDG.FaLeistungID
  LEFT  JOIN FaFall          FAL ON FAL.FaFallID     = LEI.FaFallID
  LEFT  JOIN XUser           SAR ON SAR.UserID       = LEI.UserID
  LEFT  JOIN vwPerson        PRS ON PRS.BaPersonID   = LEI.BaPersonID
WHERE BDG.KgBewilligungCode = 3
      AND((@OrgGruppeID IS NULL AND @TeamID IS NULL) OR EMP.OrgUnitID IN (SELECT OrgUnitID FROM dbo.fnOrgUnitsOfTeam(@OrgGruppeID,@TeamID)))
--- AND EMP.UserID = {ctlOrgUnitTeamUser.SucheUserID.LookupID}
--- AND SAR.UserID = {edtSucheSAR.LookupID}
--- AND PRS.BaPersonID = {edtSucheKlient.LookupID}
--- AND ABN.UserID = {edtSucheAbsender.LookupID}
UNION ALL


SELECT 
  Datum = POS.BuchungDatum,
  Typ = 'Anpassung Position',
  Bemerkung = POS.Buchungstext,
  BDG.KgBudgetID,
  LEI.FaFallID,
  LEI.FaLeistungID,
  LEI.UserID,
  SA     = SAR.LastName + IsNull(', ' + SAR.FirstName, ''), 
  FAL.BaPersonID, PRS.NameVorname,
  BDG.BewilligtVon, BDG.BewilligtBis,
  BDG.KgBewilligungCode,
  Absender   = ABN.LastName + IsNull(', ' + ABN.FirstName, ''),
  Empfaenger = SAR.LastName + IsNull(', ' + SAR.FirstName, ''),
  KgPositionID = POS.KgPositionID,
  Betrag       = POS.Betrag
FROM KgPosition              POS
  INNER JOIN KgBudget        BDG ON BDG.KgBudgetID   = POS.KgBudgetID
  INNER JOIN FaLeistung      LEI ON LEI.FaLeistungID = BDG.FaLeistungID
  INNER JOIN FaFall          FAL ON FAL.FaFallID     = LEI.FaFallID
  LEFT  JOIN XUser           ABN ON ABN.UserID       = POS.MutiertUserID
  LEFT  JOIN vwUser          SAR ON SAR.UserID       = LEI.UserID
  LEFT  JOIN vwPerson        PRS ON PRS.BaPersonID   = LEI.BaPersonID
WHERE BDG.KgMasterbudgetID IS NULL
  AND BDG.KgBewilligungCode = 5 -- Masterbudget grün
  AND POS.KgBewilligungCode = 3 -- Position blau / angefragt
  AND((@OrgGruppeID IS NULL AND @TeamID IS NULL) OR SAR.OrgUnitID IN (SELECT OrgUnitID FROM dbo.fnOrgUnitsOfTeam(@OrgGruppeID,@TeamID)))
--- AND LEI.UserID = {ctlOrgUnitTeamUser.SucheUserID.LookupID}
--- AND PRS.BaPersonID = {edtSucheKlient.LookupID}
ORDER BY Typ, SA, NameVorname</value>
