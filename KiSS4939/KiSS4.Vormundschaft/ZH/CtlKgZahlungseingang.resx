  <data name="qryZahlungseingang.SelectStatement" xml:space="preserve">
    <value>SELECT ZEI.[KgZahlungseingangID]
      ,ZEI.[Datum]
      ,ZEI.[BaPersonID]
      ,ZEI.[BaInstitutionID]
      ,ZEI.[Debitor]
      ,ZEI.[KgKontoID]
      ,ZEI.[Betrag]
      ,ZEI.[Mitteilung]
      ,ZEI.[PscdKontoKlient]
      ,ZEI.[PscdKontoauszug]
      ,ZEI.[PscdKontoauszugPos]
      ,ZEI.[KgZahlungseingangArtCode]
      ,ZEI.[Ausgeglichen]
      ,ZEI.[ErstelltUserID]
      ,ZEI.[ErstelltDatum]
      ,ZEI.[MutiertUserID]
      ,ZEI.[MutiertDatum]
      ,ZEI.[KgZahlungseingangTS],
       Mandant     = PRS.NameVorname,
       Adresse     = PRS.Wohnsitz,
       MT              = (SELECT TOP 1 USR.[User]
                      FROM [dbo].[vwVmGetMTName] USR
                      WHERE USR.BaPersonID = ZEI.BaPersonID
                      ORDER BY CASE WHEN ZEI.Datum BETWEEN ISNULL(USR.DatumVon, '17530101') AND ISNULL(USR.DatumBis, '30001231') THEN 2
                        ELSE 1 END DESC, USR.DatumVon DESC),
       DebitorText = DEB.Name,
       ZusatzInfo  = DEB.AdresseMehrzeilig
FROM   dbo.KgZahlungseingang       ZEI WITH (READUNCOMMITTED)
       LEFT JOIN dbo.vwPerson2      PRS ON PRS.BaPersonID      = ZEI.BaPersonID
       LEFT JOIN dbo.vwInstitution2 DEB ON DEB.BaInstitutionID = ZEI.BaInstitutionID
WHERE (ZEI.KgZahlungseingangArtCode BETWEEN 300 AND 399 OR 
       ZEI.KgZahlungseingangArtCode = 100 OR 
       (ZEI.KgZahlungseingangArtCode IN (10,11) AND ZEI.Betrag &gt;= 0) OR 
       ZEI.KgZahlungseingangArtCode IS NULL )
--- AND ZEI.Datum &gt;= {edtDatumVonX}
--- AND ZEI.Datum &lt;= {edtDatumBisX}
--- AND ZEI.BaPersonID = {edtBaPersonIDX.LookupID} 
--- AND ZEI.BaInstitutionID = {edtBaInstitutionIDX.LookupID}
--- AND ZEI.Betrag &gt;= {edtBetragVonX}
--- AND ZEI.Betrag &lt;= {edtBetragBisX}
--- AND ({edtNurAusgeglichenX} = 0 OR ZEI.Ausgeglichen = 0)
--- AND Mitteilung like '%' + {edtMitteilungX} + '%'
order by ZEI.Datum</value>
  </data>
