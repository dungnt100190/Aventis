  <data name="qryDebitor.SelectStatement" xml:space="preserve">
    <value>SELECT *,
       Konto      = CONVERT(VARCHAR(200), NULL),
       FaelligAm  = CONVERT(DATETIME, NULL),
       Debitor    = CONVERT(VARCHAR(200), NULL),
       ZusatzInfo = CONVERT(VARCHAR(500), NULL),
       Mandant    = CONVERT(VARCHAR(200), NULL),
       BaPersonID = CONVERT(INT, NULL)
FROM dbo.KgPosition WITH(READUNCOMMITTED)
WHERE 1 = 2;</value>
