  <data name="qryKgDokumente.SelectStatement" xml:space="preserve">
    <value>SELECT KDO.*, 
       DOC.DateCreation,
       DOC.DateLastSave
FROM   KgPosition POS
       INNER JOIN KgBudget   BDG ON BDG.KgBudgetID = POS.KgBudgetID
       INNER JOIN KgDokument KDO ON KDO.KgPositionID = POS.KgPositionID or
                                    KDO.KgBudgetID = POS.KgBudgetID or
                                    KDO.KgBudgetID = BDG.KgMasterbudgetID
       LEFT JOIN XDocument   DOC ON DOC.DocumentID = KDO.DocumentID
WHERE  POS.KgPositionID = {0}
ORDER BY KDO.KgDokumentTypCode, Stichwort</value>
  </data>
