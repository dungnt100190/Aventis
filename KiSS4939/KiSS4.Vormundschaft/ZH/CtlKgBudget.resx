  <data name="qryBelege.SelectStatement" xml:space="preserve">
    <value>SELECT *,
       BelegNrText = 'Beleg ' + CONVERT(VARCHAR(50),PscdBelegNr),
       Sel         = CONVERT(BIT,0),
       EffektivDatum = (SELECT MAX(BUC2.ValutaDatum)
                        FROM dbo.KgOpAusgleich AUS
                          INNER JOIN dbo.KgBuchung BUC2 ON BUC2.KgBuchungID = AUS.AusgleichBuchungID
                        WHERE AUS.OpBuchungID = BUC.KgBuchungID)
FROM dbo.KgBuchung BUC
WHERE KgPositionID = {0}
ORDER BY ValutaDatum;</value>
  </data>
