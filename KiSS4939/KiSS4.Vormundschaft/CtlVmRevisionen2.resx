  <data name="qryInfoMandant.SelectStatement" xml:space="preserve">
    <value>SELECT
  MandantName       = PRS.NameVorname,
  MandantStrasse    = PRS.WohnsitzStrasseHausNr,
  MandantOrt        = PRS.WohnsitzPLZOrt,
  MandantGeburt     = CONVERT(varchar,PRS.Geburtsdatum,104) + ' (' + CONVERT(varchar,CONVERT(int, ((DateDiff(dd,Geburtsdatum,GetDate())+.5)/365.25))) + ')',
  MandantZivilstand = ZIV.Text,
  MandantHeimatort  = HEI.Name,
  MandantVater      = (SELECT TOP 1 VAT.NameVorname + IsNull(' - ' + VAT.WohnsitzOrt,'')
                       FROM BaPerson_Relation DPR
                         INNER JOIN vwPerson  VAT ON VAT.BaPersonID = DPR.BaPersonID_1 AND VAT.GeschlechtCode = 1
                       WHERE DPR.BaPersonID_2 = FAL.BaPersonID
                         AND DPR.BaRelationID = 1),
  MandantMutter     = (SELECT TOP 1 MUT.NameVorname + IsNull(' - ' + MUT.WohnsitzOrt,'')
                       FROM BaPerson_Relation DPR
                         INNER JOIN vwPerson  MUT ON MUT.BaPersonID = DPR.BaPersonID_1 AND MUT.GeschlechtCode = 2
                       WHERE DPR.BaPersonID_2 = FAL.BaPersonID
                         AND DPR.BaRelationID = 1)
FROM FaLeistung          FAL
  LEFT  JOIN vwPerson    PRS ON PRS.BaPersonID = FAL.BaPersonID
  LEFT  JOIN XLOVCode    ZIV ON ZIV.LOVName = 'Zivilstand' AND ZIV.Code = PRS.ZivilstandCode
  LEFT  JOIN BaGemeinde  HEI ON HEI.BaGemeindeID = PRS.HeimatgemeindeBaGemeindeID
WHERE FAL.FaLeistungID = {0}</value>
  </data>
