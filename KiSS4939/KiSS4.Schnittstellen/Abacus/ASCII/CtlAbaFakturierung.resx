  <data name="qryFakturierung.SelectStatement" xml:space="preserve">
    <value>DECLARE @BDELeistung TABLE(
    BDELeistungID INT,
    Datum DATETIME,
    BaPersonID INT,
    FaLeistungID INT
)

DECLARE @OrgUnit TABLE (
  OrgUnitID INT
)

INSERT INTO @OrgUnit (OrgUnitID)
SELECT OrgUnitID
FROM XOrgUnit
WHERE dbo.fnOrgUnitHierarchyValue(OrgUnitID, 'mndnr') = {0};

INSERT INTO @BDELeistung (BDELeistungID, Datum, BaPersonID, FaLeistungID) 
SELECT BLE.BDELeistungID, BLE.Datum, PRS.BaPersonID, LEI.FaLeistungID
FROM dbo.BDELeistung BLE WITH(READUNCOMMITTED)
  INNER JOIN dbo.BDELeistungsart BLA WITH (READUNCOMMITTED) ON BLA.BDELeistungsartID = BLE.BDELeistungsartID 
  INNER JOIN dbo.BaPerson PRS WITH(READUNCOMMITTED) ON PRS.BaPersonID = BLE.BaPersonID
  CROSS APPLY (SELECT TOP 1 FaLeistungID, UserID
               FROM dbo.FaLeistung WITH(READUNCOMMITTED) 
               WHERE BaPersonID = PRS.BaPersonID 
                 AND ModulID = 2 --nur Fallf√ºhrung
                 AND DatumBis IS NULL --nur offene Leistungen
               ORDER BY DatumBis DESC) LEI
  INNER JOIN dbo.XUser USR WITH(READUNCOMMITTED) ON USR.UserID = LEI.UserID
  INNER JOIN dbo.XOrgUnit_User OUU WITH(READUNCOMMITTED) ON OUU.UserID = LEI.UserID
                                                        AND OUU.OrgUnitMemberCode = 2 --nur Mitglied
  INNER JOIN @OrgUnit ORG ON ORG.OrgUnitID = OUU.OrgUnitID
WHERE BLE.Visiert = 1
  AND BLE.Fakturiert = 0
  AND PRS.KontoFuehrung = 1
  AND BLA.AuswFakturaCode IS NOT NULL 
  AND ({1} = -1 OR BLA.AuswFakturaCode = {1})

SELECT
  DoExport = CAST(0 AS BIT),
  BaPersonID = PRS.BaPersonID,
  SprachCode = PRS.SprachCode,
  ErgaenzungsLeistungen = PRS.ErgaenzungsLeistungen,
  Name = PRS.Name,
  Vorname = PRS.Vorname,
  DebitorNr = PRS.DebitorNr,
  AnzahlPositionen = COUNT(BLE.BDELeistungID),
  DatumVon = MIN(BLE.Datum),
  DatumBis = MAX(BLE.Datum),
  StatusInsUpd = CONVERT(VARCHAR, NULL),
  StatusExportResult = CONVERT(VARCHAR, NULL),
  BDELeistungIDs = STUFF((SELECT ',' + CONVERT(VARCHAR(10), BDELeistungID)
                                        FROM @BDELeistung
                                        WHERE BaPersonID = PRS.BaPersonID
                                        FOR XML PATH(''), elements), 1, 1, '')
FROM @BDELeistung BLE 
  INNER JOIN dbo.BaPerson PRS WITH(READUNCOMMITTED) ON PRS.BaPersonID = BLE.BaPersonID
GROUP BY PRS.BaPersonID, PRS.SprachCode, PRS.ErgaenzungsLeistungen, PRS.Name, PRS.Vorname, PRS.DebitorNr
ORDER BY PRS.Name, PRS.Vorname;</value>
