  <data name="qryBerichtHistory.SelectStatement" xml:space="preserve">
    <value>-- Berichte die im VIS sind
;WITH BerichtCTE
AS
(
  SELECT 
    HIS.VmMassnahmeID,
    HIS.DocumentID,
    HIS.DatumVon,
    HIS.DatumBis,
    HIS.DatumBerichtMT,
    HIS.DatumVersandAnVB,
    HIS.DatumGenehmigung1,
    HIS.DatumGenehmigung2,
    HIS.Berichtsart,
    Bemerkung = CONVERT(VARCHAR(MAX), HIS.Bemerkung),
    HIS.BetragSpesen,
    HIS.DatumEingangSDS,
    HIS.DatumAusgangSDS,
    HIS.MAKlibuUserID,
    HIS.DatumFristerstreckung,
    HIS.DatumMahnung1,
    HIS.DatumMahnung2,
    HIS.DatumMahnung3,
    HIS.MandatsTraegerName,
    MaKlibuUser = USR.DisplayText,
    HIS.VIS_Berichtsnummer,
    RankBericht = RANK() OVER (PARTITION BY HIS.VIS_MandateId ORDER BY ISNULL(HIS.DatumVon, '17530101') DESC, ISNULL(HIS.DatumBis, '99991231') DESC)
  FROM dbo.VmBericht             HIS
    LEFT  JOIN dbo.vwUser        USR  ON USR.UserID = HIS.MaKlibuUserID
  WHERE HIS.VIS_MandateId = {1}
)

-- VmBerichte die nur im KiSS sind (Manuell erfasste Berichtsperiode vor VIS-Synchronisation, migriert am Dec  4 2009 10:04PM)
SELECT
  HIS.VmMassnahmeID,
  HIS.DocumentID,
  HIS.DatumVon,
  HIS.DatumBis,
  HIS.DatumBerichtMT,
  HIS.DatumVersandAnVB,
  HIS.DatumGenehmigung1,
  HIS.DatumGenehmigung2,
  HIS.Berichtsart,
  Bemerkung = CONVERT(VARCHAR(MAX), HIS.Bemerkung),
  HIS.BetragSpesen,
  HIS.DatumEingangSDS,
  HIS.DatumAusgangSDS,
  HIS.MAKlibuUserID,
  HIS.DatumFristerstreckung,
  HIS.DatumMahnung1,
  HIS.DatumMahnung2,
  HIS.DatumMahnung3,
  HIS.MandatsTraegerName,
  MaKlibuUser = USR.DisplayText,
  HIS.VIS_Berichtsnummer
FROM dbo.VmBericht HIS
  LEFT JOIN vwUser USR ON USR.UserID = HIS.MaKlibuUserID
WHERE HIS.VmMassnahmeID = {0}
  AND HIS.VIS_BK_ID IS NULL

UNION

-- Historisierte Berichte die im VIS sind
SELECT 
  VmMassnahmeID,
