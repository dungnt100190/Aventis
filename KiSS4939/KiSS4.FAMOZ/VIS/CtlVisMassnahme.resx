  <data name="qryBericht.SelectStatement" xml:space="preserve">
    <value>SELECT
  BER.*,
  MaKlibuUser                         = USR.DisplayText,
  FaDokStatusCode                     = DOC.FaDokStatusCode,
  DokumentTooltip                     = ISNULL(DOC.Stichwort + ', Status: ' + dbo.fnLOVText('FaDokStatus', DOC.FaDokStatusCode), ''),
  VermoegensAbrechnungFaDokStatusCode = DOC2.FaDokStatusCode,
  VermoegensabrechnungsDokumenteID    = DOC2.FaDokumenteID
FROM dbo.VmBericht BER 
  LEFT JOIN vwUser      USR   ON USR.UserID = BER.MaKlibuUserID
  LEFT JOIN FaDokumente DOC   ON DOC.DocumentID = BER.DocumentID
  LEFT JOIN XDocument   XDOC2 ON XDOC2.DocumentID = BER.VermoegensabrechnungDocumentID
  LEFT JOIN FaDokumente DOC2  ON DOC2.DocumentID = BER.VermoegensabrechnungDocumentID
    -- Es sollte nur ein Dokument geben. Doch beim Testen auf der Integrations-
    -- Umgebung gibt es ohne die nächsten Zeilen Probleme, da die Dokumenten-DB nicht kopiert
    -- wird, die Tabelle FaDokumente jedoch schon. Für einen neu erstellten XDocument-
    -- Eintrag kann es also noch alte FaDokumente-Einträge geben.
    AND YEAR(DOC2.DatumErstellung) = YEAR(XDOC2.DateCreation)
    AND MONTH(DOC2.DatumErstellung) = MONTH(XDOC2.DateCreation)
    AND DAY(DOC2.DatumErstellung) = DAY(XDOC2.DateCreation)
WHERE BER.VIS_BK_ID = {0}</value>
  </data>
