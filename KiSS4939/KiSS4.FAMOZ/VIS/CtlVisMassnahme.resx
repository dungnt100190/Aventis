  <data name="qryVmBerichtsperioden.SelectStatement" xml:space="preserve">
    <value>SELECT DISTINCT
  Ernennung      = VMT.M_Ernenn_DT,
  Aufhebung      = VMT.M_Aufheb_DT,
  VISMA          = VMT.Name + ', ' + VMT.Vorname,
  KiSSMA         = USR.DisplayText,
  DatumVon       = VMT.BERICHT_VON,
  DatumBis       = VMT.BERICHT_PER,
  Berichtsart    = VMT.T1,
  VIS_BK_ID      = VMT.BK_ID,
  VmMassnahmeID  = MAS.VmMassnahmeID,
  MAND_ID        = VMT.MAND_ID,
  MandateId      = VMT.MandateId,
  BERICHTSTERMIN = VMT.BERICHTSTERMIN
FROM dbo.vwVIS_MANDATSTRAEGER VMT
  LEFT JOIN vwUser USR ON USR.UserID = (SELECT TOP 1 UserID
                                        FROM XUser
                                        WHERE LastName  = VMT.Name COLLATE database_default 
                                          AND FirstName = VMT.Vorname COLLATE database_default 
                                          AND IsLocked  = 0 
                                          AND LogonName NOT LIKE '%[_]%'
                                        ORDER BY UserID DESC)
  INNER JOIN dbo.VmMassnahme MAS ON MAS.VIS_ArrangementId = VMT.ArrangementId 
  INNER JOIN dbo.FaLeistung LEI ON LEI.FaLeistungID = MAS.FaLeistungID
WHERE LEI.FaLeistungID = {0}
ORDER BY Ernennung DESC</value>
  </data>
  <data name="qryVmMassnahmeHistory.SelectStatement" xml:space="preserve">
    <value>SELECT DISTINCT
  Massnahme = ISNULL(VMAS.Massnahme, '') + ISNULL(' - ' + VMAS.Mandatstyp, ''),
  DatumVon  = VMAS.BESCH_A_ED,
  DatumBis  = VMAS.BESCH_A_AD
FROM   dbo.vwVIS_MASSNAHMEN_HIST VMAS
  INNER JOIN dbo.VmMassnahme MAS ON MAS.VIS_ArrangementId = VMAS.ArrangementId
  INNER JOIN dbo.FaLeistung LEI ON LEI.FaLeistungID = MAS.FaLeistungID
WHERE LEI.FaLeistungID = {0}
ORDER BY DatumVon</value>
  </data>
