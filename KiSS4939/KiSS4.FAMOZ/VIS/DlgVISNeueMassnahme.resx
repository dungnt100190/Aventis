  <data name="qryVISMassnahmen.SelectStatement" xml:space="preserve">
    <value>SELECT
  VISMandant    = DOS.Name + ', ' + DOS.Vorname + ' (' + CONVERT(varchar, DATEDIFF(yy, DOS.G_DAT, GetDate())) + ',' + DOS.SEX + ')',
  VMAS.ArrangementId, 
  VormschID     = VMAS.Vormsch_ID,
  Anordnung     = VMAS.BESCH_A_ED,
  Aufhebung     = VMAS.BESCH_A_AD,
  VMAS.Massnahme,
  [Behördliche Massnahme] = VMAS.Mandatstyp,
  VMExist = CONVERT(BIT, CASE WHEN EXISTS (SELECT LEI.FaLeistungID 
                                           FROM dbo.FaLeistung          LEI 
                                             INNER JOIN dbo.VmMassnahme MAS2 ON MAS2.FaLeistungID = LEI.FaLeistungID 
                                           WHERE MAS2.VIS_VormschID = VMAS.Vormsch_ID) 
                           THEN 1 
                           ELSE 0 
                         END)
FROM dbo.vwVIS_MASSNAHMEN VMAS
  INNER JOIN dbo.vwVIS_DOSSIER DOS ON DOS.Id = VMAS.DossierId
WHERE DOS.FALL_NR = CONVERT(VARCHAR(6),{0}) --DOS.FALL_NR ist VARCHAR(6), durch explizites konvertieren von {0} wird das implizite konvertieren der Werte in DOS.FALL_NR in decimal(5,0) verhindert!
  AND (VMAS.BESCH_A_AD IS NULL OR {1} = 1)
  AND ({2} IS NULL OR VMAS.Vormsch_ID = {2})
ORDER BY VMAS.BESCH_A_ED, ISNULL(VMAS.BESCH_A_AD, '99991231')
</value>
