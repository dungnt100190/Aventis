  <data name="qryFaLeistung.SelectStatement" xml:space="preserve">
    <value>SELECT 
  FLE.FaLeistungID,
  FLE.FaFallID,
  FLE.EroeffnungsGrundCode,
  FLE.AbschlussGrundCode,
  FLE.DatumVon,
  FLE.DatumBis,
  FLE.Bemerkung,
  FLE.Creator,
  FLE.Created,
  FLE.Modifier,
  FLE.Modified,
  FLE.BaPersonID,
  Mandant = PRS.Name + ISNULL(', ' + PRS.Vorname,''),
  MA      = MA.LogonNameVornameOrgUnit,
  SB      = SB.LogonNameVornameOrgUnit,
  ARC.FaLeistungArchivID,
  MAS.VIS_VormschID,
  MAS.VIS_FallNr,
  [KESB Mitglied] = MAS.Waisenrat,
  Altrechtlich = CAST(1 - MAS.IstNeurechtlich AS BIT),
  Neurechtlich = MAS.IstNeurechtlich,
  VIS_VormschID_2 = CASE WHEN MAS2.VormschID IS NOT NULL
                      THEN MAS2.VormschID
                      ELSE (SELECT TOP 1 Vormsch_ID
                            FROM dbo.vwVIS_MASSNAHMEN VMAS
                            WHERE (MAS.IstNeurechtlich = 1 AND VMAS.ArrangementId_Neurechtlich = MAS.VIS_ArrangementId)
                              OR  (MAS.IstNeurechtlich = 0 AND VMAS.ArrangementId = MAS.VIS_ArrangementId_Neurechtlich))
                    END,
  VIS_VormschID_2_InKiss = CASE WHEN EXISTS(SELECT TOP 1 1
                                            FROM dbo.VmMassnahme MAS1 WITH (READUNCOMMITTED)
                                            WHERE MAS1.VIS_ArrangementId = MAS2.ArrangementId)
                             THEN CONVERT(BIT, 1)
                             ELSE CONVERT(BIT, 0)
                           END
FROM dbo.FaLeistung               FLE WITH (READUNCOMMITTED)
  INNER JOIN dbo.BaPerson         PRS WITH (READUNCOMMITTED) ON PRS.BaPersonID = FLE.BaPersonID
  INNER JOIN dbo.VmMassnahme      MAS WITH (READUNCOMMITTED) ON MAS.FaLeistungID = FLE.FaLeistungID
  INNER JOIN dbo.vwUser           MA  WITH (READUNCOMMITTED) ON MA.UserID = FLE.UserID
  LEFT  JOIN dbo.vwUser           SB  WITH (READUNCOMMITTED) ON SB.UserID = FLE.SachbearbeiterID
  LEFT  JOIN dbo.FaLeistungArchiv ARC WITH (READUNCOMMITTED) ON ARC.FaLeistungID = FLE.FaLeistungID AND ARC.CheckOut IS NULL
  OUTER APPLY (SELECT TOP 1
                 ArrangementId = VIS_ArrangementId,
                 VormschID     = VIS_VormschID
               FROM dbo.VmMassnahme MAS1
               WHERE (MAS.IstNeurechtlich = 1 AND MAS1.VIS_ArrangementId_Neurechtlich = MAS.VIS_ArrangementId)
                 OR  (MAS.IstNeurechtlich = 0 AND MAS1.VIS_ArrangementId = MAS.VIS_ArrangementId_Neurechtlich)) MAS2
WHERE FLE.FaLeistungID = {0};</value>
