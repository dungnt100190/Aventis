  <data name="qryRelation.SelectStatement" xml:space="preserve">
    <value>SELECT	DRE.*,
		Person     = PRS.Name + isNull(', ' + PRS.Vorname,''),
		PersonID   = PRS.BaPersonID,
		RelationID = DRE.BaRelationID 
					 +  
					 case when DRE.BaPersonID_1 = {0} and 
							   (DRL.NameWeiblich1 &lt;&gt; DRL.NameWeiblich2 or DRL.NameMaennlich1 &lt;&gt; DRL.NameMaennlich2)
					 then 1000 
					 else 0 end 
					 +
					 case PRS.GeschlechtCode 
					 when 1 then 100
					 when 2 then 200
					 else 0 end,
		Age        = CONVERT(INT, ((datediff(dd,Geburtsdatum,getDate())+.5)/365.25)),
		Klient     = CONVERT(bit,
					    CASE WHEN EXISTS (SELECT 1 FROM FaLeistung WHERE BaPersonID = PRS.BaPersonID) 
					    THEN 1 ELSE 0 END),
		GlHaushalt   = dbo.fnGleicheAdresse({0}, PRS.BaPersonID),
		Unterstuetzt = dbo.fnUnterstuetzt({0}, PRS.BaPersonID, getdate()),
		PRS.GeschlechtCode
FROM    BaPerson_Relation DRE
  INNER JOIN BaPerson    PRS on PRS.BaPersonID IN (DRE.BaPersonID_1, DRE.BaPersonID_2)
  LEFT  JOIN BaRelation  DRL on DRL.BaRelationID = DRE.BaRelationID
WHERE  (DRE.BaPersonID_1 = {0} or DRE.BaPersonID_2 = {0}) AND PRS.BaPersonID != {0}
ORDER BY Age DESC</value>
  </data>
