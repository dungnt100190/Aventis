  <data name="qryFalltraeger.SelectStatement" xml:space="preserve">
    <value>SELECT Falltraeger       = CASE PRS.GeschlechtCode
                             WHEN 1 THEN dbo.fnGetMLTextFromName('CtlBaHaushalt', 'Falltraeger', {1})
                             WHEN 2 THEN dbo.fnGetMLTextFromName('CtlBaHaushalt', 'Falltraegerin', {1})
                             ELSE dbo.fnGetMLTextFromName('CtlBaHaushalt', 'FalltraegerInUnklar', {1})
                           END,
       Person            = PRS.NameVorname,
       Wohnsitz          = ISNULL(PRS.Wohnsitz, ''),
       Aufenthaltsort    = ISNULL(ISNULL(PRS.AufenthaltInstitutionName, PRS.AufenthaltAdressZusatz) + CHAR(13) + CHAR(10), '') + 
                           ISNULL(PRS.AufenthaltStrasseHausNr + ', ', '') + ISNULL(PRS.AufenthaltPLZOrt, ''),
       Heimatort         = PRS.Heimatort,
       Nationalitaet     = CASE {1}
                             WHEN 1 THEN PRS.Nationalitaet
                             WHEN 2 THEN PRS.NationalitaetFR
                             WHEN 3 THEN PRS.NationalitaetIT
                             ELSE PRS.Nationalitaet
                           END,
       [Alter]           = PRS.[Alter],
       Geburtstag        = CONVERT(VARCHAR, PRS.Geburtsdatum, 104),
       Geschlecht        = dbo.fnLOVMLText('Geschlecht', PRS.GeschlechtCode, {1}),
       Zivilstand        = dbo.fnLOVMLText('Zivilstand', PRS.ZivilstandCode, {1}),
       WohnsitzAdresseID = ISNULL(PRS.WohnsitzBaAdresseID, -1)
FROM dbo.vwPerson      PRS WITH (READUNCOMMITTED)
WHERE PRS.BaPersonID = {0};

</value>
