  <data name="qryZahlInfos.SelectStatement" xml:space="preserve">
    <value>SELECT ZAH.*,
       IsActiv = case when GetDate() between IsNull(ZAH.DatumVon, GetDate()) and IsNull(ZAH.DatumBis, GetDate()) 
                 then 1 else 0 end,
       EZTyp   = dbo.fnLOVText('BgEinzahlungsschein', ZAH.EinzahlungsscheinCode),
       Info    = IsNull('Bank: '       + char(9) + char(9) + BNK.Name + char(13) + char(10),'') +
                 IsNull('Bank PC:'     + char(9) + dbo.fnTnToPc(BNK.PCKontoNr) + char(13) + char(10),'') + 
                 IsNull('Clearing-Nr:' + char(9) + BNK.ClearingNr + char(13) + char(10),'') +
                 IsNull('Bankkonto:'   + char(9) + ZAH.Bankkontonummer + char(13) + char(10),'') + 
                 IsNull('Postkonto:'   + char(9) + dbo.fnTnToPc(ZAH.PostKontoNummer) + char(13) + char(10),'') +
                 IsNull('Referenz-Nr:' + char(9) + ZAH.ESRTeilnehmer + char(13) + char(10),'')
FROM   BaZahlungsweg ZAH
       LEFT Join BaBank BNK on BNK.BaBankID = ZAH.BaBankID
WHERE  BaInstitutionID = {0}
ORDER BY IsActiv desc, EZTyp, ZAH.DatumVon desc</value>
  </data>
