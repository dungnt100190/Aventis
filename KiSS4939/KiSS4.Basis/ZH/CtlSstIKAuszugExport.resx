  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>SELECT DISTINCT
	NameVorname = PER.DisplayText,
  PER.EinwohnerregisterID,
  PER.EinwohnerregisterIDOhne0er,
	PER.ZIPNr,
	PER.Geburtsdatum,
	PER.AHVNummer,
	PER.Versichertennummer,
	IKA.AnforderungDatum,
	[User] = USR.DisplayText,
	IKA.JahrVon,
	IKA.JahrBis,
	IKA.SstIkAuszugAnforderungCode,
	IKA.SstIkAuszugStatusCode,
	IKA.ImportFehlermeldung,
	Bemerkung = CASE 
			          WHEN IKA.SstIkAuszugAnforderungCode = 1 THEN 'Genehmigung Finanzplan (Automatisch)'
			          WHEN IKA.SstIkAuszugAnforderungCode = 2 THEN '2. jährige Fallkontrolle ((Automatisch))'
			          WHEN IKA.SstIkAuszugAnforderungCode = 3 THEN '5. jährige Fallkontrolle (Automatisch)'
			          WHEN IKA.SstIkAuszugAnforderungCode = 4 THEN 'Manuell angefordert'
			          ELSE ''
		          END,
	DatumLetzterIKAuszug = CASE 
	                         WHEN ISNULL(LastIKA.SstIkAuszugStatusCode, 0) = 5 
	                         THEN LastIKA.ImportDatum 
	                         ELSE '' 
	                       END, -- Falls es schon ein verarbeiteter IK-Auszug gibt, zeige das Importdatum an
	SstIKAuszugID$ = IKA.SstIKAuszugID,
	BaPersonID = PER.BaPersonID,
  KlientensystemBaPersonID = dbo.fnGetHauptpersonFuerMitglied(PER.BaPersonID),
  FaFallIDs = (SELECT dbo.ConcDistinctOrder(FaFallID) 
               FROM dbo.FaFallPerson 
               WHERE BaPersonID = PER.BaPersonID)
FROM dbo.vwPerson PER
  INNER JOIN dbo.vwSstIKAuszug IKA     ON IKA.BaPersonID = PER.BaPersonID
  LEFT  JOIN dbo.vwSstIKAuszug LastIKA ON LastIKA.SstIKAuszugID = (SELECT TOP 1 SstIKAuszugID 
                                                                   FROM dbo.vwSstIKAuszug 
                                                                   WHERE BaPersonID = PER.BaPersonID 
                                                                     AND SstIkAuszugStatusCode = 5 
                                                                   ORDER BY AnforderungDatum DESC)
  INNER JOIN dbo.vwUser        USR     ON USR.UserID = IKA.AnforderungUserID 
WHERE IKA.SstIkAuszugStatusCode IN (1, 4) -- "Angefordert" oder "importiert mit Fehler"
ORDER BY PER.DisplayText;</value>
