  <data name="qryBaPerson.SelectStatement" xml:space="preserve">
    <value>select distinct 
  Code = PRS.BaPersonID, 
  Text = PRS.NameVorname
from dbo.FaFall FAL 
  inner join dbo.FaFallPerson FAP on FAP.FaFallID = FAL.FaFallID
  inner join dbo.vwPerson PRS on PRS.BaPersonID = FAP.BaPersonID
where FAL.BaPersonID = {0} 
order by Text</value>
  </data>
  <data name="qryBaPerson_Relation.SelectStatement" xml:space="preserve">
    <value>SELECT 
  REL.BaPerson_RelationID,
  REL.BaPersonID_1,
  REL.BaPersonID_2,
  REL.BaRelationID,
  REL.DatumVon,
  REL.DatumBis,
  REL.BaRelationQualitaetCode,
  REL.Bemerkung,
  REL.BaPerson_RelationTS,
  Name1 = P1.NameVorname,
  Beziehung1 = dbo.fnBaRelationClient(REL.BaPersonID_2, REL.BaPersonID_1, GetDate()),
  Name2 = P2.NameVorname,
  Beziehung2 = dbo.fnBaRelationClient(REL.BaPersonID_1, REL.BaPersonID_2, GetDate())
FROM BaPerson_Relation REL
  LEFT JOIN vwPerson P1 on P1.BaPersonID = REL.BaPersonID_1
  LEFT JOIN vwPerson P2 on P2.BaPersonID = REL.BaPersonID_2
WHERE EXISTS (
  SELECT 1 FROM FaFall FAL
  INNER JOIN FaFallPerson  FP1 ON FP1.FaFallID = FAL.FaFallID AND FP1.BaPersonID = REL.BaPersonID_1
  INNER JOIN FaFallPerson  FP2 ON FP2.FaFallID = FAL.FaFallID AND FP2.BaPersonID = REL.BaPersonID_2
  WHERE FAL.BaPersonID = {0})
-- Zum Testen : FAL.BaPersonID = 40314 oder 49697</value>
  </data>
  <data name="qryKlientensystem.SelectStatement" xml:space="preserve">
    <value>select 
  FAP.FaFallPersonID, FAP.FaFallID, 
  FAP.BaPersonID, FAP.DatumVon, FAP.DatumBis,
  PRS.Geburtsdatum,
  Fall = convert(varchar,FAL.FaFallID) + ', ' + case 
    when FAL.DatumBis is null then 'ab ' + convert(varchar,FAL.DatumVon,104)
    else convert(varchar,FAL.DatumVon,104) + ' - ' + convert(varchar,FAL.DatumBis,104)
  end,
  PRS.NameVorname,
  Geschlecht = dbo.fnLOVColumnListe(
    'BaGeschlecht', Convert(varchar(100), PRS.GeschlechtCode), 'shortText'
  ),
  Beziehung  = case 
    when PRS.BaPersonID = {0} then null
    else dbo.fnBaRelationClient({0}, PRS.BaPersonID, IsNull(FAL.DatumBis,GetDate())) 
  end
from FaFall FAL
inner join FaFallPerson FAP on FAP.FaFallID = FAL.FaFallID
inner join vwPerson PRS on PRS.BaPersonID = FAP.BaPersonID
where FAL.BaPersonID = {0}
order by 
  FAL.DatumVon desc, 
  case when FAL.BaPersonID = FAP.BaPersonID then 0 else 1 end, 
  PRS.Geburtsdatum desc</value>
