                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT KRE.KontoNr, DEB.KontoNr AS Expr1, LEI.PscdVertragsgegenstandID, LEI.FaLeistungID, CASE HabenKtoNr WHEN KRE.KontoNr THEN 1 ELSE 0 END AS IstKreditorBuchung, CASE SollKtoNr WHEN DEB.KontoNr THEN 1 ELSE 0 END AS IstDebitorBuchung, CASE WHEN BaZahlungswegID NOT IN (SELECT BaZahlungswegID FROM BaZahlungsweg ZAL INNER JOIN PscdSentFaLeistung_Person SNT ON SNT.BaPersonID = ZAL.BaPersonID WHERE SNT.FaLeistungID = LEI.FaLeistungID) THEN 1 ELSE 0 END AS AnDritte, LEI.BaPersonID AS BaPersonID_LT, KBU.* FROM KbBuchung AS KBU INNER JOIN BgBudget AS BUD ON BUD.BgBudgetID = KBU.BgBudgetID INNER JOIN BgFinanzplan AS FIP ON FIP.BgFinanzplanID = BUD.BgFinanzplanID INNER JOIN FaLeistung AS LEI ON LEI.FaLeistungID = FIP.FaLeistungID LEFT OUTER JOIN KbKonto AS DEB ON DEB.KbPeriodeID = KBU.KbPeriodeID AND ',' + DEB.KbKontoartCodes + ',' LIKE '%,20,%' LEFT OUTER JOIN KbKonto AS KRE ON KRE.KbPeriodeID = KBU.KbPeriodeID AND ',' + KRE.KbKontoartCodes + ',' LIKE '%,30,%' WHERE (KbBuchungStatusCode = 2) AND (HabenKtoNr = KRE.KontoNr) OR (KbBuchungStatusCode = 2) AND (SollKtoNr = DEB.KontoNr)</CommandText>
