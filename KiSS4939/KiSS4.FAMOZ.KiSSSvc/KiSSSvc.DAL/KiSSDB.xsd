              <DbSource ConnectionRef="KiSS_40_MASTERConnectionString (Settings)" DbObjectType="Unknown" GenerateMethods="Get" GenerateShortCommands="true" GeneratorGetMethodName="GetEinzelpostenByIDs" GetMethodModifier="Private" GetMethodName="GetEinzelpostenByIDs" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetEinzelpostenByIDs" UserSourceName="GetEinzelpostenByIDs">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>
                      SELECT     WVP.BEDCode, WVP.BeguenstigterBaPersonID, WVP.Betrag, WVP.BgSplittingArtCode, WVP.Buchungstext, WVP.DatumBis, WVP.DatumVon,
                      WVP.Hauptvorgang, WVP.HeimatkantonNr, WVP.KbBuchungBruttoPersonID, WVP.KbWVEinzelpositionTS, WVP.KbWVEinzelpostenID,
                      WVP.KbWVEinzelpostenStatusCode, WVP.KbWVLaufID, WVP.PscdBelegNr, WVP.PscdFehlermeldung, WVP.SKZNummer,
                      WVP.SplittingDurchWVLaufDatumBis, WVP.StorniertKbWVEinzelpostenID, WVP.Teilvorgang, WVP.TransferDatum, WVP.Ungueltig,
                      WVP.UnterstuetzungsanzeigeDocumentID, WVP.UnterstuetzungstraegerBaPersonID, WVP.WVCode, WVP.WhWVEinheitMitgliedID,
                      'W' AS WohnHeimatkanton, WVE.WhWVEinheitID AS WVEinheitID, LEI.PscdVertragsgegenstandID AS Vertragsgegenstand,
                      KBB.BelegNr AS BelegNrBuchung, WVE.DatumVon AS WVCodeVon, WVE.DatumBis AS WVCodeBis, USR.VornameName AS Ansprechperson,
                      KBB.BgBudgetID,
                      (SELECT     COUNT(*) AS Expr1
                      FROM          BgBudget AS BDG INNER JOIN
                      BgFinanzplan_BaPerson AS FPP ON BDG.BgFinanzplanID = FPP.BgFinanzplanID
                      WHERE      (BDG.BgBudgetID = KBB.BgBudgetID) AND (FPP.IstUnterstuetzt = 1)) AS UntPersonenImHaushalt, KBB.Kostenstelle
                      FROM         KbWVEinzelposten AS WVP INNER JOIN
                      WhWVEinheitMitglied AS WVM ON WVM.WhWVEinheitMitgliedID = WVP.WhWVEinheitMitgliedID INNER JOIN
                      WhWVEinheit AS WVE ON WVE.WhWVEinheitID = WVM.WhWVEinheitID INNER JOIN
                      KbBuchungBruttoPerson AS KBP ON KBP.KbBuchungBruttoPersonID = WVP.KbBuchungBruttoPersonID INNER JOIN
                      KbBuchungBrutto AS KBB ON KBB.KbBuchungBruttoID = KBP.KbBuchungBruttoID INNER JOIN
                      FaLeistung AS LEI ON LEI.FaLeistungID = KBB.FaLeistungID INNER JOIN
                      vwUser AS USR ON USR.UserID = LEI.UserID
                      WHERE     (WVP.KbWVEinzelpostenStatusCode = 1) AND (WVP.TransferDatum IS NULL)
                    </CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
