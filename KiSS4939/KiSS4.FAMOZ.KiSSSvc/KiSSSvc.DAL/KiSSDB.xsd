                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>
                      DECLARE @KbBuchungBruttoID int,
                      @KbBuchungID int,
                      @Mahnstufe_alt int,
                      @KbBuchungStatusCode int,
                      @msg varchar(500)

                      -- Notwendige Angaben der zu bearbeitenden Brutto-Buchung holen
                      SELECT @KbBuchungBruttoID = KbBuchungBruttoID, @Mahnstufe_alt = Mahnstufe, @KbBuchungStatusCode = KbBuchungStatusCode
                      FROM   KbBuchungBrutto
                      WHERE  BelegNr = @BelegNr

                      IF @KbBuchungBruttoID IS NOT NULL BEGIN

                      IF @Mahnstufe BETWEEN 90 AND 93 BEGIN

                      UPDATE KbBuchungBrutto
                      SET Fakturasperre = CASE @Mahnstufe WHEN 90 THEN 1
                      WHEN 92 THEN 0
                      ELSE Fakturasperre END,
                      Mahnsperre = CASE @Mahnstufe WHEN 91 THEN 1
                      WHEN 93 THEN 0
                      ELSE Mahnsperre END
                      WHERE KbBuchungBruttoID = @KbBuchungBruttoID

                      END
                      ELSE BEGIN

                      IF @Mahnstufe_alt + 1 &lt;&gt; @Mahnstufe BEGIN
                      SET @msg = 'Mahnstufe darf nur um 1 erhht werden. Bisher ' + cast(@Mahnstufe_alt as varchar) + ', neu ' + cast(@Mahnstufe as varchar)
                      RAISERROR(@msg,18,1)
                      SELECT 0
                      RETURN
                      END

                      UPDATE    KbBuchungBrutto
                      SET       Mahnstufe = @Mahnstufe
                      WHERE     KbBuchungBruttoID = @KbBuchungBruttoID

                      END
                      END
                      ELSE BEGIN
                      -- Falls keine Bruttobuchung gefunden:
                      -- Notwendige Angaben der zu bearbeitenden Netto-Buchung holen (Inkassobuchungen sind Nettobelege)
                      SELECT @KbBuchungID = KbBuchungID, @Mahnstufe_alt = Mahnstufe, @KbBuchungStatusCode = KbBuchungStatusCode
                      FROM   KbBuchung
                      WHERE  BelegNr = @BelegNr

                      IF @KbBuchungID IS NULL BEGIN
                      SET @msg = 'Keine Buchung mit Belegnummer ' + cast(@BelegNr as varchar) + ' gefunden'
                      RAISERROR(@msg,18,1)
                      SELECT 0
                      RETURN
                      END

                      IF @Mahnstufe BETWEEN 90 AND 93 BEGIN

                      UPDATE KbBuchung
                      SET Fakturasperre = CASE @Mahnstufe WHEN 90 THEN 1
                      WHEN 92 THEN 0
                      ELSE Fakturasperre END,
                      Mahnsperre = CASE @Mahnstufe WHEN 91 THEN 1
                      WHEN 93 THEN 0
                      ELSE Mahnsperre END
                      WHERE KbBuchungID = @KbBuchungID

                      END
                      ELSE BEGIN

                      IF @Mahnstufe_alt + 1 &lt;&gt; @Mahnstufe BEGIN
                      SET @msg = 'Mahnstufe darf nur um 1 erhht werden. Bisher ' + cast(@Mahnstufe_alt as varchar) + ', neu ' + cast(@Mahnstufe as varchar)
                      RAISERROR(@msg,18,1)
                      SELECT 0
                      RETURN
                      END

                      UPDATE    KbBuchung
                      SET       Mahnstufe = @Mahnstufe
                      WHERE     KbBuchungID = @KbBuchungID

                      END

                      END

                      SELECT @@ROWCOUNT
                    </CommandText>
