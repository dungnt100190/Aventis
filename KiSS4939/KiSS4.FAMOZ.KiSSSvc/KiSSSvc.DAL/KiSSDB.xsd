              <DbSource ConnectionRef="KiSS_40_MASTERConnectionString (Settings)" DbObjectName="KiSS4_FAMOZ_MasterDev.dbo.KbBuchung" DbObjectType="Table" GenerateMethods="Get" GenerateShortCommands="true" GeneratorGetMethodName="GetAlimBelegeFromKbBuchungIDs" GetMethodModifier="Private" GetMethodName="GetAlimBelegeFromKbBuchungIDs" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetAlimBelegeFromKbBuchungIDs" UserSourceName="GetAlimBelegeFromKbBuchungIDs">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT LEI.PscdVertragsgegenstandID, LEI.FaLeistungID, CASE HabenKtoNr WHEN KRE.KontoNr THEN 1 ELSE 0 END AS IstKreditorBuchung, CASE SollKtoNr WHEN DEB.KontoNr THEN 1 ELSE 0 END AS IstDebitorBuchung, CASE WHEN BaZahlungswegID NOT IN (SELECT BaZahlungswegID FROM BaZahlungsweg ZAL INNER JOIN PscdSentFaLeistung_Person SNT ON SNT.BaPersonID = ZAL.BaPersonID WHERE SNT.FaLeistungID = LEI.FaLeistungID) THEN 1 ELSE 0 END AS AnDritte, KBU.* FROM KbBuchung AS KBU INNER JOIN IkPosition AS IKP ON IKP.IkPositionID = KBU.IkPositionID LEFT OUTER JOIN IkRechtstitel AS RET ON RET.IkRechtstitelID = IKP.IkRechtstitelID INNER JOIN FaLeistung AS LEI ON LEI.FaLeistungID = ISNULL(RET.FaLeistungID, IKP.FaLeistungID) LEFT OUTER JOIN KbKonto AS DEB ON DEB.KbPeriodeID = KBU.KbPeriodeID AND ',' + DEB.KbKontoartCodes + ',' LIKE '%,20,%' LEFT OUTER JOIN KbKonto AS KRE ON KRE.KbPeriodeID = KBU.KbPeriodeID AND ',' + KRE.KbKontoartCodes + ',' LIKE '%,30,%' WHERE (KBU.ModulID = 4) AND (KbBuchungStatusCode = 2) AND (HabenKtoNr = KRE.KontoNr) OR (KBU.ModulID = 4) AND (KbBuchungStatusCode = 2) AND (SollKtoNr = DEB.KontoNr)</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
