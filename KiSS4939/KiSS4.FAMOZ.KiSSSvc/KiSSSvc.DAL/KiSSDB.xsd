                      <Parameter AllowDbNull="false" AutogeneratedName="BaPersonID" ColumnName="BaPersonID" DataSourceName="" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@BaPersonID" Precision="0" ProviderType="Int" Scale="0" Size="4" SourceColumn="BaPersonID" SourceColumnNullMapping="false" SourceVersion="Current" />
