                      <Parameter AllowDbNull="true" AutogeneratedName="KbBuchungStatusCode" ColumnName="KbBuchungStatusCode" DataSourceName="KiSS4_MASTER_ZH.dbo.KbBuchungBrutto" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@KbBuchungStatusCode" Precision="0" ProviderType="Int" Scale="0" Size="4" SourceColumn="KbBuchungStatusCode" SourceColumnNullMapping="false" SourceVersion="Current" />
