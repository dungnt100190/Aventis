                <UpdateCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>
                      UPDATE KBU
                      SET KbBuchungStatusCode = @KbBuchungStatusCode, PscdFehlermeldung = @PscdFehlermeldung
                      FROM KbBuchungBrutto               KBB
                      INNER JOIN KbBuchungBruttoPerson KBP ON KBB.KbBuchungBruttoID = KBP.KbBuchungBruttoID
                      INNER JOIN KbBuchungKostenart    KBK ON KBK.BgPositionID      = KBP.BgPositionID
                      INNER JOIN KbBuchung             KBU ON KBU.KbBuchungID       = KBK.KbBuchungID AND KBU.ValutaDatum = KBB.ValutaDatum
                      INNER JOIN KbKonto               KRE ON KRE.KbPeriodeID       = KBU.KbPeriodeID AND KRE.KontoNr = KBU.SollKtoNr AND ',' + KRE.KbKontoartCodes + ',' LIKE '%,20,%' -- Nur Forderungen (Debitorenbuchungen) updaten
                      WHERE KBB.KbBuchungBruttoID = @KbBuchungBruttoID AND KBU.KbBuchungStatusCode IN (2,5)
                    </CommandText>
