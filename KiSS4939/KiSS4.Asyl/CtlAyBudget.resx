  <data name="qryBgPosition.SelectStatement" xml:space="preserve">
    <value>DECLARE @GetDate datetime  SET @GetDate = GetDate()

SELECT Style, Bezeichnung, Betrag, Total, KOA, Zahlungsmodus, Abtretung, Bemerkung, BgKategorieCode, BgPositionID
FROM dbo.fnAyGetBudget({0}, @GetDate)
ORDER BY SortKey</value>
