  <data name="qryKbBuchung.SelectStatement" xml:space="preserve">
    <value>SELECT *
FROM KbBuchung
WHERE BgBudgetID = {0}

SELECT
  BKA.*,
  PersonName   = PRS.NameVorname,
  Kostenstelle = IsNull(dbo.fnKbGetKostenstelle(KSP.BaPersonID), IsNull(KST.Nr + ': ', '') + KST.Name)
FROM KbBuchungKostenart      BKA
  INNER JOIN KbBuchung       BUC ON BUC.KbBuchungID = BKA.KbBuchungID
  LEFT  JOIN KbKostenstelle  KST ON KST.KbKostenstelleID = BKA.KbKostenstelleID
  LEFT  JOIN KbKostenstelle_BaPerson KSP ON KSP.KbKostenstelleID = KST.KbKostenstelleID
                                    AND (KSP.DatumBis IS NULL OR GetDate() BETWEEN KSP.DatumVon AND KSP.DatumBis)
  LEFT  JOIN vwPerson        PRS ON PRS.BaPersonID  = BKA.BaPersonID
WHERE BUC.BgBudgetID = {0}</value>
  </data>
