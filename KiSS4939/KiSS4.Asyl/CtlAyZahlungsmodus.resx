  <data name="qryBgZahlungsmodus.SelectStatement" xml:space="preserve">
    <value>SELECT SZM.*,
  Kreditor = IsNull(PRS.NameVorname, INS.Name) + IsNull(', ' + IsNull(PRS.WohnsitzStrasseHausNr, INS.StrasseHausNr), '')
           + IsNull(', ' + IsNull(PRS.WohnsitzPLZOrt, INS.PLZOrt), ''),
  Aktiv    = CONVERT(bit, IsNull(CASE WHEN SZM.DatumBis &lt;= GetDate() THEN 0 ELSE 1 END, 1)),
  Periodizitaet = CASE
                    WHEN TRM2.TagImMonat IS NOT NULL THEN 2  -- 2x pro Monat
                    WHEN TRM1.TagImMonat IS NOT NULL THEN 1  -- 1x pro Monat
                    WHEN TRM3.Datum      IS NOT NULL THEN 3  -- Individuell
                    ELSE 1
                  END,
  TRM1.BgZahlungsmodusTerminID, 
  TRM1.TagImMonat, 
  TRM1.ImVormonat,
  ShZahlungsmodusTerminID_2 = TRM2.BgZahlungsmodusTerminID,
  TagImMonat_2 = TRM2.TagImMonat,
  BetragGleich = CONVERT(bit, IsNull(TRM3.BetragGleich, 1))
FROM BgZahlungsmodus  SZM
  LEFT  JOIN BaZahlungsweg         FZW  ON FZW.BaZahlungswegID    = SZM.BaZahlungswegID
  LEFT  JOIN vwPerson              PRS  ON PRS.BaPersonID = FZW.BaPersonID
  LEFT  JOIN vwInstitution         INS  ON INS.BaInstitutionID = FZW.BaInstitutionID

  LEFT  JOIN BgZahlungsmodusTermin TRM1 ON TRM1.BgZahlungsmodusID = SZM.BgZahlungsmodusID AND TRM1.TagImMonat IS NOT NULL
  LEFT  JOIN BgZahlungsmodusTermin TRM2 ON TRM2.BgZahlungsmodusID = SZM.BgZahlungsmodusID AND TRM2.TagImMonat IS NOT NULL AND TRM2.BgZahlungsmodusTerminID != TRM1.BgZahlungsmodusTerminID
  LEFT  JOIN (SELECT BgZahlungsmodusID, MIN(Datum) AS Datum, MAX(CONVERT(int, BetragGleich)) AS BetragGleich
              FROM BgZahlungsmodusTermin
              WHERE Datum IS NOT NULL
              GROUP BY BgZahlungsmodusID) TRM3 ON TRM3.BgZahlungsmodusID = SZM.BgZahlungsmodusID
WHERE FaLeistungID = {0}
  AND (     (TRM1.BgZahlungsmodusTerminID IS NULL AND TRM2.BgZahlungsmodusTerminID IS NULL)
         OR (TRM1.BgZahlungsmodusTerminID IS NOT NULL AND TRM2.BgZahlungsmodusTerminID IS NULL)
         OR (TRM2.ImVormonat = 0 AND ((TRM1.TagImMonat &lt; TRM2.TagImMonat) OR TRM1.ImVormonat = 1))
      )
ORDER BY IsNull(CASE WHEN SZM.DatumBis &lt;= GetDate() THEN 0 ELSE 1 END, 1) DESC, SZM.NameZahlungsmodus</value>
