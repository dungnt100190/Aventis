    <ControlTemplate x:Key="{dxnt:CommonElementsThemeKey ResourceKey=ItemBorderTemplate}" TargetType="{x:Type ContentControl}">
        <Grid x:Name="grid" Margin="0,0,0,0" Background="#00FFFFFF">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup Name="CommonStates">
                    <VisualState Name="Normal" />
                    <VisualState Name="PressedState">
                        <Storyboard>
                            <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="pressed" />
                        </Storyboard>
                    </VisualState>
                    <VisualState Name="MouseOverState">
                        <Storyboard>
                            <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="hover" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="DisabledStates">
                    <VisualState x:Name="EnabledState" />
                    <VisualState x:Name="DisabledState">
                        <Storyboard>
                            <DoubleAnimation Duration="0" To=".35" Storyboard.TargetProperty="Opacity" />                            
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="SelectedStates">
                    <VisualState x:Name="Unselected" />
                    <VisualState x:Name="Selected">
                        <Storyboard>
                            <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="selected" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>                
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="selected" Width="Auto" Height="Auto" Background="#FFE5E5E5" CornerRadius="0,0,0,0" Padding="0,0,0,0" Opacity="0" />
            <Border x:Name="hover" Width="Auto" Height="Auto" CornerRadius="0,0,0,0" Padding="0,0,0,0" Opacity="0">
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FFFEE17A" Offset="0" />
                        <GradientStop Color="#FFFFCD5F" Offset="1" />
                    </LinearGradientBrush>
                </Border.Background>
            </Border>
            <Border x:Name="pressed" Width="Auto" Height="Auto" CornerRadius="0,0,0,0" Padding="0,0,0,0" Opacity="0">
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FFFBB34B" Offset="1" />
                        <GradientStop Color="#FFF7B24D" Offset="0" />
                    </LinearGradientBrush>
                </Border.Background>
            </Border>
            <ContentControl x:Name="wrapper" Tag="{Binding RelativeSource={RelativeSource TemplatedParent}}" />
            <ContentPresenter x:Name="item" />
        </Grid>
    </ControlTemplate>
