                    </VisualState>
                    <VisualState x:Name="InternalUnfocused" />
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="FocusElement" Margin="-1,0,-1,1" Height="3" VerticalAlignment="Center" Opacity="0" Background="#FFFFC200" CornerRadius="1" Visibility="{Binding Path=EditMode, Converter={StaticResource TrackBarEditFocusElementVisibilityConverter}}" />
