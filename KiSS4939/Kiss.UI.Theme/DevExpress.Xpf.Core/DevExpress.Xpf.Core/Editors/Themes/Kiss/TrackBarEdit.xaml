    <ControlTemplate x:Key="{dxet:TrackBarEditThemeKey ResourceKey=VerticalBackgroundTemplate}" TargetType="{x:Type ContentControl}">
        <Grid HorizontalAlignment="Center" Margin="0,1,0,1" x:Name="TrackBackground" VerticalAlignment="Stretch" Width="8" Height="Auto">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="InternalFocusStates">
                    <VisualState x:Name="InternalFocused">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="FocusElement" Storyboard.TargetProperty="Opacity" Duration="0" To="0.35" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="InternalUnfocused" />
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="FocusElement" Margin="0,-1,1,-1" Width="3" HorizontalAlignment="Center" Opacity="0" Background="#FFFFC200" CornerRadius="1" Visibility="{Binding Path=EditMode, Converter={StaticResource TrackBarEditFocusElementVisibilityConverter}}" />
            <Border HorizontalAlignment="Center" Margin="0,0,0,0" VerticalAlignment="Stretch" Width="2" Height="Auto" Grid.ColumnSpan="1" Background="#FF828790 " BorderBrush="#7FFFFFFF " BorderThickness="0,0,1,0" />
