    <ControlTemplate x:Key="{dxet:CalculatorThemeKey ResourceKey=DisplayTemplate}" TargetType="{x:Type ContentControl}">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal" />
                    <VisualState x:Name="MouseOver" />
                    <VisualState x:Name="Pressed" />
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="PART_Default" Storyboard.TargetProperty="Opacity" To="0" />
                            <DoubleAnimation Duration="0" Storyboard.TargetName="PART_Disabled" Storyboard.TargetProperty="Opacity" To="1" />
                            <ColorAnimation Duration="0" Storyboard.TargetName="Content" Storyboard.TargetProperty="(ContentControl.Foreground).(SolidColorBrush.Color)" To="#A05B616B" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border Background="#59FFFFFF" BorderBrush="{x:Null}" BorderThickness="0" CornerRadius="2" Padding="0,0,0,1">
                <Border x:Name="Display1" CornerRadius="2" Padding="1">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FF9399A2" Offset="0" />
                            <GradientStop Color="#FF9CA2AB" Offset="1" />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Grid Margin="0">
                        <Grid x:Name="PART_Disabled" Opacity="0">
                            <Border CornerRadius="1">
                                <Border.Background>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#FFE2E5E9" Offset="0" />
                                        <GradientStop Color="#FFF2F3F5" Offset="0.1" />
                                        <GradientStop Color="#FFFCFCFC" Offset="1" />
                                        <GradientStop Color="#FFE8EAED" Offset="0.5" />
                                        <GradientStop Color="#FFECEEF0" Offset="0.5" />
                                    </LinearGradientBrush>
                                </Border.Background>
                            </Border>
                        </Grid>
                        <Grid x:Name="PART_Default">
                            <Border CornerRadius="1">
                                <Border.Background>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#FFE0ECFF" Offset="0" />
                                        <GradientStop Color="#FFF4F8FF" Offset="0.1" />
                                        <GradientStop Color="#FFFFFFFF" Offset="1" />
                                        <GradientStop Color="#FFE9F2FF" Offset="0.5" />
                                        <GradientStop Color="#FFE4EEFF" Offset="0.5" />
                                    </LinearGradientBrush>
                                </Border.Background>
                            </Border>
                        </Grid>
                        <ContentControl x:Name="Content" Template="{DynamicResource {dxet:CalculatorThemeKey ResourceKey=DisplayContentTemplate}}" dx:FocusHelper2.Focusable="False">
                            <ContentControl.Foreground>
                                <SolidColorBrush Color="#FF5B616B" />
                            </ContentControl.Foreground>
                        </ContentControl>
                    </Grid>
                </Border>
            </Border>
        </Grid>
    </ControlTemplate>
    <Style x:Key="{dxet:CalculatorThemeKey ResourceKey=DisplayStyle}" TargetType="{x:Type ContentControl}">
        <Setter Property="dx:FocusHelper2.Focusable" Value="False" />
        <Setter Property="FontSize" Value="21" />
        <Setter Property="Margin" Value="0,0,0,10" />
        <Setter Property="Template" Value="{DynamicResource {dxet:CalculatorThemeKey ResourceKey=DisplayTemplate}}" />
    </Style>
    <ControlTemplate x:Key="{dxet:CalculatorThemeKey ResourceKey=BorderTemplate}" TargetType="{x:Type ContentControl}">
        <Grid>
            <Grid.Resources>
                <dx:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
                <dx:BoolToVisibilityInverseConverter x:Key="BoolToVisibilityInverseConverter" />
                <dx:BoolToThicknessConverter x:Key="BoolToThicknessConverter" ThicknessTrue="13,13,13,13" ThicknessFalse="11,11,11,11" />
            </Grid.Resources>
			<Border BorderThickness="0" CornerRadius="3" Padding="1" Background="#FF949494" Visibility="{Binding ShowBorder, Converter={StaticResource BoolToVisibilityConverter}}">
				<Grid>
					<Border Margin="0" BorderThickness="0" CornerRadius="2">
						<Border.Background>
							<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
								<GradientStop Color="#FFFAFBFB" Offset="0" />
								<GradientStop Color="#FFF4F4F4" Offset="1" />
							</LinearGradientBrush>
						</Border.Background>
					</Border>
					<Border Margin="1" BorderThickness="0" CornerRadius="1" Background="#FFE7E7EE">
                    </Border>
                </Grid>
            </Border>
            <Grid Background="#FFE7E7EE" Visibility="{Binding ShowBorder, Converter={StaticResource BoolToVisibilityInverseConverter}}" />
            <ContentPresenter Margin="{Binding ShowBorder, Converter={StaticResource BoolToThicknessConverter}}" />
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxet:CalculatorThemeKey ResourceKey=PopupContentContainerTemplate}" TargetType="{x:Type ContentControl}">
        <Border Margin="0" BorderThickness="0" Background="#FFE7E7EE" Padding="11">
            <ContentPresenter />
        </Border>
    </ControlTemplate>

</ResourceDictionary>