        <Setter Property="LastPadding" Value="2" />
    </Style>
    <Style x:Key="{dxt:DXTabControlInternalThemeKey ResourceKey=PanelContainerBottomLayoutStyle}" TargetType="{x:Type dx:TabPanelContainer}">
