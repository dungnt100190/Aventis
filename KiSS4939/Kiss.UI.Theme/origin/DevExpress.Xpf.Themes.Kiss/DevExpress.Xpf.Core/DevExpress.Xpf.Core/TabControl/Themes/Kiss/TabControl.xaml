                <Path x:Name="path" Stretch="Fill" Fill="#FF7A7999" Data="F1 M 1230,900L 1234,904L 1230,908L 1230,900 Z " HorizontalAlignment="Center" Height="8" Margin="2,0,0,0" UseLayoutRounding="False" VerticalAlignment="Center" Width="4" />
            </Grid>
        </Grid>
