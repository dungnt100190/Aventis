                    <Border x:Name="foneUpNormal" Grid.Row="0" BorderThickness="1,0,1,0" BorderBrush="#FF828790" Background="#FFF4F8FF" Margin="-1,1,0,0" Opacity="1" />
                    <Border x:Name="foneUpSelected" Grid.Row="0" BorderThickness="1,0,1,0" BorderBrush="#FF828790" Background="#FFFFFFFF" Margin="-1,1,0,0" Opacity="0" />
                    <Border x:Name="foneBottomNormal" Grid.Row="2" BorderThickness="1,0,1,0" BorderBrush="#FF828790" Background="#FFF4F8FF" Margin="-1,0,0,1" Opacity="1" />
                    <Border x:Name="foneBottomSelected" Grid.Row="2" BorderThickness="1,0,1,0" BorderBrush="#FF828790" Background="#FFFFFFFF" Margin="-1,0,0,1" Opacity="0" />
                    <Grid Grid.Row="1">
                        <Grid x:Name="borderContainer">
                            <Border x:Name="normalBorder" BorderThickness="1,1,1,0" BorderBrush="#FF828790" Margin="-1,0,0,0" Opacity="1" Padding="1">
                                <Border.Background>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#FFF9FCFF" Offset="0" />
                                        <GradientStop Color="#FFF4F8FF" Offset="1" />
                                        <GradientStop Color="#FFF6FAFF" Offset="0.5" />
                                    </LinearGradientBrush>
                                </Border.Background>
                                <Border x:Name="normal">
                                    <Border.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFEDF1F5" Offset="0" />
                                            <GradientStop Color="#FFEDF1F6" Offset="1" />
                                            <GradientStop Color="#FFE6EAEF" Offset="0.5" />
                                            <GradientStop Color="#FFCED5DE" Offset="0.5" />
                                        </LinearGradientBrush>
                                    </Border.Background>
                                </Border>
                            </Border>
                            <Border x:Name="selected" BorderThickness="1,1,1,0" BorderBrush="#FF828790" Margin="-1,-2,0,-1" Opacity="0" Background="#FFFFFFFF">
