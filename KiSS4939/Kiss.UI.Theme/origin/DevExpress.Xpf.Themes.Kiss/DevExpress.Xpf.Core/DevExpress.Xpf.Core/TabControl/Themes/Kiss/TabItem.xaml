                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#FFF9FCFF" Offset="0" />
                                        <GradientStop Color="#FFF4F8FF" Offset="1" />
                                        <GradientStop Color="#FFF6FAFF" Offset="0.5" />
                                    </LinearGradientBrush>
                                </Border.Background>
                                <Border x:Name="holdInsideBorderNormal">
                                    <Border.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFEDF1F5" Offset="0" />
                                            <GradientStop Color="#FFEDF1F6" Offset="1" />
                                            <GradientStop Color="#FFE6EAEF" Offset="0.5" />
                                            <GradientStop Color="#FFCED5DE" Offset="0.5" />
                                        </LinearGradientBrush>
                                    </Border.Background>
                                </Border>
                            </Border>
                            <Border x:Name="holdBorderSelected" BorderThickness="1" BorderBrush="#FF828790" Opacity="0" Background="#FFFFFFFF" Margin="1,1,0,0">
                                <Border x:Name="holdBorderFocused" Opacity="0" Margin="1,1,1,0">
                                    <Border.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFFBE297" Offset="0" />
                                            <GradientStop Color="#00FBE297" Offset="1" />
                                        </LinearGradientBrush>
                                    </Border.Background>
                                </Border>
                            </Border>
                        </Grid>
                        <Grid x:Name="gridContainer">
                            <Grid x:Name="headerContainer" Margin="{TemplateBinding Padding}" MinHeight="{TemplateBinding MinHeight}">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <dx:HeaderIconPresenter x:Name="headerIconPresenter" IsTabStop="False" Grid.Column="0" Content="{TemplateBinding Icon}" IconWidth="{TemplateBinding IconWidth}" IconHeight="{TemplateBinding IconHeight}" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="0,0,4,0" Visibility="Collapsed" />
                                <!--BEGIN WPF-->
                                <ContentPresenter Grid.Column="2" Content="{TemplateBinding Header}" ContentTemplate="{TemplateBinding HeaderTemplate}" VerticalAlignment="Center" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}">
                                    <ContentPresenter.RecognizesAccessKey>True</ContentPresenter.RecognizesAccessKey>
                                </ContentPresenter>
                                <!--END WPF-->
                                <!--ONLY SL<dx:DXContentPresenter Grid.Column="2" Content="{TemplateBinding Header}" ContentTemplate="{TemplateBinding HeaderTemplate}"
                                                                 VerticalAlignment="Center" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"/>ONLY SL-->
                                <!--BEGIN WPF-->
								<Button IsTabStop="False" x:Name="closeButton" Grid.Column="4" HorizontalAlignment="Right" VerticalAlignment="Center" Visibility="Collapsed" Command="{TemplateBinding CloseCommand}" CommandParameter="{TemplateBinding CloseCommandParameter}" CommandTarget="{TemplateBinding CloseCommandTarget}" Style="{DynamicResource {dxt:DXTabItemThemeKey ResourceKey=CloseButtonStyle}}" Margin="7,1,-4,0" />
								<!--END WPF-->
								<!--ONLY SL<Button IsTabStop="False" x:Name="closeButton" Grid.Column="4" HorizontalAlignment="Right" VerticalAlignment="Center" Visibility="Collapsed"
									    Command="{TemplateBinding CloseCommand}" CommandParameter="{TemplateBinding CloseCommandParameter}"
                                        Style="{DynamicResource {dxt:DXTabItemThemeKey ResourceKey=CloseButtonStyle}}" Margin="7,1,-4,0" />ONLY SL-->
                            </Grid>
                        </Grid>
