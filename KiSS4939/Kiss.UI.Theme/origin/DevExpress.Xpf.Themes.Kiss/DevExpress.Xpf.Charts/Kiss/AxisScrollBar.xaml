                </Grid>
            </Grid>
            <Grid x:Name="PART_VerticalRoot" Visibility="Collapsed">
                <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" />
                <Grid x:Name="PART_VerticalContainer">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <RepeatButton x:Name="PART_VerticalSmallIncrease" Grid.Row="0" Height="19" IsTabStop="False" Interval="50" Margin="1" Template="{StaticResource VerticalIncrementTemplate}" />
                    <!--BEGIN WPF-->
                    <RepeatButton x:Name="PART_VerticalLargeIncrease" Grid.Row="1" IsTabStop="False" Interval="50" Margin="0,0,1,0" Template="{StaticResource RepeatButtonTemplate}" />
                    <!--END WPF-->
                    <!--ONLY SL <RepeatButton x:Name="PART_VerticalLargeIncrease" Grid.Row="1" IsTabStop="False" Interval="50" Template="{StaticResource RepeatButtonTemplate}"/> ONLY SL-->
                    <local:AxisScrollBarThumb x:Name="PART_VerticalThumb" Grid.Row="2" MinHeight="30" Margin="1,0,1,0" Template="{StaticResource VerticalThumbTemplate}" />
                    <local:AxisScrollBarThumbResizer x:Name="PART_VerticalThumbIncrease" Grid.Row="2" MinHeight="3" VerticalAlignment="Top" Cursor="SizeNS" Template="{StaticResource VerticalThumbResizerTemplate}" />
                    <local:AxisScrollBarThumbResizer x:Name="PART_VerticalThumbDecrease" Grid.Row="2" MinHeight="3" VerticalAlignment="Bottom" Cursor="SizeNS" Template="{StaticResource VerticalThumbResizerTemplate}" />
                    <!--BEGIN WPF-->
                    <RepeatButton x:Name="PART_VerticalLargeDecrease" Grid.Row="3" IsTabStop="False" Interval="50" Margin="0,0,1,0" Template="{StaticResource RepeatButtonTemplate}" />
                    <!--END WPF-->
                    <!--ONLY SL <RepeatButton x:Name="PART_VerticalLargeDecrease" Grid.Row="3" IsTabStop="False" Interval="50" Template="{StaticResource RepeatButtonTemplate}"/> ONLY SL-->
                    <RepeatButton x:Name="PART_VerticalSmallDecrease" Grid.Row="4" Height="19" IsTabStop="False" Interval="50" Margin="1" Template="{StaticResource VerticalDecrementTemplate}" />
                </Grid>
            </Grid>
        </Grid>
    </ControlTemplate>
</ResourceDictionary>