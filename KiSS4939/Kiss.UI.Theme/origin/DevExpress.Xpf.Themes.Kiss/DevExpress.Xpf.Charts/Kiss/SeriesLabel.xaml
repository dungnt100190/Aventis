<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/charts/themekeys" xmlns:local="http://schemas.devexpress.com/winfx/2008/xaml/charts">

    <SolidColorBrush x:Key="{dxt:ChartBrushesThemeKey ResourceKey=SeriesLabelForeground}" Color="#FFFFFFFF" />
    <local:BrushOverlayConverter x:Key="brushConverter" />
    <DataTemplate x:Key="{dxt:ChartControlThemeKey ResourceKey=SeriesLabelTemplate}">
        <Border Padding="5,1" CornerRadius="2" Background="{Binding Path=Color, Converter={StaticResource brushConverter}}">
            <TextBlock Text="{Binding Text}" />
        </Border>
    </DataTemplate>
</ResourceDictionary>