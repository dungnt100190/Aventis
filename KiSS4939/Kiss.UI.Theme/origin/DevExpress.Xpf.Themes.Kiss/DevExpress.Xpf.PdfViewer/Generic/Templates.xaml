    <ControlTemplate TargetType="{x:Type pdfViewer:PdfPresenterControl}" x:Key="{themes:PdfViewerThemeKey ResourceKey=PdfPresenterControlTemplate}">
        <Grid Background="{DynamicResource {themes:PdfViewerThemeKey ResourceKey=PdfPresenterControlBackgroundBrush}}">
