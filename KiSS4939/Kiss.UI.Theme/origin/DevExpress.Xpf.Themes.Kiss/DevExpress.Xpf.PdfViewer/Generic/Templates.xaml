                <pdfViewer:ClockwiseRotateBarItem pdfViewer:PdfViewerControl.BarItemName="{x:Static pdfViewer:DefaultPdfBarManagerItemNames.ClockwiseRotate}" Command="{Binding Path=(pdfViewer:PdfViewerControl.ActualViewer).ActualCommandProvider.ClockwiseRotateCommand, RelativeSource={RelativeSource TemplatedParent}}" />
