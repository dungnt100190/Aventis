    <ControlTemplate TargetType="{x:Type pdfViewer:SearchControlContainer}" x:Key="{themes:PdfViewerThemeKey ResourceKey=SearchControlContainerTemplate}">
        <dxb:BarManager CreateStandardLayout="False" AllowNavigationFromEditorOnTabPress="True">
            <dxb:BarManager.Items>
