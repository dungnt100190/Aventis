        </dxb:BarManager>
    </ControlTemplate>
    <ControlTemplate TargetType="{x:Type pdfViewer:SelectionCanvas}" x:Key="{themes:PdfViewerThemeKey ResourceKey=SelectionCanvasTemplate}">
