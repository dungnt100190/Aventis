                    <pdfViewer:PdfPresenterControl BehaviorProvider="{Binding Path=(pdfViewer:PdfViewerControl.ActualViewer).ActualBehaviorProvider, RelativeSource={RelativeSource Self}}" Document="{Binding Path=(pdfViewer:PdfViewerControl.ActualViewer).Document, RelativeSource={RelativeSource Self}}" AllowCachePages="{Binding Path=(pdfViewer:PdfViewerControl.ActualViewer).AllowCachePages, RelativeSource={RelativeSource Self}}" CacheSize="{Binding Path=(pdfViewer:PdfViewerControl.ActualViewer).CacheSize, RelativeSource={RelativeSource Self}}" IsSearchControlVisible="{Binding Path=(pdfViewer:PdfViewerControl.ActualViewer).IsSearchControlVisible, Mode=OneWay, RelativeSource={RelativeSource Self}}">
