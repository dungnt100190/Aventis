                <pdfViewer:StartScreenControl RecentFiles="{Binding Path=(pdfViewer:PdfViewerControl.ActualViewer).RecentFiles, RelativeSource={RelativeSource Self}}" NumberOfRecentFiles="{Binding Path=(pdfViewer:PdfViewerControl.ActualViewer).NumberOfRecentFiles, RelativeSource={RelativeSource Self}}" ShowOpenFileButton="{Binding Path=(pdfViewer:PdfViewerControl.ActualViewer).ShowOpenFileOnStartScreen, RelativeSource={RelativeSource Self}}" Visibility="{Binding Path=(pdfViewer:PdfViewerControl.ActualViewer).IsStartScreenVisible, RelativeSource={RelativeSource Self}, Converter={StaticResource booleanToVisibilityConverter}}" />
