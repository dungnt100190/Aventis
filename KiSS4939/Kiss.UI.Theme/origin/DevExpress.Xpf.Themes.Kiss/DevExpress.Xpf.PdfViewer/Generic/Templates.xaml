    <ControlTemplate TargetType="{x:Type pdfViewer:StartScreenControl}" x:Key="{themes:PdfViewerThemeKey ResourceKey=StartScreenControlTemplate}">
        <Grid>
            <Border Background="{DynamicResource {themes:PdfViewerThemeKey ResourceKey=PdfPresenterControlBackgroundBrush}}">
