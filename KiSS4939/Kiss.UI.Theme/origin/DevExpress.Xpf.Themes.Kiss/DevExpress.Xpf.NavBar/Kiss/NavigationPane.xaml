                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FFFFD9AA" Offset="0" />
                        <GradientStop Color="#FFFEE17A" Offset="1" />
                        <GradientStop Color="#FFFFBB6E" Offset="0.5" />
                        <GradientStop Color="#FFFFAB3F" Offset="0.5" />
                    </LinearGradientBrush>
                </Border.Background>
