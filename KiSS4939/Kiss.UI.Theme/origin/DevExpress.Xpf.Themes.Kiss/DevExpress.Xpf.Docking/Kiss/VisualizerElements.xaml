                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- DockVisualizer Theme Elements -->
    <ControlTemplate x:Key="{dxt:DockVisualizerThemeKey ResourceKey=RenameHint}" TargetType="dxp:RenameHint">
        <Grid>
            <Border Background="#26000000" CornerRadius="2" Margin="2,2,0,0" />
            <Border Background="#3F000000" CornerRadius="2" Margin="1,1,1,1">
                <Border Background="White" Margin="1" CornerRadius="1" Padding="2,0,2,0">
                    <TextBox x:Name="PART_Text" BorderThickness="0" />
                </Border>
            </Border>
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxt:DockVisualizerThemeKey ResourceKey=Selection}" TargetType="dxp:SelectionHint">
        <Grid>
            <Rectangle x:Name="PART_Selection" Stretch="Fill" Fill="#FF1D4ABD" Opacity="0.15" RadiusX="3" RadiusY="3" IsHitTestVisible="False" />
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="{dxt:DockVisualizerThemeKey ResourceKey=DockZone}">
        <Grid Opacity="0.75" Margin="1" SnapsToDevicePixels="True">
            <Border x:Name="border" BorderBrush="{StaticResource BrushHot}" BorderThickness="3">
                <Border Background="{StaticResource BrushNormal}" />
            </Border>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxt:DockVisualizerThemeKey ResourceKey=Left}">
        <Grid Width="34" Height="34" HorizontalAlignment="Left" VerticalAlignment="Center" dxp:DockHintElement.DockType="Left" SnapsToDevicePixels="True">
            <Grid.Effect>
                <DropShadowEffect x:Name="Shadow" Direction="270" ShadowDepth="1" BlurRadius="3" Opacity="0.35" />
            </Grid.Effect>
            <Border Background="#FF25262F" CornerRadius="2">
                <Grid Margin="3,7,3,3">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <ve:DockHintButton x:Name="PART_Dock" Style="{StaticResource button-left}" Grid.Column="1" />
                    <ve:DockHintButton x:Name="PART_Hide" Style="{StaticResource button-lefthide}" Grid.Column="0" />
                </Grid>
            </Border>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxt:DockVisualizerThemeKey ResourceKey=Right}">
        <Grid Width="34" Height="34" HorizontalAlignment="Right" VerticalAlignment="Center" dxp:DockHintElement.DockType="Right" SnapsToDevicePixels="True">
            <Grid.Effect>
                <DropShadowEffect x:Name="Shadow" Direction="270" ShadowDepth="1" BlurRadius="3" Opacity="0.35" />
            </Grid.Effect>
            <Border Background="#FF25262F" CornerRadius="2">
                <Grid Margin="3,7,3,3">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>
                    <ve:DockHintButton x:Name="PART_Hide" Style="{StaticResource button-righthide}" Grid.Column="1" />
                    <ve:DockHintButton x:Name="PART_Dock" Style="{StaticResource button-right}" Grid.Column="0" />
                 </Grid>
            </Border>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxt:DockVisualizerThemeKey ResourceKey=Top}">
        <Grid Width="34" Height="34" HorizontalAlignment="Center" VerticalAlignment="Top" dxp:DockHintElement.DockType="Top" SnapsToDevicePixels="True">
            <Grid.Effect>
                <DropShadowEffect x:Name="Shadow" Direction="270" ShadowDepth="1" BlurRadius="3" Opacity="0.35" />
            </Grid.Effect>
            <Border Background="#FF25262F" CornerRadius="2">
                <Grid Margin="3,7,3,3">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>
                    <ve:DockHintButton x:Name="PART_Hide" Style="{StaticResource button-tophide}" Grid.Row="0" />
                    <ve:DockHintButton x:Name="PART_Dock" Style="{StaticResource button-top}" Grid.Row="1" />
                 </Grid>
            </Border>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxt:DockVisualizerThemeKey ResourceKey=Bottom}">
        <Grid Width="34" Height="34" HorizontalAlignment="Center" VerticalAlignment="Bottom" dxp:DockHintElement.DockType="Bottom" SnapsToDevicePixels="True">
            <Grid.Effect>
                <DropShadowEffect x:Name="Shadow" Direction="270" ShadowDepth="1" BlurRadius="3" Opacity="0.35" />
            </Grid.Effect>
            <Border Background="#FF25262F" CornerRadius="2">
                <Grid Margin="3,7,3,3">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <ve:DockHintButton x:Name="PART_Dock" Style="{StaticResource button-bottom}" Grid.Row="0" />
                    <ve:DockHintButton x:Name="PART_Hide" Style="{StaticResource button-bottomhide}" Grid.Row="1" />
                 </Grid>
            </Border>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxt:DockVisualizerThemeKey ResourceKey=Center}">
        <Grid x:Name="center_button" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Grid.Effect>
                <DropShadowEffect x:Name="Shadow" Direction="270" ShadowDepth="1" BlurRadius="3" Opacity="0.35" />
            </Grid.Effect>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="34" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="34" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>

            <!-- Cross -->
            <Ellipse Fill="#FF25262F" StrokeThickness="0" Margin="-22" Grid.Column="1" Grid.Row="1" />

            <!-- Content -->
            <Border x:Name="gridTop" Background="#FF25262F" CornerRadius="2" HorizontalAlignment="Center" VerticalAlignment="Bottom" Grid.Column="1" Grid.Row="0">
                <Grid Margin="3,3,3,0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <ve:DockHintButton x:Name="PART_Top" Width="28" Height="28" Margin="0,0,0,3" Style="{StaticResource button-top-3}" Grid.Row="0" dxp:DockHintElement.DockType="Top" />
                    <ve:DockHintButton x:Name="PART_TabTop" Width="28" Height="28" Style="{StaticResource button-top-2}" Grid.Row="1" dxp:DockHintElement.DockType="Top" />
                </Grid>
            </Border>

            <Border x:Name="gridBottom" Background="#FF25262F" CornerRadius="2" HorizontalAlignment="Center" VerticalAlignment="Bottom" Grid.Column="1" Grid.Row="2">
                <Grid Margin="3,0,3,3">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <ve:DockHintButton x:Name="PART_TabBottom" Width="28" Height="28" Style="{StaticResource button-bottom-2}" Grid.Row="0" dxp:DockHintElement.DockType="Bottom" />
                    <ve:DockHintButton x:Name="PART_Bottom" Width="28" Height="28" Margin="0,3,0,0" Style="{StaticResource button-bottom-3}" Grid.Row="1" dxp:DockHintElement.DockType="Bottom" />
                </Grid>
            </Border>

            <Border x:Name="gridLeft" Background="#FF25262F" CornerRadius="2" HorizontalAlignment="Right" VerticalAlignment="Center" Grid.Column="0" Grid.Row="1">
                <Grid Margin="3,3,0,3">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>
                    <ve:DockHintButton x:Name="PART_Left" Width="28" Height="28" Margin="0,0,3,0" Style="{StaticResource button-left-3}" Grid.Column="0" dxp:DockHintElement.DockType="Left" />
                    <ve:DockHintButton x:Name="PART_TabLeft" Width="28" Height="28" Style="{StaticResource button-left-2}" Grid.Column="1" dxp:DockHintElement.DockType="Left" />
                </Grid>
            </Border>

            <Border x:Name="gridRight" Background="#FF25262F" CornerRadius="2" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Column="2" Grid.Row="1">
                <Grid Margin="0,3,3,3">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>
                    <ve:DockHintButton x:Name="PART_TabRight" Width="28" Height="28" Style="{StaticResource button-right-2}" Grid.Column="0" dxp:DockHintElement.DockType="Right" />
                    <ve:DockHintButton x:Name="PART_Right" Width="28" Height="28" Margin="3,0,0,0" Style="{StaticResource button-right-3}" Grid.Column="1" dxp:DockHintElement.DockType="Right" />
                </Grid>
            </Border>

            <ve:DockHintButton x:Name="PART_Fill" Width="28" Height="28" HorizontalAlignment="Center" VerticalAlignment="Center" Style="{StaticResource button-center}" Grid.Column="1" Grid.Row="1" dxp:DockHintElement.DockType="Fill" />

        </Grid>
    </ControlTemplate>
    <!-- DockVisualizer Theme Elements -->
    <!--BEGIN WPF-->
    <!-- DocumentSelector Theme Elements -->
    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Docking.VisualElements.DocumentSelectorContainer}" TargetType="{x:Type ve:DocumentSelectorContainer}">
        <Style.Setters>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ve:DocumentSelectorContainer}">
                        <Grid Background="{x:Null}">
                            <Border x:Name="shadow" Background="Black" CornerRadius="7" Margin="6,6,0,0" Opacity="0.15" />
                            <Border x:Name="window" Background="#60000000" CornerRadius="6" Margin="3">
                                <Grid Margin="1">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <Border x:Name="header" CornerRadius="5,5,0,0" BorderThickness="0,0,0,1" BorderBrush="#FFD0D0E1">
                                        <Border.Background>
                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                <GradientStop Color="#FFE9E9FA" Offset="0" />
                                                <GradientStop Color="#FFDDDDEE" Offset="1" />
                                            </LinearGradientBrush>
                                        </Border.Background>
                                        <StackPanel VerticalAlignment="Center" Orientation="Horizontal" HorizontalAlignment="Left" Margin="12,8,12,8">
                                            <Image VerticalAlignment="Top" HorizontalAlignment="Left" Margin="0,0,6,0" Stretch="None" Source="{Binding Path=(local:DockLayoutManager.LayoutItem).CaptionImage}" />
                                            <StackPanel>
                                                <TextBlock TextWrapping="Wrap" VerticalAlignment="Bottom" HorizontalAlignment="Left" FontWeight="Bold" Text="{Binding Path=(local:DockLayoutManager.LayoutItem).Caption}" />
                                                <TextBlock TextWrapping="Wrap" HorizontalAlignment="Left" VerticalAlignment="Top" Text="{Binding Path=(local:DockLayoutManager.LayoutItem).Description}" />
                                            </StackPanel>
                                        </StackPanel>
                                    </Border>
                                    <Border x:Name="content" Background="White" Grid.Row="1">
                                        <ContentPresenter x:Name="PART_ContainerContent" Content="{Binding}" Margin="12" />
                                    </Border>
                                    <Border x:Name="footer" MinHeight="25" Background="#FFE1E1F2" CornerRadius="0,0,5,5" Grid.Row="2" BorderThickness="0,1,0,0" BorderBrush="#FFD0D0E1">
                                        <ContentPresenter HorizontalAlignment="Left" VerticalAlignment="Top" Margin="12,3,12,5" Content="{Binding Path=(local:DockLayoutManager.LayoutItem).FooterDescription}" />
                                    </Border>
                                </Grid>
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>
    <ControlTemplate x:Key="{dxt:DocumentSelectorElementsThemeKey ResourceKey=ListBoxItem}" TargetType="{x:Type ListBoxItem}">
        <Grid Background="#00FFFFFF">
            <Border x:Name="hover" Visibility="Hidden" CornerRadius="2" Padding="1">
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FFDBCE99" Offset="0" />
                        <GradientStop Color="#FFBBA67F" Offset="1" />
                        <GradientStop Color="#FFB9A074" Offset="0.5" />
                    </LinearGradientBrush>
                </Border.Background>
                <Border x:Name="border2" Visibility="Visible" CornerRadius="1" Padding="1">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FFFFFFFD" Offset="0" />
                            <GradientStop Color="#FFFFF9E0" Offset="1" />
                            <GradientStop Color="#FFFFF1BA" Offset="0.5" />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border x:Name="border1" CornerRadius="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFFFFDF2" Offset="0" />
                                <GradientStop Color="#FFFFEEAE" Offset="1" />
                                <GradientStop Color="#FFFFEA9A" Offset="0.5" />
                                <GradientStop Color="#FFFFDC58" Offset="0.5" />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Border>
            <Border x:Name="selected" Visibility="Hidden" CornerRadius="2" Padding="1">
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FF9F7559" Offset="0" />
                        <GradientStop Color="#FFC6C0B1" Offset="1" />
                        <GradientStop Color="#FF8E8165" Offset="0.1" />
                    </LinearGradientBrush>
                </Border.Background>
                <Border x:Name="border3" Visibility="Visible" CornerRadius="1" Padding="1">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FFBB9F7C" Offset="0" />
                            <GradientStop Color="#FFFFDE70" Offset="1" />
                            <GradientStop Color="#FFF8AD61" Offset="0.5" />
                            <GradientStop Color="#FFF7952F" Offset="0.5" />
                            <GradientStop Color="#FFFDD3A8" Offset="0.1" />
                        </LinearGradientBrush>
                    </Border.Background>
                </Border>
            </Border>
            <Border SnapsToDevicePixels="True" BorderThickness="0" Padding="{TemplateBinding Padding}">
                <ve:CaptionControl DataContext="{Binding}" Margin="3,2,3,2" />
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="Visibility" TargetName="selected" Value="Visible" />
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Visibility" TargetName="hover" Value="Visible" />
            </Trigger> 
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <DataTemplate x:Key="{dxt:DockPaneElementsThemeKey ResourceKey=ContentPreview}">
        <Grid SnapsToDevicePixels="True" Margin="0,0,-2,0" VerticalAlignment="Top">
            <Border x:Name="shadow1" Background="#FF000000" Margin="2,2,0,0" Opacity="0.15" />
            <Border Margin="0,0,2,2" Padding="12" Background="#FFFFFFFF" BorderBrush="#FF919191" BorderThickness="1">
                <ContentPresenter Content="{TemplateBinding Content}" />
            </Border>
        </Grid>
    </DataTemplate>
    <DataTemplate x:Key="{dxt:DocumentElementsThemeKey ResourceKey=ContentPreview}">
        <Grid SnapsToDevicePixels="True" Margin="0,0,-2,0" VerticalAlignment="Top">
            <Border x:Name="shadow1" Background="#FF000000" Margin="2,2,0,0" Opacity="0.15" />
            <Border Margin="0,0,2,2" Padding="1" Background="#FFFFFFFF" BorderBrush="#FF919191" BorderThickness="1">
                <ContentPresenter Content="{TemplateBinding Content}" />
            </Border>
        </Grid>
    </DataTemplate>
