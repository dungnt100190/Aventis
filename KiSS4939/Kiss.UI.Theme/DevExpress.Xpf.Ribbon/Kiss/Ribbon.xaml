    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=SelectedPageBorderTemplateInPopup}" TargetType="{x:Type ContentControl}">

        <Border x:Name="MainBorder" Margin="0,0,0,0">
            <Border.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FFD0D4DD" Offset="0.75" />
                    <GradientStop Color="#00b4b8bd" Offset="1" />
                </LinearGradientBrush>
            </Border.Background>
            <Border x:Name="Border1" CornerRadius="4" Margin="1,0,1,0">
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#7FC6CBD1" Offset="1" />
                        <GradientStop Color="#FFC6CBD1" />
                    </LinearGradientBrush>
                </Border.Background>
                <Border Margin="0,0,0,1" CornerRadius="3">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FFBEBEBE" Offset="0" />
                            <GradientStop Color="#FFB0B0B0" Offset="1" />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border BorderThickness="0" Margin="1" CornerRadius="2">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFf3f5f9" Offset="0" />
                                <GradientStop Color="#FFf0fafb" Offset="1" />
                                <GradientStop Color="#FFe1e6ee" Offset="0.25" />
                                <GradientStop Color="#FFdbdee4" Offset="0.25" />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Border>
        </Border>

        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding Path=(interop:BrowserInteropHelper.IsBrowserHosted)}" Value="True">
                <Setter Property="Background" TargetName="MainBorder">
                    <Setter.Value>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FFD0D4DD" Offset="0.75" />
                            <GradientStop Color="#FFb4b8bd" Offset="1" />
                        </LinearGradientBrush>
                    </Setter.Value>
                </Setter>
                <Setter Property="Margin" TargetName="Border1" Value="1,0,1,0" />
            </DataTrigger>
        </ControlTemplate.Triggers>
