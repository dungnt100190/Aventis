    <!-- office2007Blue: Footer Quick Access Toolbar Control -->
    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=FooterQuickAccessToolbarContainerTemplate}" TargetType="ContentControl">
        <Grid>
            <Border SnapsToDevicePixels="True">
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FFD0D4DD" Offset="0" />
                        <GradientStop Color="#00D0D4DD" Offset="1" />
                    </LinearGradientBrush>
                </Border.Background>
            </Border>
            <Border SnapsToDevicePixels="True" CornerRadius="2" BorderThickness="1" Margin="1,1,0,0">
                <Border.BorderBrush>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FFEDEFF3" Offset="0" />
                        <GradientStop Color="#FFE7E9ED" Offset="1" />
                    </LinearGradientBrush>
                </Border.BorderBrush>
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FFD9DEE6" Offset="0" />
                        <GradientStop Color="#FFD6DBE3" Offset="1" />
                    </LinearGradientBrush>
                </Border.Background>
            </Border>
            <Border SnapsToDevicePixels="True" x:Name="Border" BorderBrush="#FFC2C9D4" BorderThickness="1" Background="{x:Null}" CornerRadius="2" Margin="0,0,1,1" />
            <ContentPresenter HorizontalAlignment="Left" Margin="2" />
        </Grid>
    </ControlTemplate>
    <!-- R/L Repeat Buttons -->
    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=RightRepeatButtonTemplate}" TargetType="{x:Type RepeatButton}">
