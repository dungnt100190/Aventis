    <ControlTemplate x:Key="{dxrt:RibbonPageGroupThemeKey ResourceKey=HighlightedBorderTemplate}" TargetType="{x:Type ContentControl}">
        <!--BEGIN WPF-->
        <ControlTemplate.Resources>
            <Storyboard x:Key="hoverIn">
                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="Opacity" Storyboard.TargetName="hover">
                    <EasingDoubleKeyFrame KeyTime="0:0:0" Value="0">
                        <EasingDoubleKeyFrame.EasingFunction>
                            <BackEase EasingMode="EaseOut" />
                        </EasingDoubleKeyFrame.EasingFunction>
                    </EasingDoubleKeyFrame>
                    <EasingDoubleKeyFrame KeyTime="0:0:2" Value="1">
                        <EasingDoubleKeyFrame.EasingFunction>
                            <BackEase EasingMode="EaseOut" />
                        </EasingDoubleKeyFrame.EasingFunction>
                    </EasingDoubleKeyFrame>
                </DoubleAnimationUsingKeyFrames>
            </Storyboard>
            <Storyboard x:Key="hoverOut">
                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="Opacity" Storyboard.TargetName="hover">
                    <EasingDoubleKeyFrame KeyTime="0" Value="1">
                        <EasingDoubleKeyFrame.EasingFunction>
                            <BackEase EasingMode="EaseOut" />
                        </EasingDoubleKeyFrame.EasingFunction>
                    </EasingDoubleKeyFrame>
                    <EasingDoubleKeyFrame KeyTime="0:0:2" Value="0">
                        <EasingDoubleKeyFrame.EasingFunction>
                            <BackEase EasingMode="EaseOut" />
                        </EasingDoubleKeyFrame.EasingFunction>
                    </EasingDoubleKeyFrame>
                </DoubleAnimationUsingKeyFrames>
            </Storyboard>
        </ControlTemplate.Resources>
        <!--END WPF-->
        <Grid Background="Transparent" Margin="0,0,3,0" SnapsToDevicePixels="True">
            <Grid x:Name="default">
                <Border Height="17" Margin="1,0,1,1" VerticalAlignment="Bottom" CornerRadius="0,0,1,1">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FFC3C7D1" Offset="1" />
                            <GradientStop Color="#FFDFE3EF" Offset="0" />
                        </LinearGradientBrush>
                    </Border.Background>
                </Border>
                <Border BorderThickness="1" BorderBrush="#1FFFFFFF" Margin="1,1,-1,-1" CornerRadius="1,1,4,4" />
                <Border BorderThickness="1" CornerRadius="2">
                    <Border.BorderBrush>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="{Binding Path=DataContext, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource colorOverlayConverter}, ConverterParameter=#FFB0B0B0}" />
                            <GradientStop Color="#FF818181" Offset="1" />
                        </LinearGradientBrush>
                    </Border.BorderBrush>
                </Border>
            </Grid>
            <Grid x:Name="hover" Opacity="0">
                <Border Background="#BFFFFFFF" BorderThickness="1" CornerRadius="1" Margin="1" Opacity="0.75">
                    <Border.BorderBrush>
                        <LinearGradientBrush EndPoint="0,0" StartPoint="1,1">
                            <GradientStop Color="#FFFFFFFF" Offset="1" />
                            <GradientStop Color="#EFFFFFFF" Offset="0" />
                        </LinearGradientBrush>
                    </Border.BorderBrush>
                </Border>                                
                <Border Height="17" Margin="1,0,1,1" VerticalAlignment="Bottom" CornerRadius="0,0,1,1">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FFB3B9C7" Offset="1" />
                            <GradientStop Color="#FFDEE2EE" Offset="0" />
                        </LinearGradientBrush>
                    </Border.Background>
                </Border>
                <Border BorderThickness="1" CornerRadius="2">
                    <Border.BorderBrush>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="{Binding Path=DataContext, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource colorOverlayConverter}, ConverterParameter=#FF999999}" />
                            <GradientStop Color="#FF818181" Offset="1" />
                        </LinearGradientBrush>
                    </Border.BorderBrush>
                </Border>
            </Grid>
            <ContentPresenter /> 
        </Grid>
        <!--BEGIN WPF-->
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Trigger.ExitActions>
                    <BeginStoryboard x:Name="hoverOut_BeginStoryboard" Storyboard="{StaticResource hoverOut}" />
                    <StopStoryboard BeginStoryboardName="hoverIn_BeginStoryboard" />
                </Trigger.ExitActions>
                <Trigger.EnterActions>
                    <BeginStoryboard x:Name="hoverIn_BeginStoryboard" Storyboard="{StaticResource hoverIn}" />
                    <StopStoryboard BeginStoryboardName="hoverOut_BeginStoryboard" />
                </Trigger.EnterActions>
            </Trigger>
        </ControlTemplate.Triggers>
        <!--END WPF-->
    </ControlTemplate>
