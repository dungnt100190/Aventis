        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="GalleryBorder" Property="Background" Value="#fff1f1f1" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
