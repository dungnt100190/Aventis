    <ControlTemplate x:Key="{dxrt:InRibbonGalleryThemeKey ResourceKey=GalleryItemGlyphBorderTemplate}" TargetType="{x:Type dxb:ItemBorderControl}">
        <Border Name="PART_Border" BorderBrush="Transparent">
            <ContentPresenter />
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="State" Value="Checked">
                <Setter TargetName="PART_Border" Property="BorderBrush" Value="#DFFFFC" />
                <Setter TargetName="PART_Border" Property="BorderThickness" Value="4" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <!-- office2007Blue: RibbonGallery UpButtonTemplate -->
    <ControlTemplate x:Key="{dxrt:InRibbonGalleryThemeKey ResourceKey=UpButtonTemplate}" TargetType="{x:Type Button}">
        <Grid x:Name="Button" Width="15" Margin="0,-1,-1,-1">
            <Border x:Name="PART_Normal" SnapsToDevicePixels="True" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Margin="0" CornerRadius="0,3,0,0" Background="#FFaab2b8">
                <Border SnapsToDevicePixels="True" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" x:Name="border4" Margin="1" CornerRadius="0,2,0,0" Background="#FFdadde1">
                    <Border SnapsToDevicePixels="True" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" x:Name="border5" BorderBrush="{x:Null}" BorderThickness="0" CornerRadius="0,1,0,0" Margin="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFdbdfe4" Offset="0" />
                                <GradientStop Color="#FFcacfd4" Offset="1" />
                                <GradientStop Color="#FFd8dde2" Offset="0.5" />
                                <GradientStop Color="#FFb4b9c2" Offset="0.5" />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Border>
            <Border x:Name="PART_MouseOver" Visibility="Collapsed" SnapsToDevicePixels="True" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Margin="0" CornerRadius="0,3,0,0" Background="#FFb7a27e">
                <Border SnapsToDevicePixels="True" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" x:Name="border1" Margin="1" CornerRadius="0,2,0,0" Background="#FFFDF5DA">
                    <Border SnapsToDevicePixels="True" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" x:Name="border2" BorderBrush="{x:Null}" BorderThickness="0" CornerRadius="0,1,0,0" Margin="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFFEF7D5" Offset="0" />
                                <GradientStop Color="#FFFFE59F" Offset="1" />
                                <GradientStop Color="#FFFAE5A8" Offset="0.5" />
                                <GradientStop Color="#FFFFD049" Offset="0.5" />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Border>
            <Border x:Name="PART_Pressed" Visibility="Collapsed" SnapsToDevicePixels="True" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Margin="0" CornerRadius="0,3,0,0">
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FF7B6645" Offset="0" />
                        <GradientStop Color="#FFA78E66" Offset="1" />
                    </LinearGradientBrush>
                </Border.Background>
                <Border SnapsToDevicePixels="True" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" x:Name="border3" Margin="1" CornerRadius="0,2,0,0">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FFCC9666" Offset="0" />
                            <GradientStop Color="#FFFFB31B" Offset="1" />
                            <GradientStop Color="#FFFCB16D" Offset="0.2" />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border SnapsToDevicePixels="True" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" x:Name="border6" BorderBrush="{x:Null}" BorderThickness="0" CornerRadius="0,1,0,0" Margin="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#14FCB16D" Offset="0" />
                                <GradientStop Color="#FFFFC450" Offset="1" />
                                <GradientStop Color="#FFFF8D05" Offset="0.35" />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
                </Border>
            </Border>
            <Border x:Name="PART_Disabled" Visibility="Collapsed" SnapsToDevicePixels="True" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Margin="0" CornerRadius="0,3,0,0" Background="#FFD4D4D4">
                <Border SnapsToDevicePixels="True" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Margin="1" CornerRadius="0,2,0,0" Background="#FFF1F3F5">
                    <Border SnapsToDevicePixels="True" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" BorderBrush="{x:Null}" BorderThickness="0" CornerRadius="0,1,0,0" Margin="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFF0F3F6" Offset="0" />
                                <GradientStop Color="#FFE8EBEF" Offset="1" />
                                <GradientStop Color="#FFEAEDF1" Offset="0.5" />
                                <GradientStop Color="#FFE0E4E8" Offset="0.5" />
                            </LinearGradientBrush>
                        </Border.Background>
                    </Border>
