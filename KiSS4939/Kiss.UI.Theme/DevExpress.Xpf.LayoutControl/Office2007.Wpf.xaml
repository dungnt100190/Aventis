<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:lc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Office2007.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.LayoutControl.LayerSeparator}" TargetType="{x:Type lc:LayerSeparator}" BasedOn="{StaticResource Office2007.LayerSeparator}" />

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.LayoutControl.ElementSizer}" TargetType="{x:Type lc:ElementSizer}" BasedOn="{StaticResource Office2007.ElementSizer}" />

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.LayoutControl.GroupBoxButton}" TargetType="{x:Type lc:GroupBoxButton}" BasedOn="{StaticResource Office2007.GroupBoxButton}" />

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.LayoutControl.GroupBox}" TargetType="{x:Type lc:GroupBox}" BasedOn="{StaticResource Office2007.GroupBox}">
        <Setter Property="Background" Value="Transparent" />
    </Style>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.LayoutControl.GroupSeparator}" TargetType="{x:Type lc:GroupSeparator}" BasedOn="{StaticResource Office2007.GroupSeparator}" />
</ResourceDictionary>