<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:local="http://schemas.devexpress.com/winfx/2008/xaml/docking" xmlns:system="clr-namespace:System;assembly=mscorlib" xmlns:collections="clr-namespace:System.Collections;assembly=mscorlib" xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars" xmlns:ve="http://schemas.devexpress.com/winfx/2008/xaml/docking/visualelements" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/docking/themekeys" xmlns:dxp="http://schemas.devexpress.com/winfx/2008/xaml/docking/platform" xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys">
	<!-- MDIButton -->
	<ControlTemplate x:Key="{dxt:MDIButtonElementsThemeKey ResourceKey=ButtonBorder}" TargetType="dxb:ItemBorderControl">
		<Grid Height="18" Width="18">
            <Grid x:Name="PART_Hover" Opacity="0.0">
                <Border x:Name="hover" Background="#FF7A7999" CornerRadius="2">
                    <Border CornerRadius="1" Margin="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFF6F6F8" Offset="0" />
                                <GradientStop Color="#FFE6E6ED" Offset="1" />
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border CornerRadius="1" Margin="1">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                    <GradientStop Color="#FFF0F0F4" Offset="0" />
                                    <GradientStop Color="#FFDADAE5" Offset="1" />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="PART_Pressed" Opacity="0.0">
                <Border x:Name="pressed" Background="#FF7A7999" CornerRadius="2">
                    <Border CornerRadius="1" Margin="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFE6E6ED" Offset="1" />
                                <GradientStop Color="#FFF6F6F8" />
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border CornerRadius="1" Margin="1">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                    <GradientStop Color="#FFF0F0F4" Offset="1" />
                                    <GradientStop Color="#FFDADAE5" />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
			<VisualStateManager.VisualStateGroups>
				<VisualStateGroup x:Name="State">
                    <VisualState x:Name="Normal" />
                    <VisualState x:Name="Hover">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetProperty="(Control.Opacity)" Storyboard.TargetName="PART_Hover" To="1" Duration="0" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetProperty="(Control.Opacity)" Storyboard.TargetName="PART_Pressed" To="1" Duration="0" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Checked" />
					<VisualState x:Name="HoverChecked" />
					<VisualState x:Name="Focused" />
					<VisualState x:Name="Customization" />
				</VisualStateGroup>
			</VisualStateManager.VisualStateGroups>
		</Grid>
	</ControlTemplate>
    <Style x:Key="{dxt:MDIButtonElementsThemeKey ResourceKey=ButtonBorderStyle}" TargetType="dxb:ItemBorderControl">
        <Setter Property="Template" Value="{DynamicResource {dxt:MDIButtonElementsThemeKey ResourceKey=ButtonBorder}}" />
    </Style>
	<!-- Glyphs -->
    <DataTemplate x:Key="{dxt:GlyphElementsThemeKey ResourceKey=MDIButtonClose}">
        <Border Background="Transparent" Margin="-4">
            <Path x:Name="path" Stretch="Fill" Fill="#FF7A7999" Data="F1 M 1220,881L 1222,881L 1218,885L 1222,889L 1220,889L 1217,886L 1214,889L 1212,889L 1216,885L 1212,881L 1214,881L 1217,884L 1220,881 Z " HorizontalAlignment="Center" Height="8" Margin="0" UseLayoutRounding="False" VerticalAlignment="Center" Width="10" />
        </Border>
        <DataTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Fill" TargetName="path" Value="#FF000000" />
            </Trigger>
        </DataTemplate.Triggers>
    </DataTemplate>
    <DataTemplate x:Key="{dxt:GlyphElementsThemeKey ResourceKey=MDIButtonMinimize}">
        <Border Background="Transparent" Margin="-4">
            <Rectangle x:Name="path" Fill="#FF7A7999" Height="2" Width="10" UseLayoutRounding="True" HorizontalAlignment="Center" Margin="0,8,0,0" VerticalAlignment="Center" />
        </Border>
        <DataTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Fill" TargetName="path" Value="#FF000000" />
            </Trigger>
        </DataTemplate.Triggers>
    </DataTemplate>
    <DataTemplate x:Key="{dxt:GlyphElementsThemeKey ResourceKey=MDIButtonRestore}">
        <Border Background="Transparent" Margin="-4">
            <Path x:Name="path" Stretch="Fill" Fill="#FF7A7999" Data="F1 M 1177,894L 1185,894L 1185,899.999L 1183,899.999L 1183,903L 1175,903L 1175,897L 1177,897L 1177,894 Z M 1184,898.999L 1184,896L 1178,896L 1178,897L 1183,897L 1183,899L 1184,898.999 Z M 1176,899L 1176,902L 1182,902L 1182,899L 1176,899 Z " HorizontalAlignment="Center" Height="9" Margin="0,0,0,1" UseLayoutRounding="False" VerticalAlignment="Center" Width="10" />
        </Border>
        <DataTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Fill" TargetName="path" Value="#FF000000" />
            </Trigger>
        </DataTemplate.Triggers>
    </DataTemplate>
    
    <!-- GroupBox Button -->
    <Style TargetType="ve:GroupBoxControlBoxButton" x:Key="GroupPane_ExpandButton">
        <Style.Setters>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="ve:GroupBoxControlBoxButton">
                        <Grid x:Name="Button" ve:DockPane.HitTestType="ExpandButton" Width="19" Height="19" Background="Transparent">
                            <VisualStateManager.VisualStateGroups>
                                <VisualStateGroup x:Name="CommonStates">
                                    <VisualState x:Name="Normal" />
                                    <VisualState x:Name="MouseOver">
                                        <Storyboard>
                                            <ColorAnimation Duration="0" To="#FF70798A" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="path" />
                                            <ColorAnimation Duration="0" To="#FF70798A" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="path1" />
                                        </Storyboard>
                                    </VisualState>
                                    <VisualState x:Name="Pressed">
                                        <Storyboard>
                                            <ColorAnimation Duration="0" To="#FF000000" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="path" />
                                            <ColorAnimation Duration="0" To="#FF000000" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="path1" />
                                        </Storyboard>
                                    </VisualState>
                                </VisualStateGroup>
                                <VisualStateGroup x:Name="ExpandedStates">
                                    <VisualState x:Name="Expanded" />
                                    <VisualState x:Name="Collapsed">
                                        <Storyboard>
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(Control.Visibility)" Storyboard.TargetName="open">
                                                <DiscreteObjectKeyFrame KeyTime="0">
                                                    <DiscreteObjectKeyFrame.Value>
                                                        <Visibility>Visible</Visibility>
                                                    </DiscreteObjectKeyFrame.Value>
                                                </DiscreteObjectKeyFrame>
                                            </ObjectAnimationUsingKeyFrames>
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(Control.Visibility)" Storyboard.TargetName="close">
                                                <DiscreteObjectKeyFrame KeyTime="0">
                                                    <DiscreteObjectKeyFrame.Value>
                                                        <Visibility>Collapsed</Visibility>
                                                    </DiscreteObjectKeyFrame.Value>
                                                </DiscreteObjectKeyFrame>
                                            </ObjectAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </VisualState>
                                </VisualStateGroup>
                            </VisualStateManager.VisualStateGroups>
                            <Grid Background="#00000000" Width="14" Height="14" x:Name="grid">
                                <Grid HorizontalAlignment="Center" Margin="0,1,0,0" x:Name="open" VerticalAlignment="Center" Width="14" Height="11" Visibility="Collapsed">
                                    <Path Stretch="Fill" Fill="#5AFFFFFF" Data="F1 M 1385,811L 1392,818L 1392,818L 1399,811L 1395,811L 1392,814L 1392,814L 1389,811L 1385,811 Z " HorizontalAlignment="Stretch" Margin="0,4,0,0" VerticalAlignment="Stretch" />
                                    <Path Stretch="Fill" Fill="#5AFFFFFF" Data="F1 M 1385,807L 1392,814L 1392,814L 1399,807L 1395,807L 1392,810L 1392,810L 1389,807L 1385,807 Z " HorizontalAlignment="Stretch" Margin="0,0,0,4" VerticalAlignment="Stretch" />
                                    <Path Stretch="Fill" Fill="#FF9EA7B7" Data="F1 M 1392,817L 1387,812L 1389,812L 1392,815L 1395,812L 1397,812L 1392,817 Z M 1392,813L 1387,808L 1389,808L 1392,811L 1395,808L 1397,808L 1392,813 Z " HorizontalAlignment="Stretch" Margin="2,1,2,1" VerticalAlignment="Stretch" x:Name="path" />
                                </Grid>
                                <Grid HorizontalAlignment="Center" Margin="0,0,0,1" VerticalAlignment="Center" x:Name="close" Width="14" Height="11">
                                    <Path Stretch="Fill" Fill="#5AFFFFFF" Data="F1 M 1376,790L 1369,783L 1369,783L 1362,790L 1366,790L 1369,787L 1369,787L 1372,790L 1376,790 Z " HorizontalAlignment="Stretch" Margin="0,0,0,4" VerticalAlignment="Stretch" />
                                    <Path Stretch="Fill" Fill="#5AFFFFFF" Data="F1 M 1376,794L 1369,787L 1369,787L 1362,794L 1366,794L 1369,791L 1369,791L 1372,794L 1376,794 Z " HorizontalAlignment="Stretch" Margin="0,4,0,0" VerticalAlignment="Stretch" />
                                    <Path Stretch="Fill" Fill="#FF9EA7B7" Data="F1 M 1369,784L 1369,784L 1369,784L 1374,789L 1372,789L 1369,786L 1366,789L 1364,789L 1369,784L 1369,784 Z M 1369,788L 1369,788L 1369,788L 1374,793L 1372,793L 1369,790L 1366,793L 1364,793L 1369,788L 1369,788 Z " HorizontalAlignment="Stretch" Margin="2,1,2,1" VerticalAlignment="Stretch" x:Name="path1" />
                                </Grid>
                            </Grid>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>
    <DataTemplate x:Key="{dxt:GroupPaneElementsThemeKey ResourceKey=ExpandButton}">
        <ve:GroupBoxControlBoxButton x:Name="PART_Button" Style="{StaticResource GroupPane_ExpandButton}" />
    </DataTemplate>
    
	<!-- LayoutPanel Buttons -->
    <Style x:Key="DockPaneControlBoxBorder" TargetType="ve:ControlBoxButtonBorder">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ve:ControlBoxButtonBorder">
                    <Grid Margin="1,0,0,1" x:Name="Button" Background="Transparent">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Disabled" />
                                <VisualState x:Name="Normal" />
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(Control.Visibility)" Storyboard.TargetName="hoveredState">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(Control.Visibility)" Storyboard.TargetName="pressedState">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border x:Name="hoveredState" Visibility="Collapsed" Background="#FFFFBD69" Padding="1">
                            <Border Background="#FFFFE7A2" />
                        </Border>
                        <Border x:Name="pressedState" Visibility="Collapsed" Background="#FFFB8C3C" Padding="1">
                            <Border Background="#FFFB8C3C" />
                        </Border>
                        <Grid x:Name="glyph" Width="17" Height="17" Background="#00FFFEFE">
                            <ContentPresenter />
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="DockPane_PinButton" TargetType="ve:DockPaneControlBoxButton">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ve:DockPaneControlBoxButton">
                    <ve:ControlBoxButtonBorder Style="{StaticResource DockPaneControlBoxBorder}">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal" />
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ColorAnimation Duration="0" To="#FF000000" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="pinned" />
                                        <ColorAnimation Duration="0" To="#FF000000" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="unpinned" />
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <ColorAnimation Duration="0" To="#FF000000" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="pinned" />
                                        <ColorAnimation Duration="0" To="#FF000000" Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="unpinned" />
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="PinnedStates">
                                <VisualState x:Name="Unpinned" />
                                <VisualState x:Name="Pinned">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(Control.Visibility)" Storyboard.TargetName="pinned">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(Control.Visibility)" Storyboard.TargetName="unpinned">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid>
                            <Rectangle Fill="#00FFFFFF" Stroke="{x:Null}" StrokeThickness="0" HorizontalAlignment="Center" VerticalAlignment="Center" Width="9" Height="9" />
                            <Path Fill="#FF4C535C" Stretch="Fill" HorizontalAlignment="Center" Margin="0,0,0,0" x:Name="unpinned" VerticalAlignment="Center" Width="9" Height="7" Visibility="Collapsed" Data="F1M1252,785L1252,786 1257,786 1257,791 1252,791 1252,792 1251,792 1251,789 1248,789 1248,788 1251,788 1251,785 1252,785z M1256,787L1252,787 1252,788 1256,788 1256,787z" />
                            <Path Fill="#FF4C535C" Stretch="Fill" HorizontalAlignment="Center" Margin="0,1,0,0" x:Name="pinned" VerticalAlignment="Center" Width="7" Height="9" Data="F1M1265,788L1266,788 1266,783 1271,783 1271,788 1272,788 1272,789 1269,789 1269,792 1268,792 1268,789 1265,789 1265,788z M1267,784L1267,788 1268,788 1268,784 1267,784z" />
                        </Grid>
                    </ve:ControlBoxButtonBorder>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ve:DockPane.HitTestType" Value="PinButton" />
    </Style>
