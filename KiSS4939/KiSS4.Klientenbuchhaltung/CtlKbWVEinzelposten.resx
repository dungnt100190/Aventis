  <data name="qryKbWVEinzelposten.SelectStatement" xml:space="preserve">
    <value>SELECT Auswahl        = CONVERT(BIT, 0),
       WEP.*, 
       Person         = ISNULL(PRS.Name + ISNULL(', ' + PRS.Vorname, ''), ISNULL(PRS.Vorname, '')) + ' (' + CONVERT(VARCHAR, WEP.BaPersonID) + ')',
       WVCodeDatumBis = WVC.DatumBis,
       Kostenart      = KOA.Name,
       Splittingart   = dbo.fnLOVMLText('BgSplittingart', WEP.BgSplittingartCode, 1),
       Status         = dbo.fnLOVMLText('KbWVEinzelpostenStatus', WEP.KbWVEinzelpostenStatusCode, 1),
       StornoEintrag  = CONVERT(BIT, CASE WHEN WEP.StorniertKbWVEinzelpostenID IS NULL THEN 0 ELSE 1 END)
FROM dbo.KbWVEinzelposten WEP WITH (READUNCOMMITTED)
  INNER JOIN dbo.BaPerson    PRS WITH (READUNCOMMITTED) ON PRS.BaPersonID = WEP.BaPersonID
  INNER JOIN dbo.BaWVCode    WVC WITH (READUNCOMMITTED) ON WVC.BaWVCodeID = WEP.BaWVCodeID
  LEFT  JOIN dbo.BgKostenart KOA WITH (READUNCOMMITTED) ON KOA.BgKostenartID = WEP.BgKostenartID
WHERE WEP.KbWVLaufID = {0}
--- AND WEP.BaPersonID = {edtSucheBaPerson.LookupID}
--- AND WEP.BgSplittingArtCode = {edtSucheSplittingArt}
--- AND WEP.Betrag &gt;= {edtSucheBetragVon}
--- AND WEP.Betrag &lt;= {edtSucheBetragBis}
--- AND WEP.KbWVEinzelpostenStatusCode = {edtSucheEinzelpostenStatus}
--- AND WEP.Buchungstext LIKE '%' + {edtSucheBuchungstext} + '%'
--- AND WEP.HeimatkantonNr LIKE {edtSucheHeimatKantonNr} + '%'
--- AND WEP.WohnKantonNr LIKE {edtSucheWohnKantonNr} + '%'
--- AND WEP.KantonKuerzel LIKE {edtSucheKantonKuerzel} + '%'
--- AND WEP.Bemerkungen LIKE '%' + {edtSucheBemerkungen} + '%'
--- AND WEP.Fakturiert = CASE WHEN ISNULL({chkSucheFakturiert}, 0) = 1 THEN 1 ELSE WEP.Fakturiert END
ORDER BY Person, WEP.KbWVEinzelpostenStatusCode, WEP.KbWVEinzelpostenID</value>
  </data>
