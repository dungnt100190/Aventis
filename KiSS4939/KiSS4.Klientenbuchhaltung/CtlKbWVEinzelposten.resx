  <data name="qryKbWVEinzelpostenFehler.SelectStatement" xml:space="preserve">
    <value>SELECT WEP.*, 
       Person       = ISNULL(PRS.Name + ISNULL(', ' + PRS.Vorname, ''), ISNULL(PRS.Vorname, '')) + ' (' + CONVERT(VARCHAR, WEP.BaPersonID) + ')',
       Betrag       = BKA.Betrag,
       Buchungstext = BKA.Buchungstext,
       Kostenart    = KOA.Name
FROM dbo.KbWVEinzelpostenFehler WEP WITH (READUNCOMMITTED)
  LEFT JOIN dbo.BaPerson           PRS WITH (READUNCOMMITTED) ON PRS.BaPersonID = WEP.BaPersonID

  LEFT JOIN dbo.KbBuchungKostenart BKA WITH (READUNCOMMITTED) ON BKA.KbBuchungKostenartID = WEP.KbBuchungKostenartID 
  LEFT JOIN dbo.BgKostenart        KOA WITH (READUNCOMMITTED) ON KOA.BgKostenartID = BKA.BgKostenartID
WHERE WEP.KbWVLaufID = {0}
--- AND WEP.BaPersonID = {edtSucheBaPerson.LookupID}
ORDER BY Person, WEP.KbWVEinzelpostenFehlerID</value>
  </data>
