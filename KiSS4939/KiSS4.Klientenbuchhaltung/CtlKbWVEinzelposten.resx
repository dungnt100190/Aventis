  <data name="qryKbWVLauf.SelectStatement" xml:space="preserve">
    <value>SELECT KWL.*,
       UserLastFirstName = ISNULL(USR.LastName + ISNULL(', ' + USR.FirstName, ''), ISNULL(USR.FirstName, '')),
       Periode = dbo.fnKbGetPeriodeInfo(KWL.KbPeriodeID, 'wv', {0}),
       AnzahlEinzelposten = (SELECT COUNT(1)
                             FROM dbo.KbWVEinzelposten WEP WITH(READUNCOMMITTED)
                             WHERE WEP.KbWVLaufID = KWL.KbWVLaufID),
       AnzahlFehler = (SELECT COUNT(1)
                       FROM dbo.KbWVEinzelpostenFehler WEF WITH(READUNCOMMITTED)
                       WHERE WEF.KbWVLaufID = KWL.KbWVLaufID)
FROM dbo.KbWVLauf KWL WITH(READUNCOMMITTED)
  INNER JOIN dbo.XUser USR WITH(READUNCOMMITTED) ON USR.UserID = KWL.UserID
ORDER BY KWL.KbWVLaufID, KWL.DatumBisWVLauf, KWL.StartDatum</value>
