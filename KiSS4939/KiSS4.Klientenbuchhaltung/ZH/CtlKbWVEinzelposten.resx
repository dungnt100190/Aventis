  <data name="qryKbWVEinzelposten.SelectStatement" xml:space="preserve">
    <value>SELECT StatusShort       = LOV1.Shorttext,
       [Einheitstraeger] = UTR.BaPersonID,
       WEP.*,
       UT                = UTR.NameVorname,
       Beguenstigter     = BEG.NameVorname,
       Sel               = cast(0 as bit),
       PeriodCode        = KWL.KbWVLaufPeriodeCode,
       Jahr              = KWL.Jahr,
       Periode           = (SELECT Text FROM XLOVCode WHERE LOVName = 'KbWVLaufPeriode' AND Code = KWL.KbWVLaufPeriodeCode) + ', ' + CAST(KWL.Jahr AS VARCHAR),
       LA                = BKA.KontoNr,
       BruttobelegNr     = KBB.BelegNr,
       BruttobelegBetrag = KBB.Betrag
FROM KbWVEinzelposten              WEP
  LEFT  JOIN vwPerson              UTR ON UTR.BaPersonID              = WEP.UnterstuetzungstraegerBaPersonID
  LEFT  JOIN vwPerson              BEG ON BEG.BaPersonID              = WEP.BeguenstigterBaPersonID
  INNER JOIN XLOVCode              LOV1 ON LOV1.LOVName               = 'KbWVEinzelpostenStatus' AND LOV1.Code = WEP.KbWVEinzelpostenStatusCode
  LEFT  JOIN WhWVEinheitMitglied   WEM ON WEM.WhWVEinheitMitgliedID   = WEP.WhWVEinheitMitgliedID
  LEFT  JOIN WhWVEinheit           WVE ON WVE.WhWVEinheitID           = WEM.WhWVEinheitID
  INNER JOIN KbWVLauf              KWL ON KWL.KbWVLaufID              = WEP.KbWVLaufID
  LEFT  JOIN KbBuchungBruttoPerson KBP ON KBP.KbBuchungBruttoPersonID = WEP.KbBuchungBruttoPersonID
  LEFT  JOIN KbBuchungBrutto       KBB ON KBB.KbBuchungBruttoID       = KBP.KbBuchungBruttoID
  LEFT  JOIN BgKostenart           BKA ON BKA.BgKostenartID           = KBB.BgKostenartID
WHERE 1=1
AND WEP.WVCode NOT IN(1,3,6,12,14,15,16) --('?-?','I-F','S-F','Z-E','K-E','A-E','F-E','B-E')
---  AND WEP.WVCode = {edtSearchWVCode}
AND WEP.BEDCode NOT IN(16030,16031,16032)
---  AND WEP.BEDCode = {edtSearchBED}
---  AND WEP.KbWVLaufID = {edtSearchKbWVLaufID}
AND WEP.KbWVEinzelpostenStatusCode NOT IN(100,700) --('L','V','T','H')
---  AND WEP.KbWVEinzelpostenStatusCode = {edtSearchStatus}
---  AND UTR.NameVorname LIKE '%' + REPLACE({edtSearchUT},' ','%') + '%'
---  AND WEP.UnterstuetzungstraegerBaPersonID = {edtUTBaPersonID}
---  AND BEG.NameVorname LIKE '%' + REPLACE({edtSearchBeguenstigter},' ','%') + '%'
---  AND KWL.KbWVLaufPeriodeCode = {edtSearchPeriod}
---  AND KWL.Jahr = {edtSearchJahr}
---  AND WEP.PscdBelegNr = {edtPscdBelegNr}
ORDER BY CASE WHEN StorniertKbWVEinzelpostenID IS NULL THEN 1 ELSE 0 END
</value>
