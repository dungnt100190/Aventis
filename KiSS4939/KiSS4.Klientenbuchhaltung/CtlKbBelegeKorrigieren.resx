  <data name="qryKbBuchungSuche.SelectStatement" xml:space="preserve">
    <value>SELECT BUC.*
FROM KbBuchung       BUC
  INNER JOIN KbKonto KRE ON KRE.KbPeriodeID = BUC.KbPeriodeID
                        AND KRE.KontoNr = BUC.HabenKtoNr
                        AND ',' + KRE.KbKontoartCodes + ',' LIKE '%,30,%' -- Kreditor-Konto
WHERE BUC.KbPeriodeID = {0}
  AND BUC.KbBuchungStatusCode IN (
    5 /* verbucht */,
    6 /* ausgeglichen */,
   13 /* ZahlauftragFehlerhaft */)
  AND BUC.StorniertKbBuchungID IS NULL -- Stornobelege können nicht storniert werden
---  AND BUC.BelegNr = {edtSucheBelegNr}
---  AND dbo.fnDateOf(BelegDatum) &gt;= {edtSucheBelegDatumVon}
---  AND dbo.fnDateOf(BelegDatum) &lt;= {edtSucheBelegDatumBis}
---  AND EXISTS (SELECT * FROM KbBuchungKostenart WHERE KbBuchungID = BUC.KbBuchungID AND BaPersonID = {edtSucheBaPersonID.LookupID})
---  AND EXISTS (SELECT * FROM KbBuchungKostenart WHERE KbBuchungID = BUC.KbBuchungID AND BgKostenartID = {edtSucheBgKostenartID.LookupID})
</value>
  </data>
