  <data name="qryKbBuchungKostenart.SelectStatement" xml:space="preserve">
    <value>SELECT
  KBU.FaLeistungID,
  KBU.BelegDatum,
  KBU.BgBudgetID,
  BgKostenart = BKA.KontoNr + ' ' + BKA.Name,
  KBP.*,
  PRS.NameVorname
FROM KbBuchung                      KBU
  INNER JOIN KbBuchungKostenart     KBP ON KBP.KbBuchungID = KBU.KbBuchungID
  LEFT  JOIN BgKostenart            BKA ON BKA.BgKostenartID = KBP.BgKostenartID
  LEFT  JOIN vwPerson               PRS ON PRS.BaPersonID = KBP.BaPersonID
WHERE KBU.KbBuchungID = {0}</value>
  </data>
