  <data name="qryQuery.SelectStatement" xml:space="preserve">
    <value>-- Abfrage Kostenstellen journal

SELECT 
 BelegDatum            = BUC.BelegDatum, 
 BelegNr               = BUC.BelegNr,
 Kostenstelle          = dbo.fnKbGetKostenstelle(KOS.BaPersonID),
 Text                  = BUC.Text,
 Gegenkonto            = isNull(BUC.SollKtoNr, BUC.HabenKtoNr), --isNull(KBK.KontoNr + ' - ' + KBK.KontoName, KBK2.KontoNr + ' - ' + KBK2.KontoName),
 Kostenart             = BGK.KontoNr + ' - ' + BGK.Name,
 Betrag                = CASE WHEN BUC.SollKtoNr IS NULL THEN ART.Betrag ELSE -ART.Betrag END,
 Kreditor              = ISNULL(BUC.BeguenstigtName, '') + ' ' + ISNULL(BUC.BeguenstigtName2, ''),
 KbBuchungID$          = BUC.KbBuchungID
FROM dbo.KbBuchung                       BUC WITH (READUNCOMMITTED) 
  INNER JOIN  dbo.KbBuchungKostenart     ART WITH (READUNCOMMITTED)  ON ART.KbBuchungID       = BUC.KbBuchungID
  INNER JOIN  dbo.KbKostenstelle_BaPerson KOS WITH (READUNCOMMITTED)  ON KOS.KbKostenstelleID  = ART.KbKostenstelleID
                                              AND (KOS.DatumBis IS NULL OR GetDate() BETWEEN KOS.DatumVon AND KOS.DatumBis)
  INNER JOIN  dbo.BgKostenart            BGK WITH (READUNCOMMITTED)  ON BGK.BgKostenartID     = ART.BgKostenartID
WHERE 1 = 1  
      AND BUC.BelegNr IS NOT NULL       
      AND KbBuchungStatusCode IN (3,5,6,8,13)           -- verbucht, Zahlauftrag erstellt,ausgeglichen, Storno, Zahlauftrag fehlerhaft
      AND BUC.KbPeriodeID =  {0}                       -- Standardperiode
---   AND KOS.BaPersonID  =  {edtKostenstelleX}
---   AND BUC.BelegNr     &gt;= {edtBelegNrVon}
---   AND BUC.BelegNr     &lt;= {edtBelegNrBis}
---   AND dbo.fnDateOf(BUC.BelegDatum)  &gt;= {edtBuchungsDatumVon}
---   AND dbo.fnDateOf(BUC.BelegDatum)  &lt;= {edtBuchungsDatumBis}</value>
  </data>
