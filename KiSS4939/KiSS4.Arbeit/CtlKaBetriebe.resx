  <data name="qryKaBetriebVerlauf.SelectStatement" xml:space="preserve">
    <value>SELECT BES.*,
       Kontaktperson = BEK.Name + IsNull(', ' + BEK.Vorname, ''),
       Autor = USR.LastName + IsNull(', ' + USR.FirstName, '')
FROM KaBetriebBesprechung BES
   LEFT JOIN XUser USR ON USR.UserID = BES.AutorID
   LEFT JOIN KaBetriebKontakt BEK ON BEK.KaBetriebKontaktID = BES.KontaktPersonID
WHERE BES.KaBetriebID = {0}
ORDER BY BES.Datum DESC</value>
