  <data name="qryKaBetrieb.SelectStatement" xml:space="preserve">
    <value>SELECT BET.*,
       StrasseNr = isNull(ADR.Strasse,'') + isNull(' ' + ADR.HausNr,''),
       PLZOrt = isNull(ADR.PLZ + ' ','') + isNull(ADR.Ort,''),
       TeilbetriebName = TBE.BetriebName,
       Charakter = dbo.fnLOVText('KaBetriebCharakter', BET.CharakterCode)
FROM   KaBetrieb BET
   INNER JOIN BaAdresse ADR ON ADR.KaBetriebID = BET.KaBetriebID
                                   and GetDate() between isNull(ADR.DatumVon, GetDate()) and isNull(ADR.DatumBis, GetDate())
   LEFT JOIN KaBetrieb TBE ON TBE.KaBetriebID = BET.TeilbetriebID
WHERE  1 = 1
--- AND BET.BetriebName like '%' + {edtSucheName} + '%'
--- AND ADR.Ort like '%' + {edtSucheOrt} + '%'
--- AND TBE.BetriebName like '%' + {edtSucheUnterbetriebVon} + '%'
--- AND BET.KaBetriebID in (SELECT KaBetriebID FROM KaBetriebKontakt WHERE (Name + ' ' + Vorname) like '%' + {edtSucheKontaktName} + '%')
--- AND BET.Aktiv = CASE WHEN {chkInaktiveAnzeigen} = 1 THEN BET.Aktiv ELSE 1 END
ORDER BY BET.Aktiv DESC, BET.BetriebName ASC, ADR.Ort ASC</value>
  </data>
