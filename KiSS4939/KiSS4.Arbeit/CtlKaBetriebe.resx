  <data name="qryKaBetriebKontakt.SelectStatement" xml:space="preserve">
    <value>SELECT 
  KON.*,
  Kontaktperson = IsNull(KON.Name + ', ', '') + IsNull(KON.Vorname, ''),
  ADR.Strasse,
  ADR.HausNr,
  ADR.Postfach,
  ADR.PLZ,
  ADR.Ort,
  ADR.Kanton,
  ADR.BaLandID,
  ADR.OrtschaftCode
FROM KaBetriebKontakt KON
  LEFT JOIN BaAdresse ADR ON ADR.KaBetriebKontaktID = KON.KaBetriebKontaktID
WHERE KON.KaBetriebID = {0}
ORDER BY KON.Name</value>
  </data>
