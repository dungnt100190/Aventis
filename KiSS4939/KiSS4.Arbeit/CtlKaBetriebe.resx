  <data name="qryKaBetriebDokument.SelectStatement" xml:space="preserve">
    <value>SELECT BED.*,
       Absender = CASE WHEN BED.AbsenderID &gt; 0 THEN
			USR.LastName + IsNull(', ' + USR.FirstName, '')
		  ELSE
			KON.Name + IsNull(', ' + KON.Vorname, '')
		  END,
       Adressat = CASE WHEN BED.AdressatID &gt; 0 THEN
			USR1.LastName + IsNull(', ' + USR1.FirstName, '')
		  ELSE
			KON1.Name + IsNull(', ' + KON1.Vorname, '')
		  END   
FROM KaBetriebDokument BED
   LEFT JOIN XUser USR ON USR.UserID = BED.AbsenderID
   LEFT JOIN KaBetriebKontakt KON ON KON.KaBetriebKontaktID = -BED.AbsenderID
   LEFT JOIN XUser USR1 ON USR1.UserID = BED.AdressatID
   LEFT JOIN KaBetriebKontakt KON1 ON KON1.KaBetriebKontaktID = -BED.AdressatID
WHERE BED.KaBetriebID = {0}
ORDER BY BED.Datum ASC</value>
  </data>
