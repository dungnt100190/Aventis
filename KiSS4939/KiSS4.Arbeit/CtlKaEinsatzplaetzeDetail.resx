  <data name="qryKaEinsatzplatz.SelectStatement" xml:space="preserve">
    <value>SELECT EIN.*,
       EIN.Aktiv,
       BetName = BET.BetriebName,
       BetAdresse = IsNull(ADR.Strasse, '') + IsNull(' ' + ADR.HausNr, '') + IsNull(', ' + ADR.PLZ, '') + IsNull(' ' + ADR.Ort, ''),
       Zustaendig = USR.LastName + IsNull(', ' + USR.FirstName, ''),
       Kontaktperson = IsNull(Name + ', ', '') + IsNull(Vorname, ''),
       Funktion = dbo.fnLOVText('KaEinsatzplatzKontaktFunktion', EIN.FunktionCode),
       Telefon = KON.Telefon,
       TelefonMobil = KON.TelefonMobil,
       Fax = KON.Fax,
       EMail = KON.Email,
       Aktiv = KON.Aktiv
FROM   KaEinsatzplatz EIN
    LEFT JOIN KaBetrieb BET ON BET.KaBetriebID = EIN.KaBetriebID
    LEFT JOIN BaAdresse ADR on ADR.KaBetriebID = BET.KaBetriebID
                                and GetDate() between isNull(ADR.DatumVon, GetDate()) and isNull(ADR.DatumBis, GetDate())
    LEFT JOIN XUser USR ON USR.UserID = EIN.ZustaendigKA
	LEFT JOIN KaBetriebKontakt KON ON KON.KaBetriebKontaktID = EIN.KaKontaktpersonID 
WHERE  EIN.KaEinsatzplatzID = {0}
ORDER BY EIN.Bezeichnung ASC, BET.BetriebName ASC</value>
  </data>
  <data name="qryKaVermittlung.SelectStatement" xml:space="preserve">
    <value>SELECT	
  NameVorname = PRS.NameVorname,
  Vorschlag   = VER.VorschlagErfasst, 
  EinsatzVon  = NULL,
  EinsatzBis  = NULL,
  Pensum      = NULL,
  Name        = PRS.Name,
  Vorname     = PRS.Vorname,
  BaPersonID  = LEI.BaPersonID,
  Zustaendig  = USR.NameVorname,
  SortKey     = 0
FROM KaVermittlungVorschlag VER
	LEFT JOIN FaLeistung LEI ON LEI.FaLeistungID = VER.FaLeistungID
	LEFT JOIN vwUser USR ON USR.UserID = LEI.UserID
	LEFT JOIN vwPerson PRS ON PRS.BaPersonID = LEI.BaPersonID
WHERE VER.KaEinsatzplatzID = {0}
  AND VER.VorschlagResultat IS NULL
  AND VER.VorschlagResultatDatum IS NULL
  AND LEI.FaProzessCode != 711 -- Jobtimal hat kein Register 'Vorschlag'

UNION ALL

SELECT	
  NameVorname = PRS.NameVorname,
  Vorschlag   = VER.VorschlagErfasst, 
  EinsatzVon  = EIN.EinsatzVon,
  EinsatzBis  = IsNull(EIN.Abschluss, EIN.EinsatzBis),
  Pensum      = EIN.Arbeitspensum,
  Name        = PRS.Name,
  Vorname     = PRS.Vorname,
  BaPersonID  = LEI.BaPersonID,
  Zustaendig  = USR.NameVorname,
  SortKey     = 1
FROM KaVermittlungVorschlag VER
	INNER JOIN KaVermittlungEinsatz EIN ON EIN.KaVermittlungVorschlagID = VER.KaVermittlungVorschlagID
	LEFT JOIN FaLeistung LEI ON LEI.FaLeistungID = VER.FaLeistungID
	LEFT JOIN vwUser USR ON USR.UserID = LEI.UserID
	LEFT JOIN vwPerson PRS ON PRS.BaPersonID = LEI.BaPersonID
WHERE VER.KaEinsatzplatzID = {0}
  AND EIN.EinsatzVon IS NOT NULL

ORDER BY SortKey ASC, EinsatzVon DESC, NameVorname ASC, Vorschlag DESC</value>
  </data>
