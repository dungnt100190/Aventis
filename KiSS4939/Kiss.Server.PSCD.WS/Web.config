                <value>http://szhm2179.stzh.ch:8001/XISOAPAdapter/MessageServlet?channel=:BSV_FAMOZ_SD02_WS_OUT:CC_SD02_WS_OUT&amp;version=3.0&amp;Sender.Service=BSV_FAMOZ_SD02_WS_OUT&amp;Interface=http%3A%2F%2Fstzh.ch%2FFAMOZ_SD02_CREATE_WS%5EMI_SD02_CREATE_WS</value>
            </setting>
          
            <setting name="PSCDUsername" serializeAs="String">
                <value>KOMM</value>
            </setting>
            <setting name="PSCDPassword" serializeAs="String">
                <value>KOMM01</value>
            </setting>
          
            <!-- Endpoint, Belegtransfer vom KissServer zum PSCD -->
            <setting name="PSCD_URL_BELEG_ANLEGEN" serializeAs="String">
                <value>http://szhm2179.stzh.ch:8001/XISOAPAdapter/MessageServlet?channel=:BSV_FAMOZ_SD01_WS_OUT:CC_SD01_WS_OUT&amp;version=3.0&amp;Sender.Service=BSV_FAMOZ_SD01_WS_OUT&amp;Interface=http%3A%2F%2Fstzh.ch%2FFAMOZ_SD01_CREATE_WS%5EMI_SD01_CREATE_WS</value>
            </setting>
        </Kiss.Server.PSCD.WS.Properties.Settings>        

    </applicationSettings>
</configuration>
